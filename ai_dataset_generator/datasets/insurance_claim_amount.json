{
  "dataset_config": {
    "name": "insurance_claim_amount",
    "description": "Insurance claim amounts based on policyholder characteristics and claim details",
    "random_seed": 42,
    "n_rows": 5000,
    "correlations": [
      {
        "variables": [
          "age",
          "years_with_company"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "vehicle_value",
          "coverage_amount"
        ],
        "correlation": 0.78,
        "method": "cholesky"
      },
      {
        "variables": [
          "credit_score",
          "income"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "age",
        "description": "Age of policyholder in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 45,
          "std": 12,
          "min_clip": 18,
          "max_clip": 80
        }
      },
      {
        "name": "years_with_company",
        "description": "Years as customer",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 8,
          "location": 0
        }
      },
      {
        "name": "vehicle_value",
        "description": "Current vehicle value in thousands",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 28,
          "std": 12,
          "min_clip": 5,
          "max_clip": 80
        }
      },
      {
        "name": "coverage_amount",
        "description": "Insurance coverage amount in thousands",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 35,
          "std": 15,
          "min_clip": 10,
          "max_clip": 100
        }
      },
      {
        "name": "credit_score",
        "description": "Credit score 300-850",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 700,
          "std": 75,
          "min_clip": 300,
          "max_clip": 850
        }
      },
      {
        "name": "income",
        "description": "Annual income in thousands",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 25,
          "min_clip": 20,
          "max_clip": 200
        }
      },
      {
        "name": "driving_distance",
        "description": "Annual driving distance in thousands of miles",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 12,
          "std": 4,
          "min_clip": 2,
          "max_clip": 30
        }
      },
      {
        "name": "region",
        "description": "Geographic region",
        "data_type": "categorical",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 1
        },
        "categories": [
          "Northeast",
          "Northeast",
          "Northeast",
          "South",
          "South",
          "South",
          "Midwest",
          "Midwest",
          "West",
          "West"
        ]
      },
      {
        "name": "vehicle_type",
        "description": "Type of vehicle",
        "data_type": "categorical",
        "distribution": {
          "type": "normal",
          "mean": 0.5,
          "std": 0.2
        },
        "categories": [
          "Compact",
          "Compact",
          "Sedan",
          "Sedan",
          "Sedan",
          "Sedan",
          "SUV",
          "SUV",
          "Truck",
          "Luxury"
        ]
      },
      {
        "name": "claim_type",
        "description": "Type of insurance claim",
        "data_type": "categorical",
        "distribution": {
          "type": "normal",
          "mean": 0.4,
          "std": 0.25
        },
        "categories": [
          "Minor Damage",
          "Minor Damage",
          "Minor Damage",
          "Moderate Damage",
          "Moderate Damage",
          "Moderate Damage",
          "Major Damage",
          "Major Damage",
          "Total Loss",
          "Total Loss"
        ]
      }
    ],
    "target": {
      "name": "claim_amount",
      "description": "Insurance claim payout amount in dollars",
      "data_type": "float",
      "expression": "2000 + 150*vehicle_value + 80*coverage_amount + 25*driving_distance - 15*years_with_company - 2*credit_score + 50*age",
      "noise_percent": 0.0
    }
  }
}