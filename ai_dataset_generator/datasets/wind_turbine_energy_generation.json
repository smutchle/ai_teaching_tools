{
  "dataset_config": {
    "name": "wind_turbine_energy_generation",
    "description": "Hourly time series data for wind turbine energy generation with weather and operational features",
    "random_seed": 42,
    "n_rows": 240,
    "correlations": [
      {
        "variables": [
          "wind_speed",
          "rotor_speed"
        ],
        "correlation": 0.85,
        "method": "cholesky"
      },
      {
        "variables": [
          "temperature",
          "air_density"
        ],
        "correlation": -0.65,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "timestamp",
        "description": "Hourly timestamp",
        "data_type": "datetime",
        "distribution": {
          "type": "sequential_datetime",
          "start": "2024-01-01T00:00:00",
          "interval": "hourly"
        }
      },
      {
        "name": "wind_speed",
        "description": "Wind speed in meters per second",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 8.5,
          "std": 3.2,
          "min_clip": 0,
          "max_clip": 25
        },
        "lags": [
          1,
          2,
          24
        ],
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "rotor_speed",
        "description": "Rotor speed in RPM",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 15.0,
          "std": 4.5,
          "min_clip": 0,
          "max_clip": 25
        },
        "lags": [
          1
        ],
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "temperature",
        "description": "Ambient temperature in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 12.0,
          "std": 6.0,
          "min_clip": -10,
          "max_clip": 35
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "air_density",
        "description": "Air density in kg/m\u00b3",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 1.225,
          "std": 0.05,
          "min_clip": 1.1,
          "max_clip": 1.35
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "turbine_status",
        "description": "Operational status of the turbine",
        "data_type": "categorical",
        "distribution": {
          "type": "normal",
          "mean": 0.8,
          "std": 0.15
        },
        "categories": [
          "Maintenance",
          "Startup",
          "Operational",
          "Operational",
          "Operational",
          "Operational",
          "Operational",
          "Operational",
          "Operational",
          "Operational"
        ],
        "missing_rate": 0.0
      }
    ],
    "target": {
      "name": "energy_output_kw",
      "description": "Energy output in kilowatts",
      "data_type": "float",
      "expression": "50 + 120*wind_speed + 15*rotor_speed + 80*air_density + 25*wind_speed_lag1 + 10*wind_speed_lag2 - 2*temperature + 5*rotor_speed_lag1",
      "noise_percent": 1.0,
      "seasonality_multipliers": [
        1.15,
        1.12,
        1.08,
        1.05,
        1.02,
        0.98,
        0.95,
        0.92,
        0.9,
        0.92,
        0.95,
        0.98,
        1.0,
        1.02,
        1.05,
        1.08,
        1.1,
        1.12,
        1.13,
        1.12,
        1.1,
        1.08,
        1.12,
        1.15
      ]
    }
  }
}