{
  "dataset_config": {
    "name": "ds046_biology_disease_status_healthyinfectedresistant",
    "description": "Classify organism health state based on immune markers, exposure level, genetic factors, nutrition, and stress",
    "random_seed": 46,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "immune_marker_count",
          "white_cell_activity"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "pathogen_exposure_level",
          "environmental_stress_index"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "nutrition_score",
          "genetic_resistance_factor"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "immune_marker_count",
          "nutrition_score"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "immune_marker_count",
        "description": "Count of active immune markers per microliter",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 450,
          "std": 85,
          "min_clip": 200,
          "max_clip": 800
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "white_cell_activity",
        "description": "White blood cell activity level (units per ml)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7200,
          "std": 1200,
          "min_clip": 4000,
          "max_clip": 12000
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "pathogen_exposure_level",
        "description": "Environmental pathogen exposure index (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 35,
          "location": 5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "genetic_resistance_factor",
        "description": "Genetic resistance score (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.2,
          "std": 1.4,
          "min_clip": 2.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "nutrition_score",
        "description": "Overall nutrition quality index (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 72,
          "std": 12,
          "min_clip": 30,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "environmental_stress_index",
        "description": "Combined environmental stress factors (0-50)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 18,
          "location": 2
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "disease_status_healthyinfectedresistant",
      "description": "Organism health classification based on immune response and exposure factors",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(0.008*immune_marker_count + 0.0005*white_cell_activity - 0.12*pathogen_exposure_level + 0.8*genetic_resistance_factor + 0.04*nutrition_score - 0.15*environmental_stress_index - 6)))",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0,
      "categories": [
        "Infected",
        "Infected",
        "Infected",
        "Healthy",
        "Healthy",
        "Healthy",
        "Healthy",
        "Resistant",
        "Resistant",
        "Resistant"
      ]
    }
  }
}