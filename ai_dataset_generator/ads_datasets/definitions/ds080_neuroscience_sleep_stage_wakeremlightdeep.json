{
  "dataset_config": {
    "name": "ds080_neuroscience_sleep_stage_wakeremlightdeep",
    "description": "Classify polysomnography recordings into sleep stages based on EEG frequency bands, eye movement, muscle tone, heart rate, and time of night",
    "random_seed": 80,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "delta_power",
          "theta_power"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "alpha_power",
          "beta_power"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "muscle_tone",
          "heart_rate"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "rem_density",
          "theta_power"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "heart_rate",
          "beta_power"
        ],
        "correlation": 0.41,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "delta_power",
        "description": "EEG delta wave power (0.5-4 Hz) in microvolts squared",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 45.0,
          "location": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "theta_power",
        "description": "EEG theta wave power (4-8 Hz) in microvolts squared",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 28.5,
          "std": 8.2,
          "min_clip": 2.0,
          "max_clip": 80.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "alpha_power",
        "description": "EEG alpha wave power (8-12 Hz) in microvolts squared",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 22.3,
          "std": 6.8,
          "min_clip": 1.0,
          "max_clip": 65.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "beta_power",
        "description": "EEG beta wave power (12-30 Hz) in microvolts squared",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 18.7,
          "std": 5.4,
          "min_clip": 1.0,
          "max_clip": 50.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "rem_density",
        "description": "Rapid eye movement density (movements per minute)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 12.5,
          "location": 0.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "muscle_tone",
        "description": "EMG muscle tone amplitude in microvolts",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 15.2,
          "std": 4.8,
          "min_clip": 0.5,
          "max_clip": 45.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "heart_rate",
        "description": "Heart rate in beats per minute",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 65.0,
          "std": 8.5,
          "min_clip": 45,
          "max_clip": 95
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "time_of_night",
        "description": "Time since sleep onset in hours",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.0,
          "max": 8.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "sleep_stage_wakeremlightdeep",
      "description": "Sleep stage classification from polysomnography",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-2.5 + 0.08*delta_power - 0.12*alpha_power - 0.15*beta_power + 0.25*rem_density - 0.18*muscle_tone - 0.05*heart_rate + 0.3*theta_power + np.sin(2*3.14159*time_of_night/8)*0.8)))",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0,
      "categories": [
        "Wake",
        "Wake",
        "Light Sleep",
        "Light Sleep",
        "Light Sleep",
        "Light Sleep",
        "Deep Sleep",
        "Deep Sleep",
        "REM Sleep",
        "REM Sleep"
      ]
    }
  }
}