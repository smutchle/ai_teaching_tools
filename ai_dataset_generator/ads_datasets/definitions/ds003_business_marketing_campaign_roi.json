{
  "dataset_config": {
    "name": "ds003_business_marketing_campaign_roi",
    "description": "Estimate return on investment for marketing initiatives",
    "random_seed": 3,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "campaign_budget",
          "audience_size"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "creative_score",
          "brand_awareness_lift"
        ],
        "correlation": 0.42,
        "method": "cholesky"
      },
      {
        "variables": [
          "digital_channel_mix",
          "audience_size"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "campaign_budget",
          "brand_awareness_lift"
        ],
        "correlation": 0.48,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "campaign_budget",
        "description": "Total marketing campaign budget in thousands",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 150,
          "location": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "digital_channel_mix",
        "description": "Percentage of budget allocated to digital channels",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 15,
          "min_clip": 10,
          "max_clip": 95
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "audience_size",
        "description": "Target audience size in thousands",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 500,
          "location": 50
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "seasonality_factor",
        "description": "Seasonal demand multiplier based on timing",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 1.0,
          "std": 0.25,
          "min_clip": 0.5,
          "max_clip": 1.8
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "creative_score",
        "description": "Creative quality score from 1-10",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.5,
          "std": 1.5,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "brand_awareness_lift",
        "description": "Brand awareness lift percentage from campaign",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 2.0,
          "max": 25.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "competitor_activity",
        "description": "Competitor marketing activity index",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.0,
          "std": 2.0,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "marketing_campaign_roi",
      "description": "Return on investment as percentage",
      "data_type": "float",
      "expression": "50 + 0.8*campaign_budget + 0.15*digital_channel_mix + 0.02*audience_size + 35*seasonality_factor + 12*creative_score + 1.5*brand_awareness_lift - 8*competitor_activity",
      "noise_percent": 2.5
    }
  }
}