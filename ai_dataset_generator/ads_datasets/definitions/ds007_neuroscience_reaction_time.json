{
  "dataset_config": {
    "name": "ds007_neuroscience_reaction_time",
    "description": "Predict speed of response to stimuli in milliseconds",
    "random_seed": 7,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "age",
          "baseline_reaction_time"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "fatigue_level",
          "sleep_hours"
        ],
        "correlation": -0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "caffeine_intake",
          "alertness_score"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "training_sessions",
          "baseline_reaction_time"
        ],
        "correlation": -0.35,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "age",
        "description": "Participant age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 35,
          "std": 12,
          "min_clip": 18,
          "max_clip": 75
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "fatigue_level",
        "description": "Self-reported fatigue level (1-10 scale)",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 5.5,
          "std": 2.0,
          "min_clip": 1,
          "max_clip": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "task_complexity",
        "description": "Complexity level of the reaction task (1-5 scale)",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 1,
          "max": 5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "caffeine_intake",
        "description": "Caffeine consumed in past 4 hours (mg)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 120,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "training_sessions",
        "description": "Number of prior training sessions completed",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 8,
          "std": 4,
          "min_clip": 0,
          "max_clip": 20
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "sleep_hours",
        "description": "Hours of sleep in previous night",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.2,
          "std": 1.1,
          "min_clip": 3.0,
          "max_clip": 12.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "alertness_score",
        "description": "Pre-test alertness assessment score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 72,
          "std": 15,
          "min_clip": 20,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "baseline_reaction_time",
        "description": "Baseline reaction time from simple task (ms)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 280,
          "std": 45,
          "min_clip": 180,
          "max_clip": 450
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "reaction_time",
      "description": "Response time to stimulus in milliseconds",
      "data_type": "float",
      "expression": "200 + 2.5*age + 15*fatigue_level + 45*task_complexity - 0.8*caffeine_intake - 8*training_sessions + 0.6*baseline_reaction_time - 2.2*alertness_score",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}