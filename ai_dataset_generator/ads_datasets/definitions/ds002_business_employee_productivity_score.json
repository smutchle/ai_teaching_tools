{
  "dataset_config": {
    "name": "ds002_business_employee_productivity_score",
    "description": "Employee productivity dataset measuring individual output and efficiency based on experience, training, team dynamics, and work arrangements",
    "random_seed": 2,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "years_experience",
          "base_salary"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "training_hours_completed",
          "skill_certifications"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "team_size",
          "project_complexity_score"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "years_experience",
          "training_hours_completed"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "years_experience",
        "description": "Total years of professional work experience",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 8.5,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "training_hours_completed",
        "description": "Total professional development and training hours completed annually",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 45,
          "std": 18,
          "min_clip": 5,
          "max_clip": 120
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "team_size",
        "description": "Number of people in immediate working team",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 7,
          "std": 3,
          "min_clip": 2,
          "max_clip": 15
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "work_from_home_days",
        "description": "Average number of remote work days per week",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.0,
          "max": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "base_salary",
        "description": "Annual base salary in thousands USD",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 75,
          "std": 25,
          "min_clip": 35,
          "max_clip": 180
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "skill_certifications",
        "description": "Number of professional certifications held",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 3.2,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "project_complexity_score",
        "description": "Average complexity rating of assigned projects (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.2,
          "std": 1.8,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "meeting_hours_per_week",
        "description": "Average hours spent in meetings per week",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 12,
          "std": 5,
          "min_clip": 2,
          "max_clip": 30
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "employee_productivity_score",
      "description": "Composite productivity score measuring employee output and efficiency (0-100 scale)",
      "data_type": "float",
      "expression": "45 + 1.8*years_experience + 0.25*training_hours_completed + 0.8*team_size + 2.2*work_from_home_days + 0.12*base_salary + 1.5*skill_certifications + 2.5*project_complexity_score - 0.35*meeting_hours_per_week",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}