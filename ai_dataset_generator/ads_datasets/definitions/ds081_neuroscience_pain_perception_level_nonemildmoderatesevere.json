{
  "dataset_config": {
    "name": "ds081_neuroscience_pain_perception_level_nonemildmoderatesevere",
    "description": "Neuroscience dataset for categorizing subjective pain intensity based on stimulus characteristics and individual factors",
    "random_seed": 81,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "stimulus_intensity",
          "pain_threshold"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "anxiety_level",
          "attention_focus"
        ],
        "correlation": -0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "prior_pain_exposure",
          "pain_threshold"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "genetic_sensitivity",
          "pain_threshold"
        ],
        "correlation": -0.41,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "stimulus_intensity",
        "description": "Physical intensity of pain stimulus (0-100 scale)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "genetic_sensitivity",
        "description": "Genetic predisposition to pain sensitivity (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.0,
          "std": 1.5,
          "min_clip": 0,
          "max_clip": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "prior_pain_exposure",
        "description": "Previous pain experience score (0-20 scale)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 8.0,
          "location": 2.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "attention_focus",
        "description": "Level of attention directed to stimulus (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.5,
          "std": 2.0,
          "min_clip": 0,
          "max_clip": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "anxiety_level",
        "description": "Current anxiety state (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 4.2,
          "std": 1.8,
          "min_clip": 0,
          "max_clip": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "pain_threshold",
        "description": "Individual pain threshold level (0-15 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.5,
          "std": 2.2,
          "min_clip": 1,
          "max_clip": 15
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "pain_perception_level_nonemildmoderatesevere",
      "description": "Subjective pain perception category",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(0.08*stimulus_intensity + 0.15*genetic_sensitivity - 0.12*prior_pain_exposure + 0.18*attention_focus + 0.14*anxiety_level - 0.22*pain_threshold - 4.5)))",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "categories": [
        "None",
        "None",
        "Mild",
        "Mild",
        "Mild",
        "Moderate",
        "Moderate",
        "Moderate",
        "Severe",
        "Severe"
      ]
    }
  }
}