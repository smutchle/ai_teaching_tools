{
  "dataset_config": {
    "name": "ds004_business_real_estate_price_dirty",
    "description": "Predict property sale or rental prices based on property characteristics",
    "random_seed": 4,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "square_footage",
          "bedrooms"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "square_footage",
          "bathrooms"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "bedrooms",
          "bathrooms"
        ],
        "correlation": 0.72,
        "method": "cholesky"
      },
      {
        "variables": [
          "location_score",
          "amenities_count"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "property_age",
          "maintenance_score"
        ],
        "correlation": -0.38,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "square_footage",
        "description": "Total square footage of the property",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 2200,
          "std": 800,
          "min_clip": 600,
          "max_clip": 8000
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "bedrooms",
        "description": "Number of bedrooms",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 3.2,
          "std": 1.1,
          "min_clip": 1,
          "max_clip": 7
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "bathrooms",
        "description": "Number of bathrooms",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2.1,
          "std": 0.8,
          "min_clip": 1.0,
          "max_clip": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "location_score",
        "description": "Location desirability score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 18,
          "min_clip": 10,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "property_age",
        "description": "Age of property in years",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 25,
          "location": 0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "amenities_count",
        "description": "Number of amenities (pool, garage, etc.)",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 4.5,
          "std": 2.2,
          "min_clip": 0,
          "max_clip": 12
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "maintenance_score",
        "description": "Property maintenance condition score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 75,
          "std": 15,
          "min_clip": 20,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "lot_size_acres",
        "description": "Lot size in acres",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 0.8,
          "location": 0.1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "real_estate_price",
      "description": "Property sale price in dollars",
      "data_type": "float",
      "expression": "180000 + 125*square_footage + 18000*bedrooms + 22000*bathrooms + 1200*location_score - 850*property_age + 5500*amenities_count + 800*maintenance_score + 35000*lot_size_acres",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}