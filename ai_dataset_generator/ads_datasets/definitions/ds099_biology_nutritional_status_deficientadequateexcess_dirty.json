{
  "dataset_config": {
    "name": "ds099_biology_nutritional_status_deficientadequateexcess_dirty",
    "description": "Categorize micronutrient levels based on diet quality, biomarkers, anthropometry, and absorption capacity",
    "random_seed": 99,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "diet_quality_score",
          "serum_vitamin_d"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "bmi",
          "waist_circumference"
        ],
        "correlation": 0.72,
        "method": "cholesky"
      },
      {
        "variables": [
          "hemoglobin_level",
          "serum_iron"
        ],
        "correlation": 0.64,
        "method": "cholesky"
      },
      {
        "variables": [
          "diet_quality_score",
          "absorption_efficiency"
        ],
        "correlation": 0.41,
        "method": "cholesky"
      },
      {
        "variables": [
          "serum_vitamin_d",
          "absorption_efficiency"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "diet_quality_score",
        "description": "Overall diet quality index based on nutrient density",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65.0,
          "std": 18.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "serum_vitamin_d",
        "description": "Serum 25-hydroxyvitamin D concentration (ng/mL)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 28.0,
          "location": 8.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "hemoglobin_level",
        "description": "Blood hemoglobin concentration (g/dL)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 13.8,
          "std": 1.9,
          "min_clip": 7.0,
          "max_clip": 18.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "serum_iron",
        "description": "Serum iron concentration (\u03bcg/dL)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 95.0,
          "std": 32.0,
          "min_clip": 20.0,
          "max_clip": 200.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "bmi",
        "description": "Body mass index (kg/m\u00b2)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 25.8,
          "std": 5.2,
          "min_clip": 15.0,
          "max_clip": 45.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "waist_circumference",
        "description": "Waist circumference measurement (cm)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 88.5,
          "std": 14.8,
          "min_clip": 55.0,
          "max_clip": 140.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "absorption_efficiency",
        "description": "Intestinal micronutrient absorption efficiency percentage",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 3.2,
          "scale": 78.0,
          "location": 15.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "inflammatory_markers",
        "description": "C-reactive protein and inflammatory biomarker composite score",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 2.4,
          "location": 0.3
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "nutritional_status_deficientadequateexcess",
      "description": "Overall micronutrient status classification",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(0.08*diet_quality_score + 0.12*serum_vitamin_d + 0.45*hemoglobin_level + 0.02*serum_iron + 0.06*absorption_efficiency - 0.15*bmi - 0.08*inflammatory_markers - 8.5)))",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "Deficient",
        "Deficient",
        "Deficient",
        "Deficient",
        "Adequate",
        "Adequate",
        "Adequate",
        "Adequate",
        "Excess",
        "Excess"
      ]
    }
  }
}