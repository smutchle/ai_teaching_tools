{
  "dataset_config": {
    "name": "ds058_neuroscience_white_matter_integrity_dirty",
    "description": "Quantify neural pathway quality using diffusion metrics",
    "random_seed": 58,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "age",
          "cardiovascular_health_index"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "education_years",
          "cognitive_reserve_score"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "cardiovascular_health_index",
          "white_matter_lesion_volume"
        ],
        "correlation": -0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "head_trauma_severity",
          "white_matter_lesion_volume"
        ],
        "correlation": 0.33,
        "method": "cholesky"
      },
      {
        "variables": [
          "apoe4_allele_count",
          "age"
        ],
        "correlation": 0.15,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "age",
        "description": "Participant age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 12,
          "min_clip": 18,
          "max_clip": 95
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "cardiovascular_health_index",
        "description": "Composite cardiovascular health score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 72,
          "std": 15,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "education_years",
        "description": "Years of formal education completed",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 14.5,
          "std": 3.2,
          "min_clip": 6,
          "max_clip": 25
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "head_trauma_severity",
        "description": "Cumulative head trauma severity score (0-10)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 0.8,
          "scale": 2.5,
          "location": 0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "apoe4_allele_count",
        "description": "Number of APOE4 alleles (0, 1, or 2)",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 2
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "cognitive_reserve_score",
        "description": "Cognitive reserve composite score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 68,
          "std": 18,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "white_matter_lesion_volume",
        "description": "Total white matter lesion volume in cubic millimeters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.2,
          "scale": 8.5,
          "location": 0.1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "brain_volume_normalized",
        "description": "Normalized total brain volume (percentage of expected)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 95,
          "std": 8,
          "min_clip": 70,
          "max_clip": 115
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "white_matter_integrity",
      "description": "White matter integrity composite score based on DTI metrics",
      "data_type": "float",
      "expression": "85 - 0.35*age + 0.15*cardiovascular_health_index + 0.8*education_years - 2.2*head_trauma_severity - 3.5*apoe4_allele_count + 0.12*cognitive_reserve_score - 0.6*white_matter_lesion_volume + 0.25*brain_volume_normalized",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}