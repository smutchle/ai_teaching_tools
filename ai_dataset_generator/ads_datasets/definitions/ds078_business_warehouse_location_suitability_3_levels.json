{
  "dataset_config": {
    "name": "ds078_business_warehouse_location_suitability_3_levels",
    "description": "Rank site appropriateness for distribution center based on transportation access, labor costs, proximity to markets, land cost, and utilities",
    "random_seed": 78,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "transportation_access_score",
          "proximity_to_markets_km"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "land_cost_per_sqft",
          "utilities_cost_index"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "labor_cost_per_hour",
          "land_cost_per_sqft"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "proximity_to_markets_km",
          "transportation_infrastructure_rating"
        ],
        "correlation": -0.33,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "transportation_access_score",
        "description": "Transportation access quality score (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.5,
          "std": 1.8,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "labor_cost_per_hour",
        "description": "Average warehouse labor cost per hour in USD",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 18.5,
          "location": 12.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "proximity_to_markets_km",
        "description": "Distance to major markets in kilometers",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 85.0,
          "location": 15.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "land_cost_per_sqft",
        "description": "Land cost per square foot in USD",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 12.5,
          "std": 4.2,
          "min_clip": 3.0,
          "max_clip": 35.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "utilities_cost_index",
        "description": "Utilities cost index (100 = national average)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 105.0,
          "std": 15.5,
          "min_clip": 70.0,
          "max_clip": 150.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "transportation_infrastructure_rating",
        "description": "Regional transportation infrastructure rating (1-5 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 3.2,
          "std": 0.8,
          "min_clip": 1.0,
          "max_clip": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "available_workforce_thousands",
        "description": "Available warehouse workforce in thousands",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 25.0,
          "location": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "zoning_compliance_score",
        "description": "Zoning and regulatory compliance score (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 3.0,
          "max": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "warehouse_location_suitability_3_levels",
      "description": "Overall warehouse location suitability rating",
      "data_type": "categorical",
      "expression": "2.5*transportation_access_score - 0.8*labor_cost_per_hour - 0.03*proximity_to_markets_km - 0.4*land_cost_per_sqft - 0.02*utilities_cost_index + 1.8*transportation_infrastructure_rating + 0.15*available_workforce_thousands + 1.2*zoning_compliance_score",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0,
      "categories": [
        "Poor",
        "Poor",
        "Poor",
        "Fair",
        "Fair",
        "Fair",
        "Excellent",
        "Excellent",
        "Excellent",
        "Excellent"
      ]
    }
  }
}