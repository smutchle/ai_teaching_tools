{
  "dataset_config": {
    "name": "ds006_neuroscience_brain_region_volume",
    "description": "Estimate volumetric measurements of specific brain structures based on age, sex, genetic markers, lifestyle factors, and comorbidities",
    "random_seed": 6,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "age",
          "cognitive_score"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "physical_activity",
          "bmi"
        ],
        "correlation": -0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "education_years",
          "cognitive_score"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "apoe4_alleles",
          "age"
        ],
        "correlation": 0.15,
        "method": "cholesky"
      },
      {
        "variables": [
          "sleep_hours",
          "physical_activity"
        ],
        "correlation": 0.28,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "age",
        "description": "Age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 12,
          "min_clip": 18,
          "max_clip": 95
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "sex_numeric",
        "description": "Sex encoded as numeric (0=female, 1=male)",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 1
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "apoe4_alleles",
        "description": "Number of APOE4 alleles (0, 1, or 2)",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 0.8,
          "scale": 0.6,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "education_years",
        "description": "Years of formal education",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 14,
          "std": 3.5,
          "min_clip": 8,
          "max_clip": 25
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "physical_activity",
        "description": "Weekly physical activity hours",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 4.5,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "bmi",
        "description": "Body mass index",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 26.8,
          "std": 4.2,
          "min_clip": 16.0,
          "max_clip": 45.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "cognitive_score",
        "description": "Standardized cognitive assessment score",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 100,
          "std": 15,
          "min_clip": 60,
          "max_clip": 140
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "sleep_hours",
        "description": "Average nightly sleep duration in hours",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.2,
          "std": 1.1,
          "min_clip": 4.0,
          "max_clip": 11.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "brain_region_volume",
      "description": "Volumetric measurement of specific brain structure in cubic centimeters",
      "data_type": "float",
      "expression": "15000 + 150*sex_numeric - 45*age + 85*education_years + 25*physical_activity - 180*apoe4_alleles + 12*cognitive_score - 35*bmi + 45*sleep_hours",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}