{
  "dataset_config": {
    "name": "ds001_business_customer_lifetime_value_dirty",
    "description": "Predict total revenue from a customer over their relationship with company",
    "random_seed": 1,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "purchase_frequency_monthly",
          "avg_transaction_value"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "tenure_months",
          "total_products_purchased"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "avg_transaction_value",
          "premium_category_purchases"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "purchase_frequency_monthly",
          "total_products_purchased"
        ],
        "correlation": 0.51,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "purchase_frequency_monthly",
        "description": "Average number of purchases per month",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 3.5,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "avg_transaction_value",
        "description": "Average transaction value in dollars",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 85.0,
          "std": 35.0,
          "min_clip": 5.0,
          "max_clip": 500.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "tenure_months",
        "description": "Number of months as customer",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 24.0,
          "location": 1.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "total_products_purchased",
        "description": "Total number of distinct products purchased",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 12.0,
          "std": 8.0,
          "min_clip": 1.0,
          "max_clip": 50.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "premium_category_purchases",
        "description": "Number of purchases in premium categories",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 4.0,
          "location": 0.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "discount_usage_rate",
        "description": "Percentage of purchases using discounts",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.0,
          "max": 0.8
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "support_tickets_count",
        "description": "Total number of customer support tickets",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 0.8,
          "scale": 2.5,
          "location": 0.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "customer_lifetime_value",
      "description": "Total revenue from customer over their relationship with company",
      "data_type": "float",
      "expression": "purchase_frequency_monthly * avg_transaction_value * tenure_months + 15 * total_products_purchased + 25 * premium_category_purchases - 50 * discount_usage_rate * avg_transaction_value - 10 * support_tickets_count",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}