{
  "dataset_config": {
    "name": "ds001_business_customer_lifetime_value_dirty",
    "description": "Predict total revenue from a customer over their relationship with company based on purchase behavior, transaction patterns, tenure, and product preferences",
    "random_seed": 1,
    "n_rows": 1000,
    "correlations": [
      {
        "variables": [
          "purchase_frequency",
          "avg_transaction_value"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "tenure_months",
          "total_purchases"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "premium_product_purchases",
          "avg_transaction_value"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "customer_engagement_score",
          "purchase_frequency"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "purchase_frequency",
        "description": "Average number of purchases per month",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2.5,
          "std": 1.2,
          "min_clip": 0.1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both"
      },
      {
        "name": "avg_transaction_value",
        "description": "Average dollar amount per transaction",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 85.0,
          "std": 35.0,
          "min_clip": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both"
      },
      {
        "name": "tenure_months",
        "description": "Number of months as active customer",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 24.0,
          "location": 1.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both"
      },
      {
        "name": "total_purchases",
        "description": "Total number of purchases made",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 15.0,
          "std": 8.0,
          "min_clip": 1.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both"
      },
      {
        "name": "premium_product_purchases",
        "description": "Number of premium category purchases",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 3.2,
          "std": 2.1,
          "min_clip": 0.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both"
      },
      {
        "name": "product_category_diversity",
        "description": "Number of different product categories purchased",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 1.0,
          "max": 8.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both"
      },
      {
        "name": "customer_engagement_score",
        "description": "Composite engagement score based on website activity and interactions",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65.0,
          "std": 20.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both"
      },
      {
        "name": "seasonal_purchase_ratio",
        "description": "Ratio of purchases during peak seasons vs off-peak",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 1.3,
          "std": 0.4,
          "min_clip": 0.5,
          "max_clip": 3.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both"
      }
    ],
    "target": {
      "name": "customer_lifetime_value",
      "description": "Predict total revenue from a customer over their relationship with company",
      "data_type": "float",
      "expression": "500 + 180*purchase_frequency + 12*avg_transaction_value + 25*tenure_months + 45*premium_product_purchases + 15*product_category_diversity + 3.2*customer_engagement_score + 85*seasonal_purchase_ratio",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both"
    }
  }
}