{
  "dataset_config": {
    "name": "ds051_business_website_conversion_rate_dirty",
    "description": "Predict percentage of visitors who complete desired action based on website performance metrics",
    "random_seed": 51,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "page_load_time",
          "mobile_responsiveness_score"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "cta_clarity_score",
          "trust_signals_count"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "mobile_responsiveness_score",
          "cta_clarity_score"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "page_load_time",
          "bounce_rate"
        ],
        "correlation": 0.61,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "page_load_time",
        "description": "Average page load time in seconds",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 3.2,
          "location": 0.8
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "mobile_responsiveness_score",
        "description": "Mobile responsiveness score from 0-100",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 72.5,
          "std": 18.2,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "cta_clarity_score",
        "description": "Call-to-action clarity score from 1-10",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.8,
          "std": 1.9,
          "min_clip": 1,
          "max_clip": 10
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "trust_signals_count",
        "description": "Number of trust signals on page (testimonials, reviews, badges)",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 4.2,
          "location": 0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "bounce_rate",
        "description": "Percentage of visitors who leave after viewing one page",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 42.3,
          "std": 15.6,
          "min_clip": 5,
          "max_clip": 95
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "session_duration",
        "description": "Average session duration in minutes",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 3.8,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "social_proof_elements",
        "description": "Number of social proof elements (share counts, user counters)",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 8
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "page_complexity_score",
        "description": "Page complexity score based on elements and content density (1-10)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.2,
          "std": 2.1,
          "min_clip": 1,
          "max_clip": 10
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "website_conversion_rate",
      "description": "Percentage of visitors who complete desired action",
      "data_type": "float",
      "expression": "8.5 - 1.8*page_load_time + 0.15*mobile_responsiveness_score + 0.8*cta_clarity_score + 0.3*trust_signals_count - 0.12*bounce_rate + 0.4*session_duration + 0.2*social_proof_elements - 0.3*page_complexity_score",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}