{
  "dataset_config": {
    "name": "ds005_neuroscience_cognitive_test_score_dirty",
    "description": "Predict performance on standardized cognitive assessments based on demographic and lifestyle factors",
    "random_seed": 5,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "age",
          "education_years"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "sleep_hours",
          "stress_level"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "exercise_frequency",
          "stress_level"
        ],
        "correlation": -0.4,
        "method": "cholesky"
      },
      {
        "variables": [
          "education_years",
          "cognitive_test_score"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "sleep_hours",
          "exercise_frequency"
        ],
        "correlation": 0.32,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "age",
        "description": "Age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 45,
          "std": 15,
          "min_clip": 18,
          "max_clip": 85
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "education_years",
        "description": "Years of formal education",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 14,
          "std": 3,
          "min_clip": 8,
          "max_clip": 22
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "sleep_hours",
        "description": "Average hours of sleep per night",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.2,
          "std": 1.1,
          "min_clip": 4.0,
          "max_clip": 11.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "exercise_frequency",
        "description": "Hours of exercise per week",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 4.5,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "stress_level",
        "description": "Self-reported stress level on 1-10 scale",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.5,
          "std": 2.0,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "caffeine_intake",
        "description": "Daily caffeine consumption in mg",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 150,
          "location": 0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "social_interaction_hours",
        "description": "Hours per week in social activities",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 12,
          "std": 6,
          "min_clip": 0,
          "max_clip": 40
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "cognitive_test_score",
      "description": "Standardized cognitive assessment score (0-100)",
      "data_type": "float",
      "expression": "65 + 2.5*education_years - 0.15*age + 3.2*sleep_hours + 1.8*exercise_frequency - 2.1*stress_level + 0.02*caffeine_intake + 0.8*social_interaction_hours",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}