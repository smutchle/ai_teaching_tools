{
  "dataset_config": {
    "name": "ds043_geoscience_rock_type_igneoussedimentarymetamorphic_dirty",
    "description": "Geological rock type classification based on mineral composition, texture, structure, formation depth, and age",
    "random_seed": 43,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "quartz_content",
          "feldspar_content"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "formation_depth_km",
          "metamorphic_grade"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "grain_size_mm",
          "porosity_percent"
        ],
        "correlation": -0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "age_million_years",
          "formation_depth_km"
        ],
        "correlation": 0.34,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "quartz_content",
        "description": "Quartz mineral content percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 35.0,
          "std": 18.0,
          "min_clip": 0.0,
          "max_clip": 80.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "feldspar_content",
        "description": "Feldspar mineral content percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 28.0,
          "std": 15.0,
          "min_clip": 0.0,
          "max_clip": 70.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "grain_size_mm",
        "description": "Average grain size in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 2.5,
          "location": 0.1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "porosity_percent",
        "description": "Rock porosity percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 15.0,
          "std": 8.0,
          "min_clip": 0.5,
          "max_clip": 45.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "formation_depth_km",
        "description": "Formation depth below surface in kilometers",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.2,
          "scale": 8.0,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "age_million_years",
        "description": "Geological age in million years",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 180.0,
          "location": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "metamorphic_grade",
        "description": "Metamorphic grade index (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.0,
          "max": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "density_g_cm3",
        "description": "Rock density in grams per cubic centimeter",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2.65,
          "std": 0.35,
          "min_clip": 1.8,
          "max_clip": 3.8
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "rock_type_igneoussedimentarymetamorphic",
      "description": "Rock type classification: Igneous, Sedimentary, or Metamorphic",
      "data_type": "categorical",
      "expression": "2.5 + 0.08*quartz_content - 0.06*feldspar_content - 0.3*grain_size_mm + 0.15*porosity_percent + 0.4*metamorphic_grade - 0.02*formation_depth_km + 0.005*age_million_years + 1.2*density_g_cm3",
      "noise_percent": 2.5,
      "categories": [
        "Sedimentary",
        "Sedimentary",
        "Sedimentary",
        "Sedimentary",
        "Igneous",
        "Igneous",
        "Igneous",
        "Metamorphic",
        "Metamorphic",
        "Metamorphic"
      ],
      "missing_rate": 0.02
    }
  }
}