{
  "dataset_config": {
    "name": "ds071_biology_metabolic_rate_dirty",
    "description": "Estimate energy expenditure in kilocalories per day",
    "random_seed": 71,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "body_mass_kg",
          "metabolic_rate"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "body_mass_kg",
          "age_years"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "core_temperature_c",
          "activity_level_mets"
        ],
        "correlation": 0.42,
        "method": "cholesky"
      },
      {
        "variables": [
          "protein_intake_g",
          "fat_intake_g"
        ],
        "correlation": 0.48,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "body_mass_kg",
        "description": "Body mass in kilograms",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 70.0,
          "std": 15.0,
          "min_clip": 40.0,
          "max_clip": 120.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "core_temperature_c",
        "description": "Core body temperature in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 37.0,
          "std": 0.5,
          "min_clip": 35.5,
          "max_clip": 39.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "activity_level_mets",
        "description": "Activity level in metabolic equivalents",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 1.8,
          "location": 1.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "protein_intake_g",
        "description": "Daily protein intake in grams",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 80.0,
          "std": 20.0,
          "min_clip": 30.0,
          "max_clip": 150.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "fat_intake_g",
        "description": "Daily fat intake in grams",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 70.0,
          "std": 25.0,
          "min_clip": 20.0,
          "max_clip": 140.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "carb_intake_g",
        "description": "Daily carbohydrate intake in grams",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 250.0,
          "std": 50.0,
          "min_clip": 100.0,
          "max_clip": 400.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "age_years",
        "description": "Age in years",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 18,
          "max": 80
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "sleep_hours",
        "description": "Average daily sleep hours",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.5,
          "std": 1.2,
          "min_clip": 4.0,
          "max_clip": 12.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "metabolic_rate",
      "description": "Daily energy expenditure in kilocalories",
      "data_type": "float",
      "expression": "1200 + 22*body_mass_kg + 150*activity_level_mets + 80*(core_temperature_c - 37) + 3.2*protein_intake_g + 2.8*fat_intake_g + 1.6*carb_intake_g - 8*age_years + 15*sleep_hours",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}