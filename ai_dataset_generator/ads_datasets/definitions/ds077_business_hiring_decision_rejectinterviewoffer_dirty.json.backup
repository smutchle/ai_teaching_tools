{
  "dataset_config": {
    "name": "ds077_business_hiring_decision_rejectinterviewoffer_dirty",
    "description": "Categorize candidate progression stages based on resume keywords, experience, education, skills match, and references",
    "random_seed": 77,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "resume_keywords_count",
          "skills_match_score"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "experience_years",
          "education_level_score"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "skills_match_score",
          "reference_quality_score"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "resume_keywords_count",
        "description": "Number of relevant keywords found in resume",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 12,
          "std": 4,
          "min_clip": 0,
          "max_clip": 25
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "experience_years",
        "description": "Years of relevant work experience",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 8.0,
          "location": 0.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "education_level_score",
        "description": "Education level score (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.5,
          "std": 2.0,
          "min_clip": 1,
          "max_clip": 10
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "skills_match_score",
        "description": "Percentage match of candidate skills to job requirements",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 18,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "reference_quality_score",
        "description": "Quality score of professional references (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.2,
          "std": 1.8,
          "min_clip": 1,
          "max_clip": 10
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "application_completeness",
        "description": "Completeness of job application (percentage)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 45,
          "max": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "hiring_decision_rejectinterviewoffer",
      "description": "Hiring decision category: Reject, Interview, or Offer",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(\u22128 + 0.15*resume_keywords_count + 0.25*experience_years + 0.4*education_level_score + 0.08*skills_match_score + 0.6*reference_quality_score + 0.03*application_completeness)))",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "Reject",
        "Reject",
        "Reject",
        "Reject",
        "Reject",
        "Interview",
        "Interview",
        "Interview",
        "Offer",
        "Offer"
      ]
    }
  }
}