{
  "dataset_config": {
    "name": "ds098_biology_developmental_stage_5_life_phases",
    "description": "Classify organism maturation level based on size, morphological features, time since birth, and environmental cues",
    "random_seed": 98,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "body_length_mm",
          "body_mass_g"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "days_since_birth",
          "body_length_mm"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "days_since_birth",
          "body_mass_g"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "limb_development_score",
          "body_length_mm"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "reproductive_organ_size_mm",
          "body_mass_g"
        ],
        "correlation": 0.42,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "body_length_mm",
        "description": "Total body length in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.5,
          "scale": 45.0,
          "location": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "body_mass_g",
        "description": "Body mass in grams",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.2,
          "scale": 8.5,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "days_since_birth",
        "description": "Number of days since birth or hatching",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 120.0,
          "location": 1.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "limb_development_score",
        "description": "Limb development maturity score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 50.0,
          "std": 25.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "pigmentation_intensity",
        "description": "Skin/scale pigmentation intensity (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.5,
          "max": 9.8
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "reproductive_organ_size_mm",
        "description": "Size of reproductive organs in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 3.2,
          "location": 0.1
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "environmental_temperature_c",
        "description": "Average environmental temperature in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 24.0,
          "std": 6.0,
          "min_clip": 10.0,
          "max_clip": 40.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "photoperiod_hours",
        "description": "Daily light exposure in hours",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 8.0,
          "max": 16.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "developmental_stage_5_life_phases",
      "description": "Organism developmental stage classification",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-8.5 + 0.12*body_length_mm + 0.35*body_mass_g + 0.08*days_since_birth + 0.06*limb_development_score + 0.25*pigmentation_intensity + 0.45*reproductive_organ_size_mm + 0.15*environmental_temperature_c + 0.18*photoperiod_hours)))",
      "noise_percent": 2.5,
      "categories": [
        "Embryonic",
        "Embryonic",
        "Larval",
        "Larval",
        "Juvenile",
        "Juvenile",
        "Subadult",
        "Subadult",
        "Adult",
        "Adult"
      ],
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}