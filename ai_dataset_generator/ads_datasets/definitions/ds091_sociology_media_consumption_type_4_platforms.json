{
  "dataset_config": {
    "name": "ds091_sociology_media_consumption_type_4_platforms",
    "description": "Predict primary information source based on demographics and preferences",
    "random_seed": 91,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "age",
          "digital_literacy"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "income",
          "education_years"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "tech_devices_owned",
          "digital_literacy"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "social_media_hours",
          "entertainment_preference"
        ],
        "correlation": 0.41,
        "method": "cholesky"
      },
      {
        "variables": [
          "income",
          "tech_devices_owned"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "age",
        "description": "Age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 42,
          "std": 16,
          "min_clip": 18,
          "max_clip": 85
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "education_years",
        "description": "Years of formal education",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 14,
          "std": 3,
          "min_clip": 8,
          "max_clip": 22
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "income",
        "description": "Annual household income in thousands",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 65,
          "location": 20
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "tech_devices_owned",
        "description": "Number of technology devices owned",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 4.2,
          "std": 2.1,
          "min_clip": 0,
          "max_clip": 12
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "digital_literacy",
        "description": "Digital literacy score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 68,
          "std": 18,
          "min_clip": 10,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "social_media_hours",
        "description": "Daily hours spent on social media",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 2.5,
          "location": 0.2
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "news_interest",
        "description": "Interest in news and current events (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.5,
          "std": 2.2,
          "min_clip": 1,
          "max_clip": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "entertainment_preference",
        "description": "Preference for entertainment content (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.1,
          "std": 1.8,
          "min_clip": 1,
          "max_clip": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "media_consumption_type_4_platforms",
      "description": "Primary information source platform",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-2 + 0.08*digital_literacy + 0.15*social_media_hours - 0.05*age + 0.02*income + 0.12*tech_devices_owned + 0.08*news_interest + 0.06*entertainment_preference - 0.03*education_years)))",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "categories": [
        "Traditional TV",
        "Traditional TV",
        "Traditional TV",
        "Print Media",
        "Online News",
        "Online News",
        "Social Media",
        "Social Media",
        "Streaming Platforms",
        "Streaming Platforms"
      ]
    }
  }
}