{
  "dataset_config": {
    "name": "ds085_ecology_coral_reef_health_healthystressedbleacheddead",
    "description": "Coral reef health classification based on environmental factors including water temperature, pH levels, nutrient pollution, sedimentation, and fishing pressure to assess reef ecosystem condition",
    "random_seed": 85,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "water_temperature_celsius",
          "bleaching_stress_index"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "nutrient_pollution_ppm",
          "algae_coverage_percent"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "sedimentation_rate_mg_cm2_day",
          "turbidity_ntu"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "fishing_pressure_index",
          "fish_biomass_kg_hectare"
        ],
        "correlation": -0.47,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "water_temperature_celsius",
        "description": "Average water temperature in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 27.5,
          "std": 2.8,
          "min_clip": 22.0,
          "max_clip": 35.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "ph_level",
        "description": "Seawater pH level",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 8.1,
          "std": 0.25,
          "min_clip": 7.4,
          "max_clip": 8.6
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "nutrient_pollution_ppm",
        "description": "Nutrient pollution concentration in parts per million",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 12.5,
          "location": 2.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "sedimentation_rate_mg_cm2_day",
        "description": "Sedimentation rate in milligrams per square centimeter per day",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 8.2,
          "location": 1.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "fishing_pressure_index",
        "description": "Fishing pressure index from 0 to 100",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 5.0,
          "max": 85.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "bleaching_stress_index",
        "description": "Coral bleaching stress index based on temperature anomalies",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 15.8,
          "std": 8.5,
          "min_clip": 0.0,
          "max_clip": 50.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "algae_coverage_percent",
        "description": "Percentage of reef covered by algae",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 25.3,
          "std": 12.8,
          "min_clip": 2.0,
          "max_clip": 80.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "turbidity_ntu",
        "description": "Water turbidity in Nephelometric Turbidity Units",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.6,
          "scale": 4.8,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "fish_biomass_kg_hectare",
        "description": "Fish biomass in kilograms per hectare",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 850.0,
          "std": 320.0,
          "min_clip": 100.0,
          "max_clip": 2000.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "coral_reef_health_healthystressedbleacheddead",
      "description": "Coral reef health classification",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-2.5 + 0.35*(29.0 - water_temperature_celsius) + 2.8*(ph_level - 7.8) - 0.18*nutrient_pollution_ppm - 0.22*sedimentation_rate_mg_cm2_day - 0.025*fishing_pressure_index - 0.08*bleaching_stress_index - 0.035*algae_coverage_percent - 0.15*turbidity_ntu + 0.002*fish_biomass_kg_hectare)))",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0,
      "categories": [
        "Dead",
        "Dead",
        "Bleached",
        "Bleached",
        "Stressed",
        "Stressed",
        "Stressed",
        "Healthy",
        "Healthy",
        "Healthy"
      ]
    }
  }
}