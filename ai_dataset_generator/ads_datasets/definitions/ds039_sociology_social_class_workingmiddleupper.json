{
  "dataset_config": {
    "name": "ds039_sociology_social_class_workingmiddleupper",
    "description": "Categorize socioeconomic stratification based on occupation, education, income, wealth, and cultural capital",
    "random_seed": 39,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "education_years",
          "income_thousands"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "income_thousands",
          "wealth_score"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "education_years",
          "cultural_capital_index"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "occupation_prestige",
          "income_thousands"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "occupation_prestige",
          "education_years"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "education_years",
        "description": "Years of formal education completed",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 14.2,
          "std": 3.1,
          "min_clip": 8,
          "max_clip": 22
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "income_thousands",
        "description": "Annual household income in thousands",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.3,
          "scale": 65.0,
          "location": 25.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "wealth_score",
        "description": "Composite wealth index including assets and investments",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 45.0,
          "location": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "occupation_prestige",
        "description": "Occupational prestige score based on social standing",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 50.5,
          "std": 18.2,
          "min_clip": 15,
          "max_clip": 95
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "cultural_capital_index",
        "description": "Cultural capital index measuring arts, literature, and social refinement",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 42.8,
          "std": 15.6,
          "min_clip": 10,
          "max_clip": 85
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "social_network_score",
        "description": "Social network influence and connections score",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 20,
          "max": 80
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "social_class_workingmiddleupper",
      "description": "Social class categorization: Working, Middle, or Upper class",
      "data_type": "categorical",
      "expression": "0.15*education_years + 0.008*income_thousands + 0.12*wealth_score + 0.08*occupation_prestige + 0.06*cultural_capital_index + 0.04*social_network_score",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0,
      "categories": [
        "Working Class",
        "Working Class",
        "Working Class",
        "Working Class",
        "Middle Class",
        "Middle Class",
        "Middle Class",
        "Middle Class",
        "Upper Class",
        "Upper Class"
      ]
    }
  }
}