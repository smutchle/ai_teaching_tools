{
  "dataset_config": {
    "name": "ds072_biology_photosynthesis_efficiency",
    "description": "Measure carbon fixation rate per unit area based on environmental and plant factors",
    "random_seed": 72,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "temperature",
          "co2_concentration"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "water_stress_level",
          "leaf_age_days"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "light_wavelength",
          "temperature"
        ],
        "correlation": 0.25,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "light_wavelength",
        "description": "Light wavelength in nanometers",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 680,
          "std": 50,
          "min_clip": 400,
          "max_clip": 750
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "co2_concentration",
        "description": "CO2 concentration in parts per million",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 400,
          "std": 80,
          "min_clip": 200,
          "max_clip": 800
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "temperature",
        "description": "Temperature in degrees Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 25,
          "std": 8,
          "min_clip": 5,
          "max_clip": 45
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "water_stress_level",
        "description": "Water stress level (0-1 scale, 0=no stress, 1=severe stress)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 0.3,
          "location": 0.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "leaf_age_days",
        "description": "Age of leaf in days",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 25,
          "location": 5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "light_intensity",
        "description": "Light intensity in micromoles per square meter per second",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 1200,
          "std": 300,
          "min_clip": 100,
          "max_clip": 2500
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "humidity_percent",
        "description": "Relative humidity percentage",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 30,
          "max": 95
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "photosynthesis_efficiency",
      "description": "Carbon fixation rate per unit area in micromoles CO2 per square meter per second",
      "data_type": "float",
      "expression": "5.0 + 0.015*light_intensity + 0.008*co2_concentration + 0.3*temperature - 8.0*water_stress_level - 0.05*leaf_age_days + 0.002*light_wavelength + 0.01*humidity_percent - 0.002*temperature**2",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}