{
  "dataset_config": {
    "name": "ds057_neuroscience_learning_curve_slope_dirty",
    "description": "Estimate rate of skill acquisition over trials in neuroscience learning experiments",
    "random_seed": 57,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "prior_knowledge_score",
          "baseline_cognitive_ability"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "motivation_level",
          "engagement_score"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "age_years",
          "baseline_cognitive_ability"
        ],
        "correlation": -0.35,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "task_difficulty_index",
        "description": "Standardized task difficulty score from 1-10",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.5,
          "std": 1.8,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "prior_knowledge_score",
        "description": "Pre-test knowledge assessment score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65.0,
          "std": 18.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "motivation_level",
        "description": "Self-reported motivation rating (1-7 Likert scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.2,
          "std": 1.1,
          "min_clip": 1.0,
          "max_clip": 7.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "feedback_quality_rating",
        "description": "Quality of feedback provided during learning (1-5 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 3.8,
          "std": 0.9,
          "min_clip": 1.0,
          "max_clip": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "age_years",
        "description": "Participant age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 28.5,
          "std": 12.0,
          "min_clip": 18,
          "max_clip": 75
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "baseline_cognitive_ability",
        "description": "Standardized cognitive assessment score",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 100.0,
          "std": 15.0,
          "min_clip": 70.0,
          "max_clip": 140.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "engagement_score",
        "description": "Behavioral engagement during learning sessions (0-10)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.2,
          "std": 1.5,
          "min_clip": 0.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "practice_session_duration",
        "description": "Average duration of practice sessions in minutes",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 45.0,
          "std": 12.0,
          "min_clip": 15.0,
          "max_clip": 90.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "learning_curve_slope",
      "description": "Rate of skill acquisition measured as performance improvement per trial",
      "data_type": "float",
      "expression": "0.15 - 0.08*task_difficulty_index + 0.012*prior_knowledge_score + 0.25*motivation_level + 0.35*feedback_quality_rating - 0.002*age_years + 0.008*baseline_cognitive_ability + 0.18*engagement_score + 0.005*practice_session_duration",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}