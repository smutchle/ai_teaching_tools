{
  "dataset_config": {
    "name": "ds057_neuroscience_learning_curve_slope",
    "description": "Estimate rate of skill acquisition over trials in neuroscience learning experiments",
    "random_seed": 57,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "prior_knowledge_score",
          "motivation_index"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "feedback_quality_rating",
          "attention_span_minutes"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "age_years",
          "prior_knowledge_score"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "task_difficulty_level",
          "cognitive_load_score"
        ],
        "correlation": 0.67,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "task_difficulty_level",
        "description": "Standardized task difficulty rating from 1-10",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.5,
          "std": 1.8,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "prior_knowledge_score",
        "description": "Pre-test knowledge assessment score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65.0,
          "std": 18.5,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "motivation_index",
        "description": "Self-reported motivation score (1-7 Likert scale)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 5.2,
          "location": 1.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "feedback_quality_rating",
        "description": "Quality of feedback provided during learning (1-5 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 3.2,
          "std": 0.9,
          "min_clip": 1.0,
          "max_clip": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "age_years",
        "description": "Participant age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 28.5,
          "std": 12.8,
          "min_clip": 18,
          "max_clip": 75
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "cognitive_load_score",
        "description": "Measured cognitive load during task execution (1-10)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 1.5,
          "max": 9.2
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "attention_span_minutes",
        "description": "Measured attention span during learning session",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 25.0,
          "location": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "sleep_hours_previous",
        "description": "Hours of sleep the night before the learning session",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.2,
          "std": 1.1,
          "min_clip": 3.0,
          "max_clip": 12.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "learning_curve_slope",
      "description": "Rate of skill acquisition measured as improvement per trial",
      "data_type": "float",
      "expression": "0.15 - 0.025*task_difficulty_level + 0.008*prior_knowledge_score + 0.035*motivation_index + 0.045*feedback_quality_rating - 0.002*age_years - 0.018*cognitive_load_score + 0.012*attention_span_minutes + 0.028*sleep_hours_previous",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}