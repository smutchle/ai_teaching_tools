{
  "dataset_config": {
    "name": "ds002_business_employee_productivity_score_dirty",
    "description": "Employee productivity analysis dataset with years of experience, training hours, team size, and remote work patterns to predict individual output efficiency",
    "random_seed": 2,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "years_experience",
          "annual_salary"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "training_hours_completed",
          "skill_certifications"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "years_experience",
          "training_hours_completed"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "team_size",
          "project_complexity_score"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "years_experience",
        "description": "Total years of professional work experience",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.5,
          "scale": 8.0,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "training_hours_completed",
        "description": "Hours of professional training completed in past year",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 45.0,
          "std": 18.0,
          "min_clip": 0.0,
          "max_clip": 120.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "team_size",
        "description": "Number of people in immediate work team",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 7.5,
          "std": 3.2,
          "min_clip": 2,
          "max_clip": 20
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "work_from_home_days",
        "description": "Average days working from home per week",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.0,
          "max": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "annual_salary",
        "description": "Annual salary in thousands of dollars",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 75.0,
          "std": 22.0,
          "min_clip": 35.0,
          "max_clip": 180.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "skill_certifications",
        "description": "Number of professional certifications held",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 3.5,
          "location": 0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "project_complexity_score",
        "description": "Average complexity rating of assigned projects (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.2,
          "std": 1.8,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "meeting_hours_per_week",
        "description": "Average hours spent in meetings per week",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 12.5,
          "std": 4.8,
          "min_clip": 2.0,
          "max_clip": 30.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "employee_productivity_score",
      "description": "Comprehensive productivity score measuring individual output and efficiency (0-100 scale)",
      "data_type": "float",
      "expression": "35 + 2.8*years_experience + 0.4*training_hours_completed + 1.2*project_complexity_score + 0.6*skill_certifications - 0.3*meeting_hours_per_week + 0.8*work_from_home_days - 0.2*team_size",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}