{
  "dataset_config": {
    "name": "ds084_ecology_wetland_type_marshswampbogfen_dirty",
    "description": "Classify wetland ecosystem categories based on hydrology, vegetation, soil chemistry, climate, and topography",
    "random_seed": 84,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "water_depth_cm",
          "hydroperiod_months"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "soil_ph",
          "organic_matter_percent"
        ],
        "correlation": -0.42,
        "method": "cholesky"
      },
      {
        "variables": [
          "annual_precipitation_mm",
          "water_depth_cm"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "elevation_m",
          "drainage_coefficient"
        ],
        "correlation": 0.51,
        "method": "cholesky"
      },
      {
        "variables": [
          "vegetation_diversity_index",
          "organic_matter_percent"
        ],
        "correlation": 0.48,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "water_depth_cm",
        "description": "Average water depth in centimeters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 45.0,
          "location": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "hydroperiod_months",
        "description": "Duration of water presence in months per year",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 8.5,
          "std": 3.2,
          "min_clip": 0.0,
          "max_clip": 12.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "soil_ph",
        "description": "Soil pH level",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.2,
          "std": 1.4,
          "min_clip": 3.5,
          "max_clip": 8.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "organic_matter_percent",
        "description": "Percentage of organic matter in soil",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 25.0,
          "location": 8.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "vegetation_diversity_index",
        "description": "Shannon diversity index for plant species",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2.8,
          "std": 0.9,
          "min_clip": 0.5,
          "max_clip": 4.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "annual_precipitation_mm",
        "description": "Annual precipitation in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 950.0,
          "std": 280.0,
          "min_clip": 400.0,
          "max_clip": 1800.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "elevation_m",
        "description": "Elevation above sea level in meters",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 50.0,
          "max": 1200.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "drainage_coefficient",
        "description": "Soil drainage capacity coefficient",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 0.6,
          "location": 0.1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "wetland_type_marshswampbogfen",
      "description": "Wetland ecosystem category classification",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(2.5*water_depth_cm + 1.8*hydroperiod_months - 3.2*soil_ph + 0.8*organic_matter_percent + 1.4*vegetation_diversity_index + 0.002*annual_precipitation_mm - 0.003*elevation_m - 2.1*drainage_coefficient - 15)))",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "Bog",
        "Bog",
        "Fen",
        "Fen",
        "Fen",
        "Marsh",
        "Marsh",
        "Marsh",
        "Swamp",
        "Swamp"
      ]
    }
  }
}