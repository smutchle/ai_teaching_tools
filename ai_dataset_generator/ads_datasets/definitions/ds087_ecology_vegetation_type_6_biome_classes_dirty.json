{
  "dataset_config": {
    "name": "ds087_ecology_vegetation_type_6_biome_classes_dirty",
    "description": "Classify dominant plant community into 6 biome classes based on environmental factors",
    "random_seed": 87,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "mean_annual_temp",
          "growing_season_temp"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "annual_precipitation",
          "growing_season_precip"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "elevation",
          "mean_annual_temp"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "soil_ph",
          "soil_organic_content"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "mean_annual_temp",
        "description": "Mean annual temperature in degrees Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 12.5,
          "std": 8.0,
          "min_clip": -15.0,
          "max_clip": 35.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "annual_precipitation",
        "description": "Annual precipitation in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 800.0,
          "location": 100.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "elevation",
        "description": "Elevation above sea level in meters",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 1200.0,
          "location": 50.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "soil_ph",
        "description": "Soil pH level",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.8,
          "std": 1.2,
          "min_clip": 4.0,
          "max_clip": 9.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "growing_season_temp",
        "description": "Average temperature during growing season in degrees Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 18.0,
          "std": 6.5,
          "min_clip": -5.0,
          "max_clip": 40.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "growing_season_precip",
        "description": "Precipitation during growing season in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 400.0,
          "std": 180.0,
          "min_clip": 50.0,
          "max_clip": 1200.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "soil_organic_content",
        "description": "Soil organic matter content as percentage",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 4.5,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "disturbance_frequency",
        "description": "Natural disturbance frequency events per decade",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.1,
          "max": 8.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "vegetation_type_6_biome_classes",
      "description": "Dominant plant community biome classification",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(0.15*mean_annual_temp + 0.002*annual_precipitation - 0.0008*elevation + 0.8*soil_ph + 0.12*growing_season_temp + 0.001*growing_season_precip + 0.3*soil_organic_content - 0.2*disturbance_frequency - 8.5)))",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "Tundra",
        "Boreal Forest",
        "Temperate Deciduous Forest",
        "Temperate Grassland",
        "Desert",
        "Mediterranean Scrub",
        "Tropical Rainforest",
        "Tropical Savanna",
        "Montane Forest",
        "Wetland"
      ]
    }
  }
}