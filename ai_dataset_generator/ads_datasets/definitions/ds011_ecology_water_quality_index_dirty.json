{
  "dataset_config": {
    "name": "ds011_ecology_water_quality_index_dirty",
    "description": "Quantify overall water body health on continuous scale",
    "random_seed": 11,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "ph_level",
          "dissolved_oxygen"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "turbidity",
          "total_suspended_solids"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "nitrogen_concentration",
          "phosphorus_concentration"
        ],
        "correlation": 0.53,
        "method": "cholesky"
      },
      {
        "variables": [
          "water_temperature",
          "dissolved_oxygen"
        ],
        "correlation": -0.38,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "ph_level",
        "description": "pH level of water (0-14 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.2,
          "std": 0.8,
          "min_clip": 5.0,
          "max_clip": 9.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "dissolved_oxygen",
        "description": "Dissolved oxygen concentration in mg/L",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 8.5,
          "std": 2.1,
          "min_clip": 2.0,
          "max_clip": 15.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "turbidity",
        "description": "Water turbidity in NTU (Nephelometric Turbidity Units)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 12.0,
          "location": 1.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "nitrogen_concentration",
        "description": "Total nitrogen concentration in mg/L",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 3.5,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "phosphorus_concentration",
        "description": "Total phosphorus concentration in mg/L",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.9,
          "scale": 0.8,
          "location": 0.1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "water_temperature",
        "description": "Water temperature in degrees Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 18.5,
          "std": 6.2,
          "min_clip": 2.0,
          "max_clip": 32.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "total_suspended_solids",
        "description": "Total suspended solids in mg/L",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.6,
          "scale": 25.0,
          "location": 2.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "conductivity",
        "description": "Electrical conductivity in \u03bcS/cm",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 450,
          "std": 120,
          "min_clip": 100,
          "max_clip": 1200
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "water_quality_index",
      "description": "Overall water quality index (0-100 scale)",
      "data_type": "float",
      "expression": "75 + 3.5*ph_level + 2.8*dissolved_oxygen - 1.2*turbidity - 8.5*nitrogen_concentration - 12.0*phosphorus_concentration + 0.4*water_temperature - 0.8*total_suspended_solids - 0.015*conductivity",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}