{
  "dataset_config": {
    "name": "ds090_sociology_housing_tenure_ownrentother",
    "description": "Determine residential ownership status based on income, age, family size, location, and employment stability",
    "random_seed": 90,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "income",
          "age"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "income",
          "employment_stability_years"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "age",
          "family_size"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "location_desirability",
          "income"
        ],
        "correlation": 0.41,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "income",
        "description": "Annual household income in thousands",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 65,
          "location": 25
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "age",
        "description": "Age of household head",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 42,
          "std": 14,
          "min_clip": 18,
          "max_clip": 85
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "family_size",
        "description": "Number of people in household",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 2.8,
          "location": 1
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "location_desirability",
        "description": "Location desirability index (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.2,
          "std": 2.1,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "employment_stability_years",
        "description": "Years in current employment",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.4,
          "scale": 6.5,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "credit_score",
        "description": "Credit score (300-850 range)",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 675,
          "std": 95,
          "min_clip": 300,
          "max_clip": 850
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "housing_tenure_ownrentother",
      "description": "Housing tenure status: Own, Rent, or Other arrangement",
      "data_type": "categorical",
      "expression": "0.08*income + 0.12*age + 0.6*family_size + 1.2*location_desirability + 1.8*employment_stability_years + 0.02*credit_score - 25",
      "noise_percent": 2.5,
      "categories": [
        "Other",
        "Other",
        "Rent",
        "Rent",
        "Rent",
        "Rent",
        "Own",
        "Own",
        "Own",
        "Own"
      ],
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}