{
  "dataset_config": {
    "name": "ds065_sociology_income_inequality_index",
    "description": "Quantify wealth disparity using Gini coefficient with education distribution, tax policy, union strength, and industry composition",
    "random_seed": 65,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "education_completion_rate",
          "high_skill_employment_pct"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "union_membership_rate",
          "manufacturing_employment_pct"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "top_marginal_tax_rate",
          "capital_gains_tax_rate"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "education_completion_rate",
          "income_inequality_index"
        ],
        "correlation": -0.52,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "education_completion_rate",
        "description": "Percentage of population with tertiary education completion",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 35.0,
          "std": 12.0,
          "min_clip": 10.0,
          "max_clip": 70.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "top_marginal_tax_rate",
        "description": "Top marginal income tax rate percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 42.0,
          "std": 8.5,
          "min_clip": 20.0,
          "max_clip": 65.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "capital_gains_tax_rate",
        "description": "Capital gains tax rate percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 25.0,
          "std": 7.0,
          "min_clip": 0.0,
          "max_clip": 50.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "union_membership_rate",
        "description": "Labor union membership rate as percentage of workforce",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 18.0,
          "location": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "manufacturing_employment_pct",
        "description": "Percentage of workforce employed in manufacturing",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 22.0,
          "std": 6.5,
          "min_clip": 8.0,
          "max_clip": 45.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "high_skill_employment_pct",
        "description": "Percentage of workforce in high-skill professional occupations",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 28.0,
          "std": 8.0,
          "min_clip": 12.0,
          "max_clip": 55.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "financial_sector_share",
        "description": "Financial services sector share of GDP percentage",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 4.0,
          "max": 25.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "minimum_wage_ratio",
        "description": "Minimum wage as ratio of median wage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.45,
          "std": 0.12,
          "min_clip": 0.25,
          "max_clip": 0.75
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "income_inequality_index",
      "description": "Gini coefficient measuring income inequality (0-1 scale)",
      "data_type": "float",
      "expression": "0.55 - 0.003*education_completion_rate - 0.002*top_marginal_tax_rate - 0.001*capital_gains_tax_rate - 0.004*union_membership_rate + 0.001*manufacturing_employment_pct - 0.002*high_skill_employment_pct + 0.008*financial_sector_share - 0.15*minimum_wage_ratio",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}