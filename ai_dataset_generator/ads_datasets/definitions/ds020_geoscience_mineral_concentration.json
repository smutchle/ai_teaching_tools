{
  "dataset_config": {
    "name": "ds020_geoscience_mineral_concentration",
    "description": "Estimate ore grade or mineral content percentage based on geological characteristics",
    "random_seed": 20,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "depth_meters",
          "pressure_mpa"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "depth_meters",
          "temperature_celsius"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "pressure_mpa",
          "temperature_celsius"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "rock_hardness",
          "weathering_index"
        ],
        "correlation": -0.38,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "rock_type_index",
        "description": "Rock type classification index (1=sedimentary, 10=igneous)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 1.0,
          "max": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "depth_meters",
        "description": "Depth below surface in meters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 120.0,
          "location": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "temperature_celsius",
        "description": "Subsurface temperature in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 45.0,
          "std": 18.0,
          "min_clip": 5.0,
          "max_clip": 120.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "pressure_mpa",
        "description": "Hydrostatic pressure in megapascals",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 25.0,
          "std": 12.0,
          "min_clip": 1.0,
          "max_clip": 80.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "weathering_index",
        "description": "Chemical weathering intensity index (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 35.0,
          "std": 15.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "rock_hardness",
        "description": "Mohs hardness scale equivalent (1-10)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 1.5,
          "max": 9.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "porosity_percent",
        "description": "Rock porosity percentage",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.2,
          "scale": 12.0,
          "location": 2.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "ph_level",
        "description": "Groundwater pH level",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.2,
          "std": 1.5,
          "min_clip": 4.0,
          "max_clip": 11.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "mineral_concentration",
      "description": "Mineral content percentage in ore sample",
      "data_type": "float",
      "expression": "2.5 + 0.8*rock_type_index + 0.15*depth_meters + 0.12*temperature_celsius + 0.25*pressure_mpa - 0.08*weathering_index + 0.3*rock_hardness + 0.18*porosity_percent + 0.45*ph_level",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}