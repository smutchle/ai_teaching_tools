{
  "dataset_config": {
    "name": "ds082_neuroscience_handedness_leftrightambidextrous",
    "description": "Neuroscience dataset for determining motor dominance preference based on genetic markers, family history, early training, and task performance patterns",
    "random_seed": 82,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "genetic_marker_score",
          "family_history_index"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "fine_motor_asymmetry",
          "gross_motor_asymmetry"
        ],
        "correlation": 0.68,
        "method": "cholesky"
      },
      {
        "variables": [
          "early_training_bias",
          "fine_motor_asymmetry"
        ],
        "correlation": 0.42,
        "method": "cholesky"
      },
      {
        "variables": [
          "genetic_marker_score",
          "fine_motor_asymmetry"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "genetic_marker_score",
        "description": "Composite score of genetic markers associated with handedness",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.0,
          "std": 1.5,
          "min_clip": -4.0,
          "max_clip": 4.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "family_history_index",
        "description": "Weighted index of left-handedness in family members",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 2.5,
          "location": -1.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "early_training_bias",
        "description": "Score indicating directional bias in early motor training",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.2,
          "std": 1.2,
          "min_clip": -3.0,
          "max_clip": 3.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "fine_motor_asymmetry",
        "description": "Asymmetry score in fine motor task performance",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.0,
          "std": 1.8,
          "min_clip": -5.0,
          "max_clip": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "gross_motor_asymmetry",
        "description": "Asymmetry score in gross motor task performance",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.1,
          "std": 1.6,
          "min_clip": -4.5,
          "max_clip": 4.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "neural_lateralization",
        "description": "Brain lateralization index from neuroimaging",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": -2.5,
          "max": 2.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "cognitive_asymmetry",
        "description": "Asymmetry in cognitive processing tasks",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.2,
          "scale": 1.8,
          "location": -0.8
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "handedness_leftrightambidextrous",
      "description": "Motor dominance preference classification",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-0.5 + 0.8*genetic_marker_score + 0.6*family_history_index + 0.4*early_training_bias + 1.2*fine_motor_asymmetry + 0.9*gross_motor_asymmetry + 0.7*neural_lateralization + 0.3*cognitive_asymmetry)))",
      "noise_percent": 2.5,
      "categories": [
        "Left-handed",
        "Left-handed",
        "Left-handed",
        "Ambidextrous",
        "Ambidextrous",
        "Right-handed",
        "Right-handed",
        "Right-handed",
        "Right-handed",
        "Right-handed"
      ],
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}