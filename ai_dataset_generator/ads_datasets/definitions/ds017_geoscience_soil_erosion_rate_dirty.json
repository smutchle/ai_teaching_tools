{
  "dataset_config": {
    "name": "ds017_geoscience_soil_erosion_rate_dirty",
    "description": "Predict soil loss in tons per hectare per year based on slope, vegetation cover, rainfall intensity, soil type, and land use factors",
    "random_seed": 17,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "slope_angle",
          "runoff_coefficient"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "vegetation_cover_percent",
          "organic_matter_content"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "rainfall_intensity",
          "runoff_coefficient"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "clay_content_percent",
          "soil_cohesion_index"
        ],
        "correlation": 0.6,
        "method": "cholesky"
      },
      {
        "variables": [
          "slope_angle",
          "vegetation_cover_percent"
        ],
        "correlation": -0.35,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "slope_angle",
        "description": "Slope angle in degrees",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 12.0,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "vegetation_cover_percent",
        "description": "Percentage of land covered by vegetation",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65.0,
          "std": 25.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "rainfall_intensity",
        "description": "Average rainfall intensity in mm per hour",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.2,
          "scale": 8.5,
          "location": 1.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "clay_content_percent",
        "description": "Percentage of clay particles in soil",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 28.0,
          "std": 12.0,
          "min_clip": 5.0,
          "max_clip": 60.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "organic_matter_content",
        "description": "Soil organic matter content as percentage",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.5,
          "scale": 3.2,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "runoff_coefficient",
        "description": "Surface runoff coefficient (0-1 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.45,
          "std": 0.2,
          "min_clip": 0.05,
          "max_clip": 0.95
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "soil_cohesion_index",
        "description": "Soil particle cohesion index (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.5,
          "std": 2.0,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "land_disturbance_factor",
        "description": "Land use disturbance factor (1-5 scale, higher = more disturbed)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 1.0,
          "max": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "soil_erosion_rate",
      "description": "Soil loss in tons per hectare per year",
      "data_type": "float",
      "expression": "2.5 + 0.8*slope_angle + 15*runoff_coefficient + 0.4*rainfall_intensity + 1.2*land_disturbance_factor - 0.05*vegetation_cover_percent - 0.3*soil_cohesion_index - 0.2*organic_matter_content + 0.02*clay_content_percent",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}