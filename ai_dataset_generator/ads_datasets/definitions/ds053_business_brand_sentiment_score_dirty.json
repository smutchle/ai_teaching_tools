{
  "dataset_config": {
    "name": "ds053_business_brand_sentiment_score_dirty",
    "description": "Quantify public perception on continuous scale using social media mentions, review ratings, PR events, and competitor activity",
    "random_seed": 53,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "social_media_mentions",
          "review_rating_avg"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "positive_pr_events",
          "review_rating_avg"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "competitor_negative_news",
          "social_media_mentions"
        ],
        "correlation": -0.32,
        "method": "cholesky"
      },
      {
        "variables": [
          "marketing_spend",
          "positive_pr_events"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "social_media_mentions",
        "description": "Number of social media mentions per week",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 2500,
          "location": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "review_rating_avg",
        "description": "Average customer review rating (1-5 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 3.8,
          "std": 0.6,
          "min_clip": 1.0,
          "max_clip": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "positive_pr_events",
        "description": "Number of positive PR events per month",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 8,
          "std": 3,
          "min_clip": 0,
          "max_clip": 25
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "competitor_negative_news",
        "description": "Number of negative news stories about competitors",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 12,
          "location": 0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "marketing_spend",
        "description": "Monthly marketing spend in thousands USD",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 150,
          "std": 45,
          "min_clip": 20,
          "max_clip": 400
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "customer_service_score",
        "description": "Customer service satisfaction score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 72,
          "std": 12,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "influencer_endorsements",
        "description": "Number of influencer endorsements per quarter",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 2.2,
          "scale": 15,
          "location": 0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "crisis_incidents",
        "description": "Number of brand crisis incidents per year",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.2,
          "scale": 3,
          "location": 0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "brand_sentiment_score",
      "description": "Brand sentiment score on continuous scale from -100 to 100",
      "data_type": "float",
      "expression": "-20 + 0.015*social_media_mentions + 18*review_rating_avg + 2.5*positive_pr_events + 1.8*competitor_negative_news + 0.12*marketing_spend + 0.8*customer_service_score + 1.2*influencer_endorsements - 8*crisis_incidents",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}