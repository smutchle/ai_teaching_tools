{
  "dataset_config": {
    "name": "ds100_business_delivery_success_on_timedelayedfailed",
    "description": "Predict shipment outcome categories based on distance, traffic, weather, package size, carrier, and time window",
    "random_seed": 100,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "delivery_distance_km",
          "base_delivery_time_hours"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "traffic_congestion_index",
          "weather_severity_score"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "package_weight_kg",
          "package_volume_m3"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "carrier_reliability_score",
          "delivery_time_window_hours"
        ],
        "correlation": 0.42,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "delivery_distance_km",
        "description": "Distance from warehouse to delivery address in kilometers",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 25,
          "location": 2
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "traffic_congestion_index",
        "description": "Traffic congestion level from 0-100",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 45,
          "std": 20,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "weather_severity_score",
        "description": "Weather impact score from 0-10",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.5,
          "scale": 3,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "package_weight_kg",
        "description": "Package weight in kilograms",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 8,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "package_volume_m3",
        "description": "Package volume in cubic meters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 0.15,
          "location": 0.01
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "carrier_reliability_score",
        "description": "Carrier reliability rating from 1-10",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.2,
          "std": 1.5,
          "min_clip": 1,
          "max_clip": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "delivery_time_window_hours",
        "description": "Available delivery window in hours",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 1,
          "max": 8
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "base_delivery_time_hours",
        "description": "Estimated base delivery time in hours",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6,
          "std": 3,
          "min_clip": 0.5,
          "max_clip": 24
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "delivery_success_on_timedelayedfailed",
      "description": "Delivery outcome classification",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-2 + 0.8*carrier_reliability_score + 0.6*delivery_time_window_hours - 0.05*delivery_distance_km - 0.03*traffic_congestion_index - 0.4*weather_severity_score - 0.08*package_weight_kg - 2*package_volume_m3 + 0.1*base_delivery_time_hours)))",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "categories": [
        "Failed",
        "Failed",
        "Delayed",
        "Delayed",
        "Delayed",
        "Delayed",
        "On-time",
        "On-time",
        "On-time",
        "On-time"
      ]
    }
  }
}