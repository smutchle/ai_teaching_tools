{
  "dataset_config": {
    "name": "ds028_business_product_purchase_yesno_dirty",
    "description": "Predict whether visitor will complete purchase",
    "random_seed": 28,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "browsing_time_minutes",
          "cart_additions"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "email_opens",
          "previous_purchases"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "age",
          "annual_income_k"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "cart_additions",
          "product_views"
        ],
        "correlation": 0.7,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "browsing_time_minutes",
        "description": "Total time spent browsing the website in minutes",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 12.0,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "cart_additions",
        "description": "Number of items added to shopping cart",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 2.5,
          "location": 0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "email_opens",
        "description": "Number of marketing emails opened in last 30 days",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 4.0,
          "location": 0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "product_views",
        "description": "Number of product pages viewed in current session",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.6,
          "scale": 8.0,
          "location": 1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "previous_purchases",
        "description": "Number of previous purchases with the company",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.2,
          "scale": 3.0,
          "location": 0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "age",
        "description": "Customer age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 42,
          "std": 15,
          "min_clip": 18,
          "max_clip": 80
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "annual_income_k",
        "description": "Annual household income in thousands",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 25,
          "min_clip": 20,
          "max_clip": 200
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "session_pages",
        "description": "Total number of pages visited in current session",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.4,
          "scale": 12.0,
          "location": 2
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "product_purchase_yesno",
      "description": "Whether visitor completed a product purchase",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-4.5 + 0.15*browsing_time_minutes + 0.8*cart_additions + 0.12*email_opens + 0.08*product_views + 0.25*previous_purchases + 0.02*annual_income_k - 0.01*age + 0.05*session_pages)))",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "No Purchase",
        "No Purchase",
        "No Purchase",
        "No Purchase",
        "No Purchase",
        "No Purchase",
        "Purchase",
        "Purchase",
        "Purchase",
        "Purchase"
      ]
    }
  }
}