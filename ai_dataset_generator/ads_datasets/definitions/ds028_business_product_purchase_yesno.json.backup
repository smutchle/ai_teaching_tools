{
  "dataset_config": {
    "name": "ds028_business_product_purchase_yesno",
    "description": "Predict whether visitor will complete purchase based on browsing behavior and demographics",
    "random_seed": 28,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "browsing_time_minutes",
          "cart_additions"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "email_opens",
          "previous_purchases"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "age",
          "income_thousands"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "cart_additions",
          "cart_value"
        ],
        "correlation": 0.73,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "browsing_time_minutes",
        "description": "Total time spent browsing website in minutes",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 15.0,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "cart_additions",
        "description": "Number of items added to shopping cart",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 3.2,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "email_opens",
        "description": "Number of marketing emails opened in last 30 days",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.2,
          "scale": 4.5,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "age",
        "description": "Customer age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 42,
          "std": 15,
          "min_clip": 18,
          "max_clip": 80
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "income_thousands",
        "description": "Annual household income in thousands",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 25,
          "min_clip": 20,
          "max_clip": 200
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "previous_purchases",
        "description": "Number of purchases made in last 12 months",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.3,
          "scale": 2.8,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "cart_value",
        "description": "Total value of items in cart in dollars",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.6,
          "scale": 120,
          "location": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "page_views",
        "description": "Number of product pages viewed in current session",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.4,
          "scale": 8.5,
          "location": 1
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "product_purchase_yesno",
      "description": "Whether visitor completed a product purchase",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(\u22124.5 + 0.08*browsing_time_minutes + 0.35*cart_additions + 0.12*email_opens + 0.015*age + 0.02*income_thousands + 0.25*previous_purchases + 0.005*cart_value + 0.06*page_views)))",
      "noise_percent": 2.5,
      "categories": [
        "No",
        "No",
        "No",
        "No",
        "No",
        "No",
        "Yes",
        "Yes",
        "Yes",
        "Yes"
      ],
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}