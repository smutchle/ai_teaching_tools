{
  "dataset_config": {
    "name": "ds076_business_fraud_detection_genuinefraudulent_dirty",
    "description": "Identify suspicious financial activities",
    "random_seed": 76,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "transaction_amount",
          "merchant_risk_score"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "velocity_transactions_hour",
          "velocity_amount_hour"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "location_risk_score",
          "merchant_risk_score"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "time_since_last_transaction",
          "velocity_transactions_hour"
        ],
        "correlation": -0.42,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "transaction_amount",
        "description": "Transaction amount in USD",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 0.8,
          "scale": 150,
          "location": 5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "location_risk_score",
        "description": "Risk score based on transaction location",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 35,
          "std": 15,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "time_since_last_transaction",
        "description": "Minutes since last transaction by this user",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.2,
          "scale": 480,
          "location": 1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "merchant_risk_score",
        "description": "Risk score for merchant category",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 25,
          "std": 12,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "velocity_transactions_hour",
        "description": "Number of transactions in past hour",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 0.9,
          "scale": 2.5,
          "location": 1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "velocity_amount_hour",
        "description": "Total amount transacted in past hour",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.1,
          "scale": 300,
          "location": 0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "fraud_detection_genuinefraudulent",
      "description": "Fraud detection classification",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-4.5 + 0.008*transaction_amount + 0.05*location_risk_score - 0.002*time_since_last_transaction + 0.06*merchant_risk_score + 0.3*velocity_transactions_hour + 0.003*velocity_amount_hour)))",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "Genuine",
        "Genuine",
        "Genuine",
        "Genuine",
        "Genuine",
        "Genuine",
        "Genuine",
        "Genuine",
        "Fraudulent",
        "Fraudulent"
      ]
    }
  }
}