{
  "dataset_config": {
    "name": "ds009_ecology_species_abundance_dirty",
    "description": "Estimate population count of a species in a given area",
    "random_seed": 9,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "temperature",
          "precipitation"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "elevation",
          "temperature"
        ],
        "correlation": -0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "habitat_area",
          "species_abundance"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "predator_density",
          "prey_availability"
        ],
        "correlation": -0.41,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "temperature",
        "description": "Average annual temperature in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 15.2,
          "std": 8.5,
          "min_clip": -5.0,
          "max_clip": 35.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "precipitation",
        "description": "Annual precipitation in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 800,
          "location": 200
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "elevation",
        "description": "Elevation above sea level in meters",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 50,
          "max": 3200
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "habitat_area",
        "description": "Available habitat area in square kilometers",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 25,
          "location": 2
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "predator_density",
        "description": "Number of predators per square kilometer",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 3.5,
          "std": 1.8,
          "min_clip": 0.1,
          "max_clip": 12.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "prey_availability",
        "description": "Prey species density index (0-100 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 45,
          "std": 18,
          "min_clip": 5,
          "max_clip": 95
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "vegetation_density",
        "description": "Vegetation cover percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 22,
          "min_clip": 5,
          "max_clip": 98
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "human_disturbance",
        "description": "Human disturbance index (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 4.2,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "species_abundance",
      "description": "Population count of species in the area",
      "data_type": "float",
      "expression": "np.exp(2.1 + 0.08*temperature + 0.002*precipitation - 0.0003*elevation + 0.15*habitat_area - 0.25*predator_density + 0.02*prey_availability + 0.01*vegetation_density - 0.12*human_disturbance)",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}