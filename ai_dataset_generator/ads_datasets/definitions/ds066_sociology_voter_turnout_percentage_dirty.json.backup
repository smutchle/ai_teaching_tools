{
  "dataset_config": {
    "name": "ds066_sociology_voter_turnout_percentage_dirty",
    "description": "Predict electoral participation rate based on competitiveness, weather, registration barriers, civic education, and age demographics",
    "random_seed": 66,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "election_competitiveness",
          "voter_turnout_percentage"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "civic_education_hours",
          "median_age"
        ],
        "correlation": 0.42,
        "method": "cholesky"
      },
      {
        "variables": [
          "registration_difficulty_score",
          "voter_turnout_percentage"
        ],
        "correlation": -0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "weather_favorability",
          "voter_turnout_percentage"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "elderly_population_pct",
          "median_age"
        ],
        "correlation": 0.73,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "election_competitiveness",
        "description": "Margin of victory in previous election (lower = more competitive)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 12.0,
          "location": 1.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "weather_favorability",
        "description": "Weather favorability score on election day (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65.0,
          "std": 18.0,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "registration_difficulty_score",
        "description": "Difficulty of voter registration process (1-10, higher = more difficult)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.5,
          "std": 2.2,
          "min_clip": 1,
          "max_clip": 10
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "civic_education_hours",
        "description": "Average hours of civic education in local schools per year",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 25.0,
          "std": 8.5,
          "min_clip": 5,
          "max_clip": 60
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "median_age",
        "description": "Median age of registered voters in the district",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 45.2,
          "std": 7.8,
          "min_clip": 25,
          "max_clip": 70
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "elderly_population_pct",
        "description": "Percentage of population aged 65 and older",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 16.8,
          "std": 5.2,
          "min_clip": 5,
          "max_clip": 35
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "polling_locations_per_1000",
        "description": "Number of polling locations per 1000 registered voters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 1.8,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "voter_turnout_percentage",
      "description": "Percentage of registered voters who participated in the election",
      "data_type": "float",
      "expression": "45 + 1.2*election_competitiveness + 0.15*weather_favorability - 2.8*registration_difficulty_score + 0.8*civic_education_hours + 0.3*median_age + 0.4*elderly_population_pct + 3.5*polling_locations_per_1000",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}