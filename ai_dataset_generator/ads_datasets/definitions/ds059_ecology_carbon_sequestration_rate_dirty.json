{
  "dataset_config": {
    "name": "ds059_ecology_carbon_sequestration_rate_dirty",
    "description": "Predict tons of CO2 absorbed per hectare annually based on vegetation, soil, climate and management factors",
    "random_seed": 59,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "soil_organic_matter",
          "vegetation_biomass"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "annual_rainfall",
          "vegetation_biomass"
        ],
        "correlation": 0.42,
        "method": "cholesky"
      },
      {
        "variables": [
          "mean_temperature",
          "annual_rainfall"
        ],
        "correlation": -0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "soil_organic_matter",
          "management_intensity"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "vegetation_biomass",
        "description": "Above-ground vegetation biomass in tons per hectare",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 85.0,
          "location": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "soil_organic_matter",
        "description": "Soil organic matter percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 3.8,
          "std": 1.2,
          "min_clip": 0.5,
          "max_clip": 12.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "annual_rainfall",
        "description": "Annual rainfall in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 850.0,
          "std": 280.0,
          "min_clip": 200.0,
          "max_clip": 2500.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "mean_temperature",
        "description": "Mean annual temperature in degrees Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 15.2,
          "std": 8.5,
          "min_clip": -10.0,
          "max_clip": 35.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "management_intensity",
        "description": "Land management intensity score (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.5,
          "max": 9.8
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "forest_age",
        "description": "Average age of dominant vegetation in years",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 45.0,
          "location": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "soil_ph",
        "description": "Soil pH level",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.2,
          "std": 0.8,
          "min_clip": 4.0,
          "max_clip": 8.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "elevation",
        "description": "Site elevation in meters above sea level",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 50,
          "max": 2800
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "carbon_sequestration_rate",
      "description": "Tons of CO2 absorbed per hectare annually",
      "data_type": "float",
      "expression": "2.5 + 0.08*vegetation_biomass + 1.2*soil_organic_matter + 0.003*annual_rainfall - 0.15*mean_temperature + 0.4*management_intensity + 0.02*forest_age + 0.8*soil_ph - 0.0008*elevation",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}