{
  "dataset_config": {
    "name": "ds041_sociology_marriage_stability_stableat_riskdissolved",
    "description": "Predict relationship outcome categories based on marriage duration, demographics, and relationship factors",
    "random_seed": 41,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "age_at_marriage",
          "household_income"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "marriage_duration_years",
          "number_of_children"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "household_income",
          "number_of_children"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "conflict_frequency_per_month",
          "communication_quality_score"
        ],
        "correlation": -0.6,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "marriage_duration_years",
        "description": "Number of years married",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 12.0,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "age_at_marriage",
        "description": "Age when married",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 27.5,
          "std": 6.2,
          "min_clip": 18,
          "max_clip": 45
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "household_income",
        "description": "Annual household income in thousands",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 72.0,
          "std": 28.0,
          "min_clip": 20.0,
          "max_clip": 180.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "number_of_children",
        "description": "Number of children in the household",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.2,
          "scale": 2.1,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "conflict_frequency_per_month",
        "description": "Number of significant conflicts per month",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 4.2,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "communication_quality_score",
        "description": "Communication quality rating from 1-10",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.8,
          "std": 2.1,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "financial_stress_level",
        "description": "Financial stress level from 1-10",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.2,
          "std": 2.5,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "marriage_stability_stableat_riskdissolved",
      "description": "Marriage stability outcome category",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(2.5 + 0.15*marriage_duration_years + 0.08*age_at_marriage + 0.02*household_income - 0.12*number_of_children - 0.45*conflict_frequency_per_month + 0.35*communication_quality_score - 0.25*financial_stress_level)))",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0,
      "categories": [
        "Dissolved",
        "Dissolved",
        "At-risk",
        "At-risk",
        "At-risk",
        "At-risk",
        "Stable",
        "Stable",
        "Stable",
        "Stable"
      ]
    }
  }
}