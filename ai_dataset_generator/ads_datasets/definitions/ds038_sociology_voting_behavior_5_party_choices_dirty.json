{
  "dataset_config": {
    "name": "ds038_sociology_voting_behavior_5_party_choices_dirty",
    "description": "Predict electoral preferences based on demographic and social factors",
    "random_seed": 38,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "age",
          "income"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "education_years",
          "income"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "urban_rural_index",
          "income"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "social_conservatism_score",
          "age"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "education_years",
          "social_progressivism_score"
        ],
        "correlation": 0.42,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "age",
        "description": "Age of voter in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 45,
          "std": 16,
          "min_clip": 18,
          "max_clip": 85
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "income",
        "description": "Annual household income in thousands",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 55,
          "location": 25
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "education_years",
        "description": "Years of formal education completed",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 13.5,
          "std": 3.2,
          "min_clip": 8,
          "max_clip": 22
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "urban_rural_index",
        "description": "Urban-rural index (0=rural, 100=urban)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "social_conservatism_score",
        "description": "Social conservatism score (0-100 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 50,
          "std": 20,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "social_progressivism_score",
        "description": "Social progressivism score (0-100 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 45,
          "std": 22,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "economic_satisfaction",
        "description": "Personal economic satisfaction index (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 55,
          "std": 18,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "voting_behavior_5_party_choices",
      "description": "Electoral preference among 5 major political parties",
      "data_type": "categorical",
      "expression": "-2 + 0.02*age + 0.015*income + 0.25*education_years + 0.01*urban_rural_index - 0.03*social_conservatism_score + 0.035*social_progressivism_score + 0.02*economic_satisfaction",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "Conservative Party",
        "Conservative Party",
        "Liberal Party",
        "Liberal Party",
        "Progressive Party",
        "Progressive Party",
        "Green Party",
        "Socialist Party",
        "Socialist Party",
        "Independent"
      ]
    }
  }
}