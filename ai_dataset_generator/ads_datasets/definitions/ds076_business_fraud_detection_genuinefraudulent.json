{
  "dataset_config": {
    "name": "ds076_business_fraud_detection_genuinefraudulent",
    "description": "Fraud detection dataset for identifying suspicious financial activities based on transaction patterns",
    "random_seed": 76,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "transaction_amount",
          "merchant_risk_score"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "transaction_velocity",
          "location_risk_index"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "time_anomaly_score",
          "transaction_velocity"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "transaction_amount",
        "description": "Transaction amount in dollars",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 150.0,
          "location": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "location_risk_index",
        "description": "Risk index based on transaction location (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 25.0,
          "std": 15.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "time_anomaly_score",
        "description": "Anomaly score based on transaction timing patterns (0-10)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2.5,
          "std": 1.8,
          "min_clip": 0.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "merchant_risk_score",
        "description": "Risk score associated with merchant category (0-50)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.0,
          "max": 50.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "transaction_velocity",
        "description": "Number of transactions per hour from same account",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 3.5,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "account_age_days",
        "description": "Age of account in days",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 800,
          "std": 450,
          "min_clip": 1,
          "max_clip": 3650
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "cross_border_indicator",
        "description": "Cross-border transaction indicator (0-1)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.0,
          "max": 1.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "fraud_detection_genuinefraudulent",
      "description": "Fraud detection classification: Genuine or Fraudulent transaction",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-6.5 + 0.008*transaction_amount + 0.12*location_risk_index + 0.85*time_anomaly_score + 0.06*merchant_risk_score + 0.95*transaction_velocity - 0.002*account_age_days + 3.2*cross_border_indicator)))",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0,
      "categories": [
        "Genuine",
        "Genuine",
        "Genuine",
        "Genuine",
        "Genuine",
        "Genuine",
        "Genuine",
        "Genuine",
        "Fraudulent",
        "Fraudulent"
      ]
    }
  }
}