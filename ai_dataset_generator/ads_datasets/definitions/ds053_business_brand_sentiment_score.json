{
  "dataset_config": {
    "name": "ds053_business_brand_sentiment_score",
    "description": "Quantify public perception on continuous scale using social media mentions, review ratings, PR events, and competitor activity",
    "random_seed": 53,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "social_media_mentions",
          "review_rating_avg"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "pr_event_impact",
          "media_coverage_score"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "competitor_negative_mentions",
          "market_share_loss"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "customer_service_rating",
          "review_rating_avg"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "social_media_mentions",
        "description": "Number of social media mentions per day",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 850,
          "location": 50
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "review_rating_avg",
        "description": "Average review rating on scale 1-5",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 3.8,
          "std": 0.6,
          "min_clip": 1.0,
          "max_clip": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "pr_event_impact",
        "description": "Impact score of PR events (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "competitor_negative_mentions",
        "description": "Number of negative mentions about competitors",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 120,
          "location": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "media_coverage_score",
        "description": "Media coverage sentiment score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 18,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "customer_service_rating",
        "description": "Customer service satisfaction rating (1-10)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.2,
          "std": 1.4,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "market_share_loss",
        "description": "Market share loss percentage (0-15)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 15
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "influencer_endorsements",
        "description": "Number of influencer endorsements per month",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 25,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "brand_sentiment_score",
      "description": "Brand sentiment score on continuous scale (0-100)",
      "data_type": "float",
      "expression": "30 + 0.015*social_media_mentions + 8.5*review_rating_avg + 0.25*pr_event_impact + 0.08*competitor_negative_mentions + 0.18*media_coverage_score + 2.2*customer_service_rating - 0.9*market_share_loss + 0.4*influencer_endorsements",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}