{
  "dataset_config": {
    "name": "ds014_sociology_community_trust_index_dirty",
    "description": "Quantify interpersonal trust levels within communities",
    "random_seed": 14,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "median_income",
          "civic_participation_rate"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "crime_rate_per_1000",
          "median_income"
        ],
        "correlation": -0.42,
        "method": "cholesky"
      },
      {
        "variables": [
          "diversity_index",
          "civic_participation_rate"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "average_tenure_years",
          "median_income"
        ],
        "correlation": 0.48,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "crime_rate_per_1000",
        "description": "Number of crimes per 1000 residents annually",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 25.0,
          "location": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "diversity_index",
        "description": "Cultural diversity index (0-1 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.65,
          "std": 0.18,
          "min_clip": 0.0,
          "max_clip": 1.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "median_income",
        "description": "Median household income in thousands of dollars",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 68.5,
          "std": 22.0,
          "min_clip": 25.0,
          "max_clip": 150.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "civic_participation_rate",
        "description": "Percentage of residents participating in civic activities",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 42.0,
          "std": 15.0,
          "min_clip": 5.0,
          "max_clip": 85.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "average_tenure_years",
        "description": "Average number of years residents have lived in community",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.2,
          "scale": 12.0,
          "location": 2.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "population_density",
        "description": "Number of people per square kilometer",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 2500.0,
          "location": 100.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "community_trust_index",
      "description": "Community interpersonal trust index (0-100 scale)",
      "data_type": "float",
      "expression": "45 + 0.35*median_income + 0.25*civic_participation_rate + 0.15*average_tenure_years + 8*diversity_index - 0.4*crime_rate_per_1000 - 0.002*population_density",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}