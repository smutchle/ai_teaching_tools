{
  "dataset_config": {
    "name": "ds065_sociology_income_inequality_index_dirty",
    "description": "Quantify wealth disparity using Gini coefficient based on education distribution, tax policy, union strength, and industry composition",
    "random_seed": 65,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "higher_education_rate",
          "avg_tax_rate"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "union_membership_rate",
          "manufacturing_share"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "higher_education_rate",
          "service_sector_share"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "avg_tax_rate",
          "social_spending_pct"
        ],
        "correlation": 0.67,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "higher_education_rate",
        "description": "Percentage of population with higher education",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 35.2,
          "std": 12.8,
          "min_clip": 8.0,
          "max_clip": 75.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "avg_tax_rate",
        "description": "Average effective tax rate percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 28.5,
          "std": 8.3,
          "min_clip": 10.0,
          "max_clip": 55.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "union_membership_rate",
        "description": "Labor union membership rate percentage",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 22.0,
          "location": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "manufacturing_share",
        "description": "Manufacturing sector employment share percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 18.7,
          "std": 7.2,
          "min_clip": 3.0,
          "max_clip": 45.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "service_sector_share",
        "description": "Service sector employment share percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 68.4,
          "std": 9.6,
          "min_clip": 35.0,
          "max_clip": 85.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "social_spending_pct",
        "description": "Social spending as percentage of GDP",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 15.8,
          "std": 6.4,
          "min_clip": 2.0,
          "max_clip": 35.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "gdp_per_capita_k",
        "description": "GDP per capita in thousands USD",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 42.6,
          "std": 18.3,
          "min_clip": 8.0,
          "max_clip": 120.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "min_wage_ratio",
        "description": "Minimum wage as ratio of median wage",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.25,
          "max": 0.75
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "income_inequality_index",
      "description": "Income inequality index (Gini coefficient scaled 0-100)",
      "data_type": "float",
      "expression": "45.0 - 0.18*higher_education_rate - 0.24*avg_tax_rate - 0.15*union_membership_rate + 0.08*manufacturing_share - 0.12*service_sector_share - 0.32*social_spending_pct - 0.06*gdp_per_capita_k + 8.5*min_wage_ratio",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}