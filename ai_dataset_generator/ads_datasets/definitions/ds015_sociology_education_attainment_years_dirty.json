{
  "dataset_config": {
    "name": "ds015_sociology_education_attainment_years_dirty",
    "description": "Predict total years of formal education completed based on socioeconomic and environmental factors",
    "random_seed": 15,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "parent_education_years",
          "household_income"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "household_income",
          "school_quality_score"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "parent_education_years",
          "peer_college_bound_pct"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "school_quality_score",
          "teacher_student_ratio"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "peer_college_bound_pct",
          "neighborhood_education_index"
        ],
        "correlation": 0.48,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "parent_education_years",
        "description": "Average years of education completed by parents",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 13.2,
          "std": 3.8,
          "min_clip": 6.0,
          "max_clip": 22.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "household_income",
        "description": "Annual household income in thousands USD",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 65.0,
          "location": 25.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "school_quality_score",
        "description": "School quality rating from 1-100 based on standardized test scores and resources",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 72.5,
          "std": 18.2,
          "min_clip": 20.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "peer_college_bound_pct",
        "description": "Percentage of peers planning to attend college",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 58.3,
          "std": 22.1,
          "min_clip": 10.0,
          "max_clip": 95.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "teacher_student_ratio",
        "description": "Number of students per teacher in primary school",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 18.5,
          "std": 4.2,
          "min_clip": 8.0,
          "max_clip": 35.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "neighborhood_education_index",
        "description": "Composite index of neighborhood educational attainment (0-100 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 62.8,
          "std": 19.5,
          "min_clip": 15.0,
          "max_clip": 98.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "family_academic_support",
        "description": "Level of academic support at home (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.2,
          "std": 2.1,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "extracurricular_participation",
        "description": "Number of extracurricular activities participated in",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 2.8,
          "location": 0.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "education_attainment_years",
      "description": "Total years of formal education completed",
      "data_type": "float",
      "expression": "8.5 + 0.42*parent_education_years + 0.025*household_income + 0.035*school_quality_score + 0.018*peer_college_bound_pct - 0.08*teacher_student_ratio + 0.022*neighborhood_education_index + 0.28*family_academic_support + 0.15*extracurricular_participation",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}