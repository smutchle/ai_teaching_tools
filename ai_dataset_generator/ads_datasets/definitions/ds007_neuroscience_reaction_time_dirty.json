{
  "dataset_config": {
    "name": "ds007_neuroscience_reaction_time_dirty",
    "description": "Predict speed of response to stimuli in milliseconds",
    "random_seed": 7,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "age",
          "fatigue_level"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "task_complexity",
          "prior_training_hours"
        ],
        "correlation": -0.42,
        "method": "cholesky"
      },
      {
        "variables": [
          "caffeine_intake_mg",
          "fatigue_level"
        ],
        "correlation": -0.28,
        "method": "cholesky"
      },
      {
        "variables": [
          "age",
          "prior_training_hours"
        ],
        "correlation": 0.31,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "age",
        "description": "Participant age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 35,
          "std": 12,
          "min_clip": 18,
          "max_clip": 75
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "fatigue_level",
        "description": "Self-reported fatigue level (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.2,
          "std": 2.1,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "task_complexity",
        "description": "Task difficulty rating (1-5 scale)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 1.0,
          "max": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "caffeine_intake_mg",
        "description": "Caffeine consumed in past 4 hours (mg)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 85,
          "location": 0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "prior_training_hours",
        "description": "Hours of prior training on similar tasks",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.2,
          "scale": 15,
          "location": 0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "sleep_hours_last_night",
        "description": "Hours of sleep obtained last night",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.2,
          "std": 1.3,
          "min_clip": 3.0,
          "max_clip": 12.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "attention_score",
        "description": "Pre-test attention assessment score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 72,
          "std": 15,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "reaction_time",
      "description": "Speed of response to stimuli in milliseconds",
      "data_type": "float",
      "expression": "450 + 3.2*age + 18*fatigue_level + 45*task_complexity - 0.8*caffeine_intake_mg - 12*prior_training_hours + 15*(10 - sleep_hours_last_night) - 2.1*attention_score",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}