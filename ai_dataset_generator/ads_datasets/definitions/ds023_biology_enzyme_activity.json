{
  "dataset_config": {
    "name": "ds023_biology_enzyme_activity",
    "description": "Enzyme activity measurement dataset showing reaction rates in micromoles per minute based on temperature, pH, substrate concentration, inhibitor presence, and cofactor levels",
    "random_seed": 23,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "temperature",
          "enzyme_activity_base"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "ph_level",
          "substrate_concentration"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "cofactor_concentration",
          "enzyme_activity_base"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "temperature",
        "description": "Reaction temperature in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 37.0,
          "std": 8.0,
          "min_clip": 10.0,
          "max_clip": 70.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "ph_level",
        "description": "pH level of reaction solution",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.2,
          "std": 1.5,
          "min_clip": 4.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "substrate_concentration",
        "description": "Substrate concentration in mM",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 15.0,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "inhibitor_concentration",
        "description": "Competitive inhibitor concentration in mM",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 5.0,
          "location": 0.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "cofactor_concentration",
        "description": "Essential cofactor concentration in mM",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2.5,
          "std": 0.8,
          "min_clip": 0.1,
          "max_clip": 8.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "enzyme_activity_base",
        "description": "Baseline enzyme activity factor",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 50.0,
          "std": 12.0,
          "min_clip": 10.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "ionic_strength",
        "description": "Ionic strength of reaction buffer in M",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.05,
          "max": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "reaction_time",
        "description": "Reaction time in minutes",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 5.0,
          "max": 60.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "enzyme_activity",
      "description": "Enzyme activity measured in micromoles per minute",
      "data_type": "float",
      "expression": "enzyme_activity_base + 2.5*temperature - 0.08*temperature**2 + 15*np.exp(-(ph_level-7.4)**2/2) + 3.2*substrate_concentration/(substrate_concentration + 8.5) - 1.8*inhibitor_concentration + 4.5*cofactor_concentration - 0.3*cofactor_concentration**2 + 2.1*ionic_strength + 0.15*reaction_time",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}