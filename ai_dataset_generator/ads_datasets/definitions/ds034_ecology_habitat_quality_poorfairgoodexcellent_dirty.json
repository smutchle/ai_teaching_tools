{
  "dataset_config": {
    "name": "ds034_ecology_habitat_quality_poorfairgoodexcellent_dirty",
    "description": "Classify ecosystem condition based on species richness, vegetation health, disturbance level, and connectivity",
    "random_seed": 34,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "species_richness",
          "vegetation_health_index"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "vegetation_health_index",
          "connectivity_index"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "disturbance_level",
          "species_richness"
        ],
        "correlation": -0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "connectivity_index",
          "species_richness"
        ],
        "correlation": 0.47,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "species_richness",
        "description": "Number of different species observed in the habitat",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 35,
          "location": 5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "vegetation_health_index",
        "description": "Composite index of vegetation condition (0-100 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 68.5,
          "std": 18.2,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "disturbance_level",
        "description": "Level of human and natural disturbance (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 3.2,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "connectivity_index",
        "description": "Habitat connectivity to other natural areas (0-1 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.42,
          "std": 0.22,
          "min_clip": 0,
          "max_clip": 1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "water_quality_score",
        "description": "Water quality assessment score (1-5 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 3.1,
          "std": 0.9,
          "min_clip": 1,
          "max_clip": 5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "soil_quality_index",
        "description": "Soil health and nutrient content index (0-100 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 62.8,
          "std": 16.5,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "habitat_quality_poorfairgoodexcellent",
      "description": "Overall habitat quality classification",
      "data_type": "categorical",
      "expression": "2.5*species_richness + 0.8*vegetation_health_index - 8.5*disturbance_level + 45*connectivity_index + 12*water_quality_score + 0.6*soil_quality_index",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "Poor",
        "Poor",
        "Poor",
        "Fair",
        "Fair",
        "Good",
        "Good",
        "Good",
        "Excellent",
        "Excellent"
      ]
    }
  }
}