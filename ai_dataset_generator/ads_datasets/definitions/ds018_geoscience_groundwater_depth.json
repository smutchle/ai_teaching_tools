{
  "dataset_config": {
    "name": "ds018_geoscience_groundwater_depth",
    "description": "Estimate depth to water table in meters based on elevation, precipitation, aquifer type, extraction rate, and soil porosity",
    "random_seed": 18,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "elevation",
          "precipitation"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "soil_porosity",
          "hydraulic_conductivity"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "extraction_rate",
          "well_density"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "precipitation",
          "recharge_rate"
        ],
        "correlation": 0.68,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "elevation",
        "description": "Ground surface elevation in meters above sea level",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 450,
          "std": 200,
          "min_clip": 0,
          "max_clip": 2000
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "precipitation",
        "description": "Annual precipitation in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 800,
          "std": 300,
          "min_clip": 200,
          "max_clip": 2000
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "aquifer_permeability",
        "description": "Aquifer permeability index (0-100 scale)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 45,
          "location": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "extraction_rate",
        "description": "Groundwater extraction rate in cubic meters per day",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 150,
          "location": 20
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "soil_porosity",
        "description": "Soil porosity as percentage of void space",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 35,
          "std": 12,
          "min_clip": 10,
          "max_clip": 65
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "hydraulic_conductivity",
        "description": "Hydraulic conductivity in meters per day",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 8,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "well_density",
        "description": "Number of wells per square kilometer",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.2,
          "scale": 3.5,
          "location": 0.1
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "recharge_rate",
        "description": "Annual groundwater recharge rate in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 120,
          "std": 45,
          "min_clip": 10,
          "max_clip": 400
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "groundwater_depth",
      "description": "Depth to water table in meters below ground surface",
      "data_type": "float",
      "expression": "15 + 0.08*elevation - 0.015*precipitation + 0.12*extraction_rate - 0.25*soil_porosity - 1.5*hydraulic_conductivity + 2.5*well_density - 0.05*recharge_rate + 0.15*aquifer_permeability",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}