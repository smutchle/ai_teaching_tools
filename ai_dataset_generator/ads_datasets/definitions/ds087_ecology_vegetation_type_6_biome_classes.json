{
  "dataset_config": {
    "name": "ds087_ecology_vegetation_type_6_biome_classes",
    "description": "Classify dominant plant community into 6 biome classes based on environmental factors",
    "random_seed": 87,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "mean_annual_temp",
          "growing_season_temp"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "annual_precipitation",
          "summer_precipitation"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "elevation",
          "mean_annual_temp"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "soil_ph",
          "soil_organic_matter"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "mean_annual_temp",
        "description": "Mean annual temperature in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 12.5,
          "std": 8.5,
          "min_clip": -15.0,
          "max_clip": 35.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "growing_season_temp",
        "description": "Mean growing season temperature in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 18.0,
          "std": 9.0,
          "min_clip": -5.0,
          "max_clip": 40.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "annual_precipitation",
        "description": "Annual precipitation in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 850.0,
          "location": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "summer_precipitation",
        "description": "Summer precipitation in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.2,
          "scale": 280.0,
          "location": 50.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "elevation",
        "description": "Elevation above sea level in meters",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 1200.0,
          "location": 50.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "soil_ph",
        "description": "Soil pH level",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.2,
          "std": 1.1,
          "min_clip": 3.5,
          "max_clip": 8.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "soil_organic_matter",
        "description": "Soil organic matter percentage",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 4.5,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "disturbance_frequency",
        "description": "Natural disturbance frequency events per decade",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.3,
          "scale": 2.8,
          "location": 0.1
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "vegetation_type_6_biome_classes",
      "description": "Dominant plant community biome classification",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(0.15*mean_annual_temp + 0.008*annual_precipitation - 0.0003*elevation + 0.8*soil_ph - 0.12*disturbance_frequency + 0.1*growing_season_temp - 0.005*summer_precipitation + 0.2*soil_organic_matter - 8.5)))",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0,
      "categories": [
        "Tundra",
        "Boreal Forest",
        "Boreal Forest",
        "Temperate Forest",
        "Temperate Forest",
        "Temperate Grassland",
        "Temperate Grassland",
        "Desert Scrub",
        "Tropical Forest",
        "Tropical Grassland"
      ]
    }
  }
}