{
  "dataset_config": {
    "name": "ds008_neuroscience_memory_recall_accuracy",
    "description": "Predict percentage of correctly recalled items based on study conditions and individual factors",
    "random_seed": 8,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "study_duration_minutes",
          "cognitive_load_score"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "sleep_quality_score",
          "stress_level"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "age_years",
          "baseline_attention"
        ],
        "correlation": -0.3,
        "method": "cholesky"
      },
      {
        "variables": [
          "interference_level",
          "cognitive_load_score"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "study_duration_minutes",
        "description": "Duration of study session in minutes",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 45,
          "std": 15,
          "min_clip": 10,
          "max_clip": 120
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "interference_level",
        "description": "Level of interference during memory encoding (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "sleep_quality_score",
        "description": "Sleep quality score from previous night (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.5,
          "std": 1.8,
          "min_clip": 1,
          "max_clip": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "stress_level",
        "description": "Perceived stress level during testing (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 4.5,
          "location": 1.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "age_years",
        "description": "Participant age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 35,
          "std": 12,
          "min_clip": 18,
          "max_clip": 75
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "cognitive_load_score",
        "description": "Cognitive load during encoding phase (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.5,
          "std": 2.0,
          "min_clip": 1,
          "max_clip": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "baseline_attention",
        "description": "Baseline attention capacity score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 75,
          "std": 12,
          "min_clip": 30,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "memory_recall_accuracy",
      "description": "Percentage of correctly recalled items",
      "data_type": "float",
      "expression": "65 + 0.35*study_duration_minutes - 2.8*interference_level + 2.5*sleep_quality_score - 1.8*stress_level - 0.15*age_years - 1.2*cognitive_load_score + 0.25*baseline_attention",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}