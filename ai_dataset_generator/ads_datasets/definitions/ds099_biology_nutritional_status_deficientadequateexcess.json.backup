{
  "dataset_config": {
    "name": "ds099_biology_nutritional_status_deficientadequateexcess",
    "description": "Categorize micronutrient levels based on diet quality, biomarkers, anthropometry, and absorption capacity",
    "random_seed": 99,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "diet_quality_score",
          "serum_vitamin_d"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "diet_quality_score",
          "hemoglobin_level"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "bmi",
          "serum_folate"
        ],
        "correlation": -0.32,
        "method": "cholesky"
      },
      {
        "variables": [
          "absorption_efficiency",
          "hemoglobin_level"
        ],
        "correlation": 0.42,
        "method": "cholesky"
      },
      {
        "variables": [
          "serum_vitamin_d",
          "serum_folate"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "diet_quality_score",
        "description": "Composite diet quality score (0-100 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 15,
          "min_clip": 20,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "serum_vitamin_d",
        "description": "Serum 25-hydroxyvitamin D level (ng/mL)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 28,
          "location": 8
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "hemoglobin_level",
        "description": "Hemoglobin concentration (g/dL)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 13.2,
          "std": 1.8,
          "min_clip": 8.0,
          "max_clip": 18.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "serum_folate",
        "description": "Serum folate concentration (ng/mL)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 12,
          "location": 2
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "bmi",
        "description": "Body Mass Index (kg/m\u00b2)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 24.8,
          "std": 4.2,
          "min_clip": 16.0,
          "max_clip": 45.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "absorption_efficiency",
        "description": "Intestinal absorption efficiency percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 78,
          "std": 12,
          "min_clip": 35,
          "max_clip": 95
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "nutritional_status_deficientadequateexcess",
      "description": "Micronutrient nutritional status classification",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(\u22128 + 0.08*diet_quality_score + 0.12*serum_vitamin_d + 0.85*hemoglobin_level + 0.15*serum_folate - 0.06*bmi + 0.04*absorption_efficiency)))",
      "noise_percent": 2.5,
      "categories": [
        "Deficient",
        "Deficient",
        "Deficient",
        "Adequate",
        "Adequate",
        "Adequate",
        "Adequate",
        "Adequate",
        "Excess",
        "Excess"
      ],
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}