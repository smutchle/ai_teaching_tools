{
  "dataset_config": {
    "name": "ds069_geoscience_coastal_erosion_rate",
    "description": "Coastal erosion rate prediction based on wave energy, tidal range, sediment type, vegetation coverage, and sea level trends",
    "random_seed": 69,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "wave_energy_kw_per_m",
          "tidal_range_m"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "sediment_grain_size_mm",
          "vegetation_coverage_pct"
        ],
        "correlation": -0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "wave_energy_kw_per_m",
          "sea_level_rise_mm_per_year"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "beach_slope_degrees",
          "sediment_grain_size_mm"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "wave_energy_kw_per_m",
        "description": "Average wave energy in kilowatts per meter of coastline",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 15.5,
          "location": 2.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "tidal_range_m",
        "description": "Mean tidal range in meters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2.8,
          "std": 1.2,
          "min_clip": 0.5,
          "max_clip": 8.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "sediment_grain_size_mm",
        "description": "Average sediment grain size in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.35,
          "std": 0.18,
          "min_clip": 0.05,
          "max_clip": 2.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "vegetation_coverage_pct",
        "description": "Percentage of coastal vegetation coverage",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 85
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "sea_level_rise_mm_per_year",
        "description": "Local sea level rise rate in millimeters per year",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 3.2,
          "std": 1.8,
          "min_clip": -2.0,
          "max_clip": 12.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "beach_slope_degrees",
        "description": "Beach slope angle in degrees",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 8.5,
          "std": 4.2,
          "min_clip": 1.0,
          "max_clip": 25.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "storm_frequency_per_year",
        "description": "Number of major storms per year",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 2.5,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "rock_hardness_mpa",
        "description": "Coastal rock hardness in megapascals",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 15,
          "max": 180
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "coastal_erosion_rate",
      "description": "Shoreline retreat rate in meters per year",
      "data_type": "float",
      "expression": "0.8 + 0.15*wave_energy_kw_per_m + 0.25*tidal_range_m - 0.45*sediment_grain_size_mm - 0.008*vegetation_coverage_pct + 0.12*sea_level_rise_mm_per_year + 0.08*beach_slope_degrees + 0.35*storm_frequency_per_year - 0.003*rock_hardness_mpa",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}