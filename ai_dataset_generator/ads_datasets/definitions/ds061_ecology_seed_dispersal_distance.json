{
  "dataset_config": {
    "name": "ds061_ecology_seed_dispersal_distance",
    "description": "Predict mean seed dispersal distance in meters based on seed morphology, wind conditions, animal vectors, plant characteristics, and habitat features",
    "random_seed": 61,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "wind_speed",
          "habitat_openness"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "plant_height",
          "seed_mass"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "seed_wing_area",
          "wind_dispersal_potential"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "animal_vector_activity",
          "habitat_openness"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "seed_mass",
        "description": "Seed mass in milligrams",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 15.0,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "seed_wing_area",
        "description": "Wing or appendage area in square millimeters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 8.5,
          "std": 4.2,
          "min_clip": 0.1,
          "max_clip": 25.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "wind_speed",
        "description": "Average wind speed in meters per second",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 4.8,
          "location": 0.2
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "plant_height",
        "description": "Height of parent plant in meters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2.8,
          "std": 1.6,
          "min_clip": 0.1,
          "max_clip": 12.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "habitat_openness",
        "description": "Habitat openness index (0-100 scale)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 5.0,
          "max": 95.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "animal_vector_activity",
        "description": "Animal disperser activity index (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 4.2,
          "std": 2.1,
          "min_clip": 0.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "wind_dispersal_potential",
        "description": "Wind dispersal morphology score (0-1 scale)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.05,
          "max": 0.98
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "seed_terminal_velocity",
        "description": "Terminal velocity in meters per second",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 2.8,
          "location": 0.1
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "seed_dispersal_distance",
      "description": "Mean seed dispersal distance in meters",
      "data_type": "float",
      "expression": "12.5 + 3.2*plant_height + 8.5*wind_speed*wind_dispersal_potential + 1.8*seed_wing_area - 0.15*seed_mass + 0.12*habitat_openness + 2.4*animal_vector_activity - 4.2*seed_terminal_velocity + 0.8*np.sqrt(plant_height*wind_speed)",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}