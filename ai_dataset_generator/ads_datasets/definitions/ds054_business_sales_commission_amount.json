{
  "dataset_config": {
    "name": "ds054_business_sales_commission_amount",
    "description": "Calculate compensation for sales personnel based on deal characteristics",
    "random_seed": 54,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "deal_size_k",
          "negotiation_duration_days"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "deal_size_k",
          "client_company_size"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "product_mix_score",
          "deal_size_k"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "client_company_size",
          "negotiation_duration_days"
        ],
        "correlation": 0.31,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "deal_size_k",
        "description": "Deal value in thousands of dollars",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 120,
          "location": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "product_mix_score",
        "description": "Product portfolio complexity score (1-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 18,
          "min_clip": 10,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "negotiation_duration_days",
        "description": "Number of days from first contact to deal closure",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 45,
          "location": 5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "client_company_size",
        "description": "Client company employee count",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.3,
          "scale": 850,
          "location": 25
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "industry_growth_rate",
        "description": "Client industry annual growth rate percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 4.2,
          "std": 2.8,
          "min_clip": -5.0,
          "max_clip": 15.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "regional_market_factor",
        "description": "Regional market adjustment factor (0.8-1.3)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 1.05,
          "std": 0.12,
          "min_clip": 0.8,
          "max_clip": 1.3
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "sales_rep_experience_years",
        "description": "Sales representative years of experience",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.5,
          "scale": 6.8,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "competitive_pressure_score",
        "description": "Level of competitive pressure in deal (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.2,
          "std": 2.1,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "sales_commission_amount",
      "description": "Sales commission amount in dollars",
      "data_type": "float",
      "expression": "2500 + 85*deal_size_k + 15*product_mix_score + 8*negotiation_duration_days + 0.8*client_company_size + 180*industry_growth_rate + 1200*regional_market_factor + 320*sales_rep_experience_years - 95*competitive_pressure_score",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}