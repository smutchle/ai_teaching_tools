{
  "dataset_config": {
    "name": "ds056_neuroscience_neural_firing_rate_dirty",
    "description": "Predict neural firing rate (action potentials per second) based on stimulus intensity, neuron type, network connectivity, and neuromodulator levels",
    "random_seed": 56,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "stimulus_intensity",
          "neural_firing_rate_baseline"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "network_connectivity",
          "synaptic_strength"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "dopamine_level",
          "acetylcholine_level"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "membrane_potential",
          "neural_firing_rate_baseline"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "stimulus_intensity",
        "description": "Intensity of external stimulus in microvolts",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 12.5,
          "std": 4.2,
          "min_clip": 0.0,
          "max_clip": 30.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "neuron_type_index",
        "description": "Numerical index representing neuron type characteristics",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 1.0,
          "max": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "network_connectivity",
        "description": "Degree of synaptic connectivity (0-1 scale)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 0.7,
          "location": 0.1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "dopamine_level",
        "description": "Dopamine concentration in nanomolar",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 85.0,
          "std": 18.5,
          "min_clip": 10.0,
          "max_clip": 200.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "acetylcholine_level",
        "description": "Acetylcholine concentration in nanomolar",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 45.0,
          "std": 12.0,
          "min_clip": 5.0,
          "max_clip": 120.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "membrane_potential",
        "description": "Resting membrane potential in millivolts",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": -65.0,
          "std": 8.5,
          "min_clip": -85.0,
          "max_clip": -45.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "synaptic_strength",
        "description": "Average synaptic weight strength",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 2.5,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "neural_firing_rate_baseline",
        "description": "Baseline firing rate without stimulus",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 8.0,
          "std": 3.2,
          "min_clip": 0.5,
          "max_clip": 20.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "neural_firing_rate",
      "description": "Neural firing rate in action potentials per second",
      "data_type": "float",
      "expression": "neural_firing_rate_baseline + 1.8*stimulus_intensity + 2.5*neuron_type_index + 15.0*network_connectivity + 0.12*dopamine_level + 0.08*acetylcholine_level + 0.3*(membrane_potential + 65) + 3.2*synaptic_strength",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}