{
  "dataset_config": {
    "name": "ds072_biology_photosynthesis_efficiency_dirty",
    "description": "Measure carbon fixation rate per unit area",
    "random_seed": 72,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "light_wavelength",
          "photosynthetic_photon_flux"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "temperature_celsius",
          "water_stress_index"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "co2_concentration",
          "stomatal_conductance"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "leaf_age_days",
          "chlorophyll_content"
        ],
        "correlation": -0.61,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "light_wavelength",
        "description": "Light wavelength in nanometers",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 680,
          "std": 45,
          "min_clip": 400,
          "max_clip": 750
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "co2_concentration",
        "description": "CO2 concentration in ppm",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 410,
          "std": 85,
          "min_clip": 200,
          "max_clip": 1200
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "temperature_celsius",
        "description": "Ambient temperature in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 25,
          "std": 8,
          "min_clip": 5,
          "max_clip": 45
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "water_stress_index",
        "description": "Water stress index (0-1, higher = more stress)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 0.35,
          "location": 0.1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "leaf_age_days",
        "description": "Age of leaf in days",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 45,
          "location": 5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "photosynthetic_photon_flux",
        "description": "Photosynthetic photon flux density in \u03bcmol/m\u00b2/s",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 800,
          "std": 250,
          "min_clip": 50,
          "max_clip": 2000
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "stomatal_conductance",
        "description": "Stomatal conductance in mol/m\u00b2/s",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.15,
          "std": 0.08,
          "min_clip": 0.01,
          "max_clip": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "chlorophyll_content",
        "description": "Chlorophyll content in mg/g",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2.8,
          "std": 0.9,
          "min_clip": 0.5,
          "max_clip": 6.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "photosynthesis_efficiency",
      "description": "Carbon fixation rate per unit area in \u03bcmol CO2/m\u00b2/s",
      "data_type": "float",
      "expression": "5 + 0.015*photosynthetic_photon_flux + 0.08*co2_concentration + 0.45*temperature_celsius - 12*water_stress_index - 0.05*leaf_age_days + 25*stomatal_conductance + 3.2*chlorophyll_content - 0.002*light_wavelength",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}