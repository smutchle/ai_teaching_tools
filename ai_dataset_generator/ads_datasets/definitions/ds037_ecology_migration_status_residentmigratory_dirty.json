{
  "dataset_config": {
    "name": "ds037_ecology_migration_status_residentmigratory_dirty",
    "description": "Classify movement patterns of organisms based on species traits, habitat type, latitude, resource availability, and climate",
    "random_seed": 37,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "latitude",
          "temperature_range"
        ],
        "correlation": -0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "body_mass",
          "wing_span"
        ],
        "correlation": 0.72,
        "method": "cholesky"
      },
      {
        "variables": [
          "resource_availability",
          "habitat_quality"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "temperature_range",
          "seasonal_variation"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "body_mass",
        "description": "Average body mass in grams",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 85,
          "location": 5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "wing_span",
        "description": "Wing span in centimeters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 28.5,
          "std": 12.0,
          "min_clip": 5.0,
          "max_clip": 120.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "latitude",
        "description": "Absolute latitude of primary habitat",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 75
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "resource_availability",
        "description": "Resource availability index (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 18,
          "min_clip": 10,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "temperature_range",
        "description": "Annual temperature range in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 25,
          "std": 8,
          "min_clip": 2,
          "max_clip": 50
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "habitat_quality",
        "description": "Habitat quality score (0-10)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.2,
          "std": 1.8,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "seasonal_variation",
        "description": "Seasonal resource variation coefficient",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 0.45,
          "location": 0.1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "migration_status_residentmigratory",
      "description": "Migration behavior classification",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-2.5 + 0.015*body_mass + 0.08*wing_span + 0.12*latitude - 0.05*resource_availability + 0.18*temperature_range - 0.35*habitat_quality + 4.2*seasonal_variation)))",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "Resident",
        "Resident",
        "Resident",
        "Resident",
        "Short-Distance Migrant",
        "Short-Distance Migrant",
        "Long-Distance Migrant",
        "Long-Distance Migrant",
        "Long-Distance Migrant",
        "Long-Distance Migrant"
      ]
    }
  }
}