{
  "dataset_config": {
    "name": "ds086_ecology_pest_outbreak_noneminormajor",
    "description": "Predict agricultural pest severity levels based on environmental and management factors",
    "random_seed": 86,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "temperature",
          "host_plant_density"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "natural_enemy_abundance",
          "pesticide_applications"
        ],
        "correlation": -0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "temperature",
          "natural_enemy_abundance"
        ],
        "correlation": 0.3,
        "method": "cholesky"
      },
      {
        "variables": [
          "host_plant_density",
          "pesticide_history_years"
        ],
        "correlation": 0.4,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "temperature",
        "description": "Average temperature in Celsius during growing season",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 22.5,
          "std": 4.2,
          "min_clip": 8.0,
          "max_clip": 38.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "host_plant_density",
        "description": "Host plant density per square meter",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 15.0,
          "location": 2.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "natural_enemy_abundance",
        "description": "Natural enemy abundance index (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 35.0,
          "std": 12.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "pesticide_applications",
        "description": "Number of pesticide applications in current season",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 3.5,
          "location": 0.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "pesticide_history_years",
        "description": "Years of continuous pesticide use",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 20
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "humidity",
        "description": "Average relative humidity percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65.0,
          "std": 15.0,
          "min_clip": 25.0,
          "max_clip": 95.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "rainfall_mm",
        "description": "Total rainfall in millimeters during season",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 450.0,
          "location": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "crop_diversity_index",
        "description": "Crop diversity index in surrounding area (0-1)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.1,
          "max": 0.9
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "pest_outbreak_noneminormajor",
      "description": "Pest outbreak severity level",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-3.5 + 0.15*temperature + 0.08*host_plant_density - 0.05*natural_enemy_abundance + 0.25*pesticide_applications + 0.12*pesticide_history_years + 0.03*humidity + 0.002*rainfall_mm - 2.0*crop_diversity_index)))",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0,
      "categories": [
        "None",
        "None",
        "None",
        "Minor",
        "Minor",
        "Minor",
        "Minor",
        "Major",
        "Major",
        "Major"
      ]
    }
  }
}