{
  "dataset_config": {
    "name": "ds025_business_inventory_turnover_days",
    "description": "Estimate days to sell through current stock based on product category, seasonality, price point, storage costs, and demand variability",
    "random_seed": 25,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "price_point",
          "storage_costs"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "demand_variability",
          "seasonal_index"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "product_value_score",
          "price_point"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "product_value_score",
        "description": "Product category value score (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.5,
          "std": 2.0,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "seasonal_index",
        "description": "Seasonal demand multiplier",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.6,
          "max": 1.8
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "price_point",
        "description": "Product price in dollars",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 75.0,
          "location": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "storage_costs",
        "description": "Monthly storage cost per unit in dollars",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 8.5,
          "std": 3.2,
          "min_clip": 1.0,
          "max_clip": 25.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "demand_variability",
        "description": "Coefficient of variation for demand (higher = more variable)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.15,
          "max": 0.85
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "supplier_lead_time",
        "description": "Supplier lead time in days",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 14,
          "std": 6,
          "min_clip": 3,
          "max_clip": 45
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "inventory_turnover_days",
      "description": "Number of days to sell through current stock",
      "data_type": "float",
      "expression": "25 + 8*product_value_score + 15*seasonal_index + 0.3*price_point + 2.5*storage_costs + 35*demand_variability + 0.8*supplier_lead_time",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}