{
  "dataset_config": {
    "name": "ds074_biology_root_depth_dirty",
    "description": "Predict maximum rooting depth in centimeters based on soil conditions and plant characteristics",
    "random_seed": 74,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "soil_moisture",
          "soil_compaction"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "clay_content",
          "soil_compaction"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "nitrogen_content",
          "phosphorus_content"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "soil_moisture",
          "nitrogen_content"
        ],
        "correlation": 0.31,
        "method": "cholesky"
      },
      {
        "variables": [
          "plant_biomass",
          "nitrogen_content"
        ],
        "correlation": 0.43,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "soil_moisture",
        "description": "Volumetric water content as percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 25.0,
          "std": 8.0,
          "min_clip": 5.0,
          "max_clip": 50.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "clay_content",
        "description": "Percentage of clay particles in soil",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 28.0,
          "location": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "soil_compaction",
        "description": "Bulk density in grams per cubic centimeter",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 1.35,
          "std": 0.25,
          "min_clip": 0.8,
          "max_clip": 2.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "nitrogen_content",
        "description": "Total nitrogen content in mg per kg soil",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 850.0,
          "location": 200.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "phosphorus_content",
        "description": "Available phosphorus content in mg per kg soil",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 45.0,
          "std": 18.0,
          "min_clip": 5.0,
          "max_clip": 120.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "soil_ph",
        "description": "Soil pH level",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.5,
          "std": 1.2,
          "min_clip": 4.0,
          "max_clip": 9.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "plant_biomass",
        "description": "Above-ground plant biomass in grams per square meter",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.3,
          "scale": 420.0,
          "location": 80.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "organic_matter",
        "description": "Soil organic matter content as percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 3.8,
          "std": 1.5,
          "min_clip": 0.5,
          "max_clip": 8.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "root_depth",
      "description": "Maximum rooting depth in centimeters",
      "data_type": "float",
      "expression": "45 + 2.8*soil_moisture - 35*soil_compaction - 0.8*clay_content + 0.015*nitrogen_content + 0.25*phosphorus_content + 1.2*soil_ph + 0.08*plant_biomass + 4.5*organic_matter",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}