{
  "dataset_config": {
    "name": "ds066_sociology_voter_turnout_percentage",
    "description": "Predict electoral participation rate based on competitiveness, weather, registration barriers, civic education, and age demographics",
    "random_seed": 66,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "election_competitiveness",
          "campaign_spending"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "civic_education_score",
          "median_age"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "registration_barriers_index",
          "population_density"
        ],
        "correlation": -0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "weather_quality_index",
          "voter_turnout_percentage"
        ],
        "correlation": 0.25,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "election_competitiveness",
        "description": "Competitiveness score of election (0-100, higher = more competitive)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 50.0,
          "std": 20.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "weather_quality_index",
        "description": "Weather quality on election day (0-100, higher = better weather)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65.0,
          "std": 25.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "registration_barriers_index",
        "description": "Difficulty of voter registration process (0-100, higher = more barriers)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 35.0,
          "location": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "civic_education_score",
        "description": "Quality of civic education in district (0-100, higher = better education)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 55.0,
          "std": 18.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "median_age",
        "description": "Median age of eligible voters in district",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 42.0,
          "std": 8.5,
          "min_clip": 25.0,
          "max_clip": 70.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "youth_population_percent",
        "description": "Percentage of voters aged 18-29",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 15.0,
          "max": 35.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "campaign_spending",
        "description": "Total campaign spending per capita in district (dollars)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 25.0,
          "location": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "population_density",
        "description": "Population density per square mile (thousands)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.2,
          "scale": 2.5,
          "location": 0.1
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "voter_turnout_percentage",
      "description": "Electoral participation rate as percentage of eligible voters",
      "data_type": "float",
      "expression": "35.0 + 0.25*election_competitiveness + 0.15*weather_quality_index - 0.20*registration_barriers_index + 0.30*civic_education_score + 0.40*median_age - 0.25*youth_population_percent + 0.10*campaign_spending + 0.05*population_density",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}