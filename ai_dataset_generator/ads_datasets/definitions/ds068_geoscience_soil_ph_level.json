{
  "dataset_config": {
    "name": "ds068_geoscience_soil_ph_level",
    "description": "Predict soil acidity-alkalinity on continuous scale based on parent material, rainfall, vegetation type, land use, and time",
    "random_seed": 68,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "annual_rainfall",
          "vegetation_density"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "limestone_content",
          "calcium_concentration"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "organic_matter_percent",
          "vegetation_density"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "agricultural_intensity",
          "fertilizer_application"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "limestone_content",
        "description": "Limestone content in parent material (percent)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 25.0,
          "location": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "calcium_concentration",
        "description": "Calcium concentration in soil (mg/kg)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 1200,
          "std": 400,
          "min_clip": 200,
          "max_clip": 3000
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "annual_rainfall",
        "description": "Annual rainfall amount (mm)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 800,
          "std": 300,
          "min_clip": 150,
          "max_clip": 2000
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "vegetation_density",
        "description": "Vegetation density index (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 60,
          "std": 20,
          "min_clip": 10,
          "max_clip": 95
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "organic_matter_percent",
        "description": "Organic matter content (percent)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.2,
          "scale": 4.5,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "agricultural_intensity",
        "description": "Agricultural land use intensity (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "fertilizer_application",
        "description": "Annual fertilizer application rate (kg/ha)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 150,
          "std": 80,
          "min_clip": 0,
          "max_clip": 500
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "years_since_disturbance",
        "description": "Years since last major soil disturbance",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 15,
          "location": 1
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "soil_ph_level",
      "description": "Soil pH level (acidity-alkalinity scale)",
      "data_type": "float",
      "expression": "5.8 + 0.025*limestone_content + 0.0008*calcium_concentration - 0.0003*annual_rainfall + 0.008*vegetation_density + 0.15*organic_matter_percent - 0.08*agricultural_intensity - 0.002*fertilizer_application + 0.012*years_since_disturbance",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}