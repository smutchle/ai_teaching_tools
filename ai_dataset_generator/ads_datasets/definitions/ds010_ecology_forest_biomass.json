{
  "dataset_config": {
    "name": "ds010_ecology_forest_biomass",
    "description": "Predict total organic matter per hectare in forest ecosystems based on ecological factors",
    "random_seed": 10,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "tree_density",
          "species_richness"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "rainfall",
          "soil_nitrogen"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "temperature",
          "soil_phosphorus"
        ],
        "correlation": 0.42,
        "method": "cholesky"
      },
      {
        "variables": [
          "species_richness",
          "soil_organic_carbon"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "tree_density",
        "description": "Number of trees per hectare",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 450,
          "std": 120,
          "min_clip": 100,
          "max_clip": 800
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "species_richness",
        "description": "Number of distinct tree species per hectare",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 2.5,
          "scale": 15,
          "location": 5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "soil_nitrogen",
        "description": "Soil nitrogen content in mg/kg",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 850,
          "std": 200,
          "min_clip": 300,
          "max_clip": 1500
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "soil_phosphorus",
        "description": "Soil phosphorus content in mg/kg",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 25,
          "std": 8,
          "min_clip": 5,
          "max_clip": 60
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "soil_organic_carbon",
        "description": "Soil organic carbon percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 3.5,
          "std": 1.2,
          "min_clip": 0.5,
          "max_clip": 8.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "rainfall",
        "description": "Annual rainfall in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 1200,
          "std": 350,
          "min_clip": 400,
          "max_clip": 2500
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "temperature",
        "description": "Mean annual temperature in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 18.5,
          "std": 6.2,
          "min_clip": 2.0,
          "max_clip": 35.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "elevation",
        "description": "Elevation above sea level in meters",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 50,
          "max": 2200
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "forest_biomass",
      "description": "Total organic matter per hectare in tonnes",
      "data_type": "float",
      "expression": "50 + 0.35*tree_density + 4.2*species_richness + 0.08*soil_nitrogen + 1.5*soil_phosphorus + 12*soil_organic_carbon + 0.02*rainfall + 2.1*temperature - 0.01*elevation",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}