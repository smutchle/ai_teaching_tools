{
  "dataset_config": {
    "name": "ds068_geoscience_soil_ph_level_dirty",
    "description": "Predict soil pH level (acidity-alkalinity) based on parent material, rainfall, vegetation type, land use, and time factors",
    "random_seed": 68,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "annual_rainfall_mm",
          "vegetation_density_index"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "parent_material_ph",
          "soil_ph_level"
        ],
        "correlation": 0.72,
        "method": "cholesky"
      },
      {
        "variables": [
          "organic_matter_percent",
          "vegetation_density_index"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "land_use_intensity",
          "organic_matter_percent"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "parent_material_ph",
        "description": "pH of parent geological material",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.2,
          "std": 1.8,
          "min_clip": 3.5,
          "max_clip": 10.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "annual_rainfall_mm",
        "description": "Annual precipitation in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 850,
          "location": 200
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "vegetation_density_index",
        "description": "Normalized vegetation density index (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 20,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "land_use_intensity",
        "description": "Agricultural/urban land use intensity score (0-10)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 10
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "organic_matter_percent",
        "description": "Percentage of organic matter in soil",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 4.2,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "soil_age_years",
        "description": "Age of soil formation in thousands of years",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 15.5,
          "std": 8.2,
          "min_clip": 0.1,
          "max_clip": 50.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "drainage_capacity",
        "description": "Soil drainage capacity index (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 55,
          "std": 18,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "temperature_variation",
        "description": "Annual temperature variation in degrees Celsius",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 8,
          "max": 35
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "soil_ph_level",
      "description": "Soil pH level (acidity-alkalinity scale)",
      "data_type": "float",
      "expression": "4.5 + 0.35*parent_material_ph - 0.0008*annual_rainfall_mm + 0.015*vegetation_density_index - 0.12*land_use_intensity + 0.18*organic_matter_percent + 0.02*soil_age_years + 0.008*drainage_capacity - 0.025*temperature_variation",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}