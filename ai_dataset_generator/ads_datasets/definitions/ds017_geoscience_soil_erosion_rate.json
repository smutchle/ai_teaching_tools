{
  "dataset_config": {
    "name": "ds017_geoscience_soil_erosion_rate",
    "description": "Predict soil loss in tons per hectare per year based on slope, vegetation cover, rainfall intensity, soil type, and land use factors",
    "random_seed": 17,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "slope_degrees",
          "vegetation_cover_percent"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "rainfall_intensity_mm_hr",
          "vegetation_cover_percent"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "soil_clay_content_percent",
          "water_infiltration_rate"
        ],
        "correlation": -0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "slope_degrees",
          "runoff_coefficient"
        ],
        "correlation": 0.68,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "slope_degrees",
        "description": "Terrain slope angle in degrees",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 12.0,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "vegetation_cover_percent",
        "description": "Percentage of ground covered by vegetation",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65.0,
          "std": 25.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "rainfall_intensity_mm_hr",
        "description": "Average rainfall intensity in millimeters per hour",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.2,
          "scale": 8.5,
          "location": 0.1
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "soil_clay_content_percent",
        "description": "Clay content percentage in soil composition",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 28.0,
          "std": 12.0,
          "min_clip": 5.0,
          "max_clip": 60.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "water_infiltration_rate",
        "description": "Soil water infiltration rate in mm/hr",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 15.0,
          "location": 2.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "runoff_coefficient",
        "description": "Surface runoff coefficient (0-1 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.35,
          "std": 0.18,
          "min_clip": 0.05,
          "max_clip": 0.85
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "land_use_intensity",
        "description": "Land use intensity factor (1=natural, 5=intensive agriculture)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 1.0,
          "max": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "soil_organic_matter_percent",
        "description": "Organic matter content percentage in soil",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 3.5,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "soil_erosion_rate",
      "description": "Soil loss in tons per hectare per year",
      "data_type": "float",
      "expression": "2.5 + 0.8*slope_degrees + 0.15*rainfall_intensity_mm_hr + 1.2*runoff_coefficient + 0.4*land_use_intensity - 0.08*vegetation_cover_percent - 0.05*water_infiltration_rate - 0.02*soil_clay_content_percent - 0.3*soil_organic_matter_percent",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}