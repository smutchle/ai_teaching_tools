{
  "dataset_config": {
    "name": "ds041_sociology_marriage_stability_stableat_riskdissolved_dirty",
    "description": "Predict relationship outcome categories based on marriage duration, demographics, and relationship factors",
    "random_seed": 41,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "age_at_marriage",
          "income_thousands"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "marriage_duration_years",
          "number_of_children"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "income_thousands",
          "number_of_children"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "conflict_frequency_monthly",
          "communication_score"
        ],
        "correlation": -0.65,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "marriage_duration_years",
        "description": "Number of years married",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 12.0,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "age_at_marriage",
        "description": "Age when first married",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 27.5,
          "std": 5.2,
          "min_clip": 18,
          "max_clip": 45
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "income_thousands",
        "description": "Combined household income in thousands",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 65.0,
          "location": 25.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "number_of_children",
        "description": "Number of children in the household",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 2.2,
          "location": 0.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "conflict_frequency_monthly",
        "description": "Number of significant conflicts per month",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.3,
          "scale": 4.5,
          "location": 0.2
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "communication_score",
        "description": "Communication quality score (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.8,
          "std": 1.8,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "financial_stress_score",
        "description": "Financial stress level (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 4.2,
          "std": 2.1,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "relationship_satisfaction",
        "description": "Overall relationship satisfaction (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.1,
          "std": 1.9,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "marriage_stability_stableat_riskdissolved",
      "description": "Marriage stability outcome category",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(2.5 + 0.08*marriage_duration_years + 0.05*age_at_marriage + 0.008*income_thousands - 0.15*number_of_children - 0.45*conflict_frequency_monthly + 0.35*communication_score - 0.25*financial_stress_score + 0.55*relationship_satisfaction)))",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "Dissolved",
        "Dissolved",
        "At-risk",
        "At-risk",
        "At-risk",
        "Stable",
        "Stable",
        "Stable",
        "Stable",
        "Stable"
      ]
    }
  }
}