{
  "dataset_config": {
    "name": "ds096_biology_pollination_syndrome_windinsectbirdbat",
    "description": "Classify plant reproductive strategy based on flower morphology, color, scent, nectar, and blooming time",
    "random_seed": 96,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "flower_diameter_mm",
          "nectar_volume_ul"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "scent_intensity",
          "nectar_sugar_concentration"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "petal_length_mm",
          "flower_diameter_mm"
        ],
        "correlation": 0.71,
        "method": "cholesky"
      },
      {
        "variables": [
          "color_brightness",
          "scent_intensity"
        ],
        "correlation": -0.43,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "flower_diameter_mm",
        "description": "Diameter of flower in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 15.0,
          "location": 2.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "petal_length_mm",
        "description": "Length of petals in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 12.5,
          "std": 4.2,
          "min_clip": 1.0,
          "max_clip": 35.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "color_brightness",
        "description": "Flower color brightness index (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 5.0,
          "max": 95.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "scent_intensity",
        "description": "Scent intensity score (0-10)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 4.8,
          "std": 2.1,
          "min_clip": 0.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "nectar_volume_ul",
        "description": "Nectar volume in microliters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 25.0,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "nectar_sugar_concentration",
        "description": "Nectar sugar concentration percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 32.0,
          "std": 8.5,
          "min_clip": 10.0,
          "max_clip": 65.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "bloom_time_hours",
        "description": "Hours of day when flower blooms (0-24)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.0,
          "max": 24.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "stamen_length_mm",
        "description": "Length of stamen in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 8.2,
          "std": 3.1,
          "min_clip": 1.0,
          "max_clip": 25.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "pollination_syndrome_windinsectbirdbat",
      "description": "Plant pollination syndrome classification",
      "data_type": "categorical",
      "expression": "2.1*flower_diameter_mm + 1.8*petal_length_mm - 0.02*color_brightness + 3.2*scent_intensity + 0.15*nectar_volume_ul + 0.08*nectar_sugar_concentration - 0.3*bloom_time_hours + 1.5*stamen_length_mm",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0,
      "categories": [
        "Wind",
        "Wind",
        "Wind",
        "Insect",
        "Insect",
        "Insect",
        "Insect",
        "Bird",
        "Bird",
        "Bat"
      ]
    }
  }
}