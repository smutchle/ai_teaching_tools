{
  "dataset_config": {
    "name": "ds051_business_website_conversion_rate",
    "description": "Predict percentage of visitors who complete desired action based on website performance metrics",
    "random_seed": 51,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "page_load_time_seconds",
          "mobile_page_speed_score"
        ],
        "correlation": -0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "mobile_responsiveness_score",
          "mobile_page_speed_score"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "cta_clarity_score",
          "trust_signals_count"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "ssl_certificate_present",
          "trust_signals_count"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "page_load_time_seconds",
        "description": "Average page load time in seconds",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.5,
          "scale": 3.2,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "mobile_responsiveness_score",
        "description": "Mobile responsiveness score from 0-100",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 75,
          "std": 15,
          "min_clip": 20,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "mobile_page_speed_score",
        "description": "Mobile page speed score from 0-100",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 18,
          "min_clip": 10,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "cta_clarity_score",
        "description": "Call-to-action clarity rating from 1-10",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.5,
          "std": 1.8,
          "min_clip": 1,
          "max_clip": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "trust_signals_count",
        "description": "Number of trust signals present on page",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 4.2,
          "std": 2.1,
          "min_clip": 0,
          "max_clip": 12
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "ssl_certificate_present",
        "description": "SSL certificate present (1) or not (0)",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 1
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "social_proof_elements",
        "description": "Number of social proof elements (reviews, testimonials, etc.)",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 3.5,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "checkout_steps_count",
        "description": "Number of steps in checkout process",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 1,
          "max": 8
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "website_conversion_rate",
      "description": "Percentage of visitors who complete desired action",
      "data_type": "float",
      "expression": "15.5 - 2.8*page_load_time_seconds + 0.12*mobile_responsiveness_score + 0.08*mobile_page_speed_score + 1.2*cta_clarity_score + 0.65*trust_signals_count + 3.2*ssl_certificate_present + 0.45*social_proof_elements - 0.8*checkout_steps_count",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}