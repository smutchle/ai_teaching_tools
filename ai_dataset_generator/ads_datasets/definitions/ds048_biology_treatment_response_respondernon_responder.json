{
  "dataset_config": {
    "name": "ds048_biology_treatment_response_respondernon_responder",
    "description": "Predict therapeutic efficacy based on genetic profile, disease severity, age, comorbidities, and biomarkers",
    "random_seed": 48,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "age",
          "comorbidity_count"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "disease_severity_score",
          "inflammatory_biomarker"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "genetic_risk_score",
          "protein_biomarker_level"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "age",
        "description": "Patient age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 58,
          "std": 12,
          "min_clip": 18,
          "max_clip": 85
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "genetic_risk_score",
        "description": "Polygenic risk score from genetic profile analysis",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.5,
          "std": 0.15,
          "min_clip": 0.0,
          "max_clip": 1.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "disease_severity_score",
        "description": "Clinical disease severity score (0-100 scale)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.5,
          "scale": 45,
          "location": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "comorbidity_count",
        "description": "Number of comorbid conditions",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 2.2,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "inflammatory_biomarker",
        "description": "C-reactive protein level (mg/L)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 8.5,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "protein_biomarker_level",
        "description": "Target protein expression level (normalized)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2.8,
          "std": 0.8,
          "min_clip": 0.5,
          "max_clip": 6.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "metabolic_score",
        "description": "Metabolic pathway activity score",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 1.2,
          "max": 4.8
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "treatment_response_respondernon_responder",
      "description": "Treatment response classification (Responder vs Non-responder)",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-2.5 + 1.2*genetic_risk_score - 0.08*age - 0.04*disease_severity_score - 0.3*comorbidity_count - 0.15*inflammatory_biomarker + 0.6*protein_biomarker_level + 0.4*metabolic_score)))",
      "noise_percent": 2.5,
      "categories": [
        "Non-responder",
        "Non-responder",
        "Non-responder",
        "Non-responder",
        "Non-responder",
        "Responder",
        "Responder",
        "Responder",
        "Responder",
        "Responder"
      ],
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}