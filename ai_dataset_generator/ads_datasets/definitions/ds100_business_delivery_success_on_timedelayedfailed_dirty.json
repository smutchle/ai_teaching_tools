{
  "dataset_config": {
    "name": "ds100_business_delivery_success_on_timedelayedfailed_dirty",
    "description": "Predict shipment outcome categories based on distance, traffic, weather, package size, carrier, and time window",
    "random_seed": 100,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "distance_km",
          "delivery_time_window_hours"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "traffic_density_score",
          "weather_severity_index"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "package_weight_kg",
          "package_volume_m3"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "distance_km",
        "description": "Delivery distance in kilometers",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 25,
          "location": 2
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "traffic_density_score",
        "description": "Traffic density score from 0-100",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 45,
          "std": 20,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "weather_severity_index",
        "description": "Weather severity index from 0-10",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 3.5,
          "std": 2.0,
          "min_clip": 0,
          "max_clip": 10
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "package_weight_kg",
        "description": "Package weight in kilograms",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.2,
          "scale": 8,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "package_volume_m3",
        "description": "Package volume in cubic meters",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.01,
          "max": 2.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "carrier_efficiency_rating",
        "description": "Carrier efficiency rating from 1-10",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.2,
          "std": 1.5,
          "min_clip": 1,
          "max_clip": 10
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "delivery_time_window_hours",
        "description": "Available delivery time window in hours",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 2,
          "max": 48
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "route_complexity_score",
        "description": "Route complexity score from 1-20",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 8.5,
          "std": 3.5,
          "min_clip": 1,
          "max_clip": 20
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "delivery_success_on_timedelayedfailed",
      "description": "Delivery outcome category",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(2 - 0.08*distance_km - 0.02*traffic_density_score - 0.3*weather_severity_index - 0.1*package_weight_kg - 0.2*package_volume_m3 + 0.4*carrier_efficiency_rating + 0.05*delivery_time_window_hours - 0.15*route_complexity_score)))",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "Failed",
        "Failed",
        "Delayed",
        "Delayed",
        "Delayed",
        "On-time",
        "On-time",
        "On-time",
        "On-time",
        "On-time"
      ]
    }
  }
}