{
  "dataset_config": {
    "name": "ds067_geoscience_glacier_retreat_rate_dirty",
    "description": "Estimate meters of terminus retreat per year for glaciers based on climate and topographic factors",
    "random_seed": 67,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "temperature_anomaly",
          "precipitation_mm"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "elevation_m",
          "temperature_anomaly"
        ],
        "correlation": -0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "albedo",
          "precipitation_mm"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "aspect_degrees",
          "solar_radiation"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "temperature_anomaly",
        "description": "Temperature anomaly from historical baseline in degrees Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 1.2,
          "std": 2.8,
          "min_clip": -5.0,
          "max_clip": 8.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "precipitation_mm",
        "description": "Annual precipitation in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 800.0,
          "location": 200.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "elevation_m",
        "description": "Glacier terminus elevation in meters above sea level",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2850.0,
          "std": 950.0,
          "min_clip": 500.0,
          "max_clip": 5500.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "aspect_degrees",
        "description": "Glacier aspect in degrees from north (0-360)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.0,
          "max": 360.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "albedo",
        "description": "Surface albedo (reflectance) of glacier ice",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.65,
          "std": 0.12,
          "min_clip": 0.25,
          "max_clip": 0.95
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "solar_radiation",
        "description": "Mean annual solar radiation in watts per square meter",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 185.0,
          "std": 35.0,
          "min_clip": 100.0,
          "max_clip": 280.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "slope_degrees",
        "description": "Average glacier surface slope in degrees",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 18.0,
          "location": 2.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "ice_thickness_m",
        "description": "Average ice thickness in meters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.3,
          "scale": 120.0,
          "location": 15.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "glacier_retreat_rate",
      "description": "Annual glacier terminus retreat rate in meters per year",
      "data_type": "float",
      "expression": "5.2 + 3.8*temperature_anomaly - 0.002*precipitation_mm - 0.0008*elevation_m + 0.015*solar_radiation - 8.5*albedo + 0.12*slope_degrees + 0.025*ice_thickness_m",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}