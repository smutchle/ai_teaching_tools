{
  "dataset_config": {
    "name": "ds032_neuroscience_brain_tumor_type_4_classes_dirty",
    "description": "Classification dataset for brain tumor type prediction using MRI features, patient demographics, symptoms, and genetic markers",
    "random_seed": 32,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "tumor_volume_cm3",
          "contrast_enhancement_ratio"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "age_years",
          "genetic_risk_score"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "symptom_severity_score",
          "tumor_volume_cm3"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "mri_texture_complexity",
          "contrast_enhancement_ratio"
        ],
        "correlation": 0.41,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "tumor_volume_cm3",
        "description": "Tumor volume measured from MRI in cubic centimeters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 15.0,
          "location": 2.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "contrast_enhancement_ratio",
        "description": "Ratio of contrast enhancement in MRI imaging",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 1.25,
          "std": 0.35,
          "min_clip": 0.3,
          "max_clip": 3.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "age_years",
        "description": "Patient age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 52,
          "std": 18,
          "min_clip": 18,
          "max_clip": 85
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "symptom_severity_score",
        "description": "Composite score of neurological symptoms (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 4.2,
          "std": 2.1,
          "min_clip": 0,
          "max_clip": 10
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "genetic_risk_score",
        "description": "Genetic predisposition score based on molecular markers",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.1,
          "max": 0.95
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "mri_texture_complexity",
        "description": "Texture analysis complexity measure from MRI scans",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.2,
          "scale": 45.0,
          "location": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "edema_extent_mm",
        "description": "Extent of peritumoral edema in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 12.5,
          "std": 6.8,
          "min_clip": 0,
          "max_clip": 40
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "brain_tumor_type_4_classes",
      "description": "Brain tumor classification into 4 main types",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-2.5 + 0.15*tumor_volume_cm3 + 0.8*contrast_enhancement_ratio + 0.02*age_years + 0.4*symptom_severity_score + 2.2*genetic_risk_score + 0.01*mri_texture_complexity + 0.05*edema_extent_mm)))",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "Benign Meningioma",
        "Benign Meningioma",
        "Benign Meningioma",
        "Low Grade Glioma",
        "Low Grade Glioma",
        "High Grade Glioma",
        "High Grade Glioma",
        "High Grade Glioma",
        "Metastatic Tumor",
        "Metastatic Tumor"
      ]
    }
  }
}