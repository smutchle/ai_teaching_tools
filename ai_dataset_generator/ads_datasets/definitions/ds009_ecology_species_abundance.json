{
  "dataset_config": {
    "name": "ds009_ecology_species_abundance",
    "description": "Estimate population count of a species in a given area based on environmental factors",
    "random_seed": 9,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "temperature",
          "precipitation"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "elevation",
          "temperature"
        ],
        "correlation": -0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "habitat_area",
          "species_abundance_index"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "predator_density",
          "prey_availability"
        ],
        "correlation": -0.41,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "temperature",
        "description": "Average temperature in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 18.5,
          "std": 8.2,
          "min_clip": -5.0,
          "max_clip": 35.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "precipitation",
        "description": "Annual precipitation in mm",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 850,
          "location": 200
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "elevation",
        "description": "Elevation above sea level in meters",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 1200,
          "location": 50
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "habitat_area",
        "description": "Available habitat area in square kilometers",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 25.0,
          "location": 2.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "predator_density",
        "description": "Predator density per square kilometer",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.3,
          "scale": 3.5,
          "location": 0.1
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "species_abundance_index",
        "description": "Historical species abundance index",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.65,
          "std": 0.18,
          "min_clip": 0.1,
          "max_clip": 1.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "prey_availability",
        "description": "Prey species availability index",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.72,
          "std": 0.15,
          "min_clip": 0.2,
          "max_clip": 1.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "vegetation_density",
        "description": "Vegetation cover density percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 68.5,
          "std": 22.3,
          "min_clip": 5.0,
          "max_clip": 95.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "species_abundance",
      "description": "Population count of species in the area",
      "data_type": "float",
      "expression": "np.exp(2.8 + 0.12*temperature + 0.002*precipitation - 0.0008*elevation + 0.85*np.log(habitat_area) - 1.2*predator_density + 1.8*species_abundance_index + 0.9*prey_availability + 0.015*vegetation_density)",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}