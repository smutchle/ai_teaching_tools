{
  "dataset_config": {
    "name": "ds073_biology_antibody_titer_dirty",
    "description": "Quantify immune response concentration in serum based on vaccination parameters",
    "random_seed": 73,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "age",
          "baseline_immune_score"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "booster_doses",
          "antibody_baseline"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "days_post_vaccination",
          "antibody_decay_rate"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "adjuvant_potency",
          "immune_activation_score"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "days_post_vaccination",
        "description": "Number of days since vaccination",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 1,
          "max": 365
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "age",
        "description": "Patient age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 45,
          "std": 18,
          "min_clip": 18,
          "max_clip": 85
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "baseline_immune_score",
        "description": "Baseline immune system strength score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 15,
          "min_clip": 20,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "booster_doses",
        "description": "Number of booster doses received",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 4
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "adjuvant_potency",
        "description": "Adjuvant strength factor (1.0-3.5)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 2.2,
          "location": 1.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "antibody_baseline",
        "description": "Pre-vaccination antibody level (IU/mL)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 25,
          "std": 8,
          "min_clip": 5,
          "max_clip": 60
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "immune_activation_score",
        "description": "Post-vaccination immune activation level (0-10)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.5,
          "std": 1.8,
          "min_clip": 1.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "antibody_decay_rate",
        "description": "Rate of antibody decay over time (0.001-0.01)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.001,
          "max": 0.01
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "antibody_titer",
      "description": "Antibody concentration in serum (IU/mL)",
      "data_type": "float",
      "expression": "antibody_baseline + 150*adjuvant_potency + 25*booster_doses + 8*immune_activation_score + 0.6*baseline_immune_score - 0.15*age - 200*antibody_decay_rate*np.log(days_post_vaccination + 1)",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}