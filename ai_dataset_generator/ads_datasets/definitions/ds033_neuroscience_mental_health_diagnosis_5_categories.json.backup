{
  "dataset_config": {
    "name": "ds033_neuroscience_mental_health_diagnosis_5_categories",
    "description": "Categorize psychiatric conditions based on symptom checklist, sleep patterns, social function, and trauma history",
    "random_seed": 33,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "anxiety_symptoms",
          "depression_symptoms"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "sleep_quality_index",
          "depression_symptoms"
        ],
        "correlation": -0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "social_functioning_score",
          "anxiety_symptoms"
        ],
        "correlation": -0.48,
        "method": "cholesky"
      },
      {
        "variables": [
          "trauma_severity_score",
          "anxiety_symptoms"
        ],
        "correlation": 0.43,
        "method": "cholesky"
      },
      {
        "variables": [
          "cognitive_symptoms",
          "depression_symptoms"
        ],
        "correlation": 0.56,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "anxiety_symptoms",
        "description": "Anxiety symptom severity score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 35,
          "std": 18,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "depression_symptoms",
        "description": "Depression symptom severity score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 32,
          "std": 20,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "sleep_quality_index",
        "description": "Sleep quality index (0-10, higher is better)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.2,
          "std": 2.1,
          "min_clip": 0,
          "max_clip": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "social_functioning_score",
        "description": "Social functioning assessment score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 72,
          "std": 16,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "trauma_severity_score",
        "description": "Trauma history severity score (0-50)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 15,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "cognitive_symptoms",
        "description": "Cognitive impairment symptoms score (0-80)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 28,
          "std": 14,
          "min_clip": 0,
          "max_clip": 80
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "psychotic_symptoms",
        "description": "Psychotic symptoms severity score (0-60)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.2,
          "scale": 8,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "substance_use_score",
        "description": "Substance use disorder symptoms score (0-40)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 12,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "mental_health_diagnosis_5_categories",
      "description": "Mental health diagnosis classification",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(\u22123.2 + 0.08*anxiety_symptoms + 0.06*depression_symptoms - 0.25*sleep_quality_index - 0.04*social_functioning_score + 0.12*trauma_severity_score + 0.09*cognitive_symptoms + 0.15*psychotic_symptoms + 0.07*substance_use_score)))",
      "noise_percent": 2.5,
      "categories": [
        "No Disorder",
        "No Disorder",
        "Anxiety Disorder",
        "Anxiety Disorder",
        "Depressive Disorder",
        "Depressive Disorder",
        "Bipolar Disorder",
        "Psychotic Disorder",
        "Substance Use Disorder",
        "Comorbid Conditions"
      ],
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}