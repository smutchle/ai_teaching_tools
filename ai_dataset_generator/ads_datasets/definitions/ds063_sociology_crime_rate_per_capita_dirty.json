{
  "dataset_config": {
    "name": "ds063_sociology_crime_rate_per_capita_dirty",
    "description": "Predict offenses per 1000 residents annually based on socioeconomic factors",
    "random_seed": 63,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "unemployment_rate",
          "poverty_rate"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "poverty_rate",
          "education_index"
        ],
        "correlation": -0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "unemployment_rate",
          "education_index"
        ],
        "correlation": -0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "housing_density",
          "police_per_capita"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "median_income",
          "poverty_rate"
        ],
        "correlation": -0.72,
        "method": "cholesky"
      },
      {
        "variables": [
          "median_income",
          "education_index"
        ],
        "correlation": 0.63,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "unemployment_rate",
        "description": "Unemployment rate as percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 8.5,
          "std": 3.2,
          "min_clip": 1.0,
          "max_clip": 25.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "poverty_rate",
        "description": "Percentage of population below poverty line",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 15.0,
          "location": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "police_per_capita",
        "description": "Police officers per 1000 residents",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2.8,
          "std": 0.9,
          "min_clip": 0.5,
          "max_clip": 8.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "education_index",
        "description": "Education index score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 72.5,
          "std": 12.8,
          "min_clip": 30.0,
          "max_clip": 95.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "housing_density",
        "description": "Housing units per square mile",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 1200.0,
          "location": 200.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "median_income",
        "description": "Median household income in thousands",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 52.8,
          "std": 18.5,
          "min_clip": 20.0,
          "max_clip": 120.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "population_age_median",
        "description": "Median age of population",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 37.2,
          "std": 6.8,
          "min_clip": 20.0,
          "max_clip": 65.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "social_cohesion_score",
        "description": "Community social cohesion index (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65.3,
          "std": 15.2,
          "min_clip": 10.0,
          "max_clip": 95.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "crime_rate_per_capita",
      "description": "Offenses per 1000 residents annually",
      "data_type": "float",
      "expression": "15.2 + 0.85*unemployment_rate + 0.62*poverty_rate - 1.8*police_per_capita - 0.18*education_index + 0.008*housing_density - 0.12*median_income - 0.15*population_age_median - 0.08*social_cohesion_score",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}