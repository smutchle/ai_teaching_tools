{
  "dataset_config": {
    "name": "ds079_business_ad_click_yesno_dirty",
    "description": "Predict engagement with digital advertisement",
    "random_seed": 79,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "headline_relevance",
          "ad_position"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "user_age",
          "user_income"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "image_quality",
          "headline_relevance"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "ad_position",
        "description": "Position of ad on page (1=top, 10=bottom)",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 1,
          "max": 10
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "image_quality",
        "description": "Image content quality score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 15,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "headline_relevance",
        "description": "Headline relevance score (0-10)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.5,
          "scale": 6.5,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "user_age",
        "description": "User age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 35,
          "std": 12,
          "min_clip": 18,
          "max_clip": 75
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "user_income",
        "description": "User annual income in thousands",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 55,
          "location": 25
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "time_of_day",
        "description": "Hour of day when ad shown (0-23)",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 23
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "device_type",
        "description": "Device type score (1=mobile, 5=desktop)",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 1,
          "max": 5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "previous_clicks",
        "description": "User's previous ad clicks this month",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.2,
          "scale": 3.5,
          "location": 0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "ad_click_yesno",
      "description": "Whether user clicked the ad",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-3.5 + 0.8*headline_relevance + 0.02*image_quality - 0.15*ad_position + 0.01*user_income + 0.25*previous_clicks + 0.1*device_type - 0.02*np.abs(time_of_day - 14))))",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "No",
        "No",
        "No",
        "No",
        "No",
        "No",
        "Yes",
        "Yes",
        "Yes",
        "Yes"
      ]
    }
  }
}