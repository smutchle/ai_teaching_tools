{
  "dataset_config": {
    "name": "ds042_geoscience_landslide_risk_lowmoderatehigh",
    "description": "Classify terrain stability hazard levels based on geological and environmental factors",
    "random_seed": 42,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "slope_angle",
          "soil_cohesion"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "annual_rainfall",
          "soil_moisture"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "vegetation_density",
          "soil_cohesion"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "seismic_activity",
          "slope_angle"
        ],
        "correlation": 0.31,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "slope_angle",
        "description": "Terrain slope angle in degrees",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 25,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "soil_cohesion",
        "description": "Soil cohesion strength in kPa",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 45,
          "std": 15,
          "min_clip": 5,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "vegetation_density",
        "description": "Vegetation coverage percentage",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "annual_rainfall",
        "description": "Annual rainfall in millimeters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 1200,
          "std": 400,
          "min_clip": 200,
          "max_clip": 3000
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "soil_moisture",
        "description": "Soil moisture content percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 35,
          "std": 12,
          "min_clip": 5,
          "max_clip": 80
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "seismic_activity",
        "description": "Seismic activity index (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 3.2,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "drainage_quality",
        "description": "Terrain drainage quality index (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 60,
          "std": 20,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "bedrock_depth",
        "description": "Depth to bedrock in meters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 8,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "landslide_risk_lowmoderatehigh",
      "description": "Landslide risk classification",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(0.08*slope_angle - 0.05*soil_cohesion - 0.02*vegetation_density + 0.0008*annual_rainfall + 0.04*soil_moisture + 0.3*seismic_activity - 0.015*drainage_quality + 0.15*bedrock_depth - 4.5)))",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0,
      "categories": [
        "Low",
        "Low",
        "Low",
        "Low",
        "Moderate",
        "Moderate",
        "Moderate",
        "High",
        "High",
        "High"
      ]
    }
  }
}