{
  "dataset_config": {
    "name": "ds047_biology_cell_type_6_tissue_classes",
    "description": "Cellular identity and function classification based on gene expression profile, morphology, markers, and location",
    "random_seed": 47,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "gene_expression_intensity",
          "protein_marker_density"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "cell_diameter",
          "nuclear_size"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "protein_marker_density",
          "membrane_permeability"
        ],
        "correlation": 0.42,
        "method": "cholesky"
      },
      {
        "variables": [
          "tissue_depth",
          "oxygen_concentration"
        ],
        "correlation": -0.38,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "gene_expression_intensity",
        "description": "Normalized gene expression intensity level",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2.5,
          "std": 0.8,
          "min_clip": 0.1,
          "max_clip": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "cell_diameter",
        "description": "Cell diameter in micrometers",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 18.0,
          "location": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "nuclear_size",
        "description": "Nuclear size as percentage of cell volume",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 15.0,
          "std": 4.5,
          "min_clip": 3.0,
          "max_clip": 35.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "protein_marker_density",
        "description": "Surface protein marker density per square micrometer",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 450.0,
          "std": 120.0,
          "min_clip": 50.0,
          "max_clip": 900.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "membrane_permeability",
        "description": "Cell membrane permeability index",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.1,
          "max": 2.8
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "tissue_depth",
        "description": "Depth within tissue in micrometers",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 85.0,
          "location": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "oxygen_concentration",
        "description": "Local oxygen concentration in mmHg",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65.0,
          "std": 18.0,
          "min_clip": 15.0,
          "max_clip": 120.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "cell_type_6_tissue_classes",
      "description": "Cell type classification across 6 tissue classes",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(0.8*gene_expression_intensity + 0.05*cell_diameter + 0.12*nuclear_size + 0.003*protein_marker_density + 0.6*membrane_permeability - 0.01*tissue_depth + 0.02*oxygen_concentration - 4.5)))",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0,
      "categories": [
        "Epithelial",
        "Epithelial",
        "Connective",
        "Muscle",
        "Nervous",
        "Nervous",
        "Adipose",
        "Vascular",
        "Vascular",
        "Immune"
      ]
    }
  }
}