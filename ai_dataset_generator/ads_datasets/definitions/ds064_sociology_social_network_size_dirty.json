{
  "dataset_config": {
    "name": "ds064_sociology_social_network_size_dirty",
    "description": "Estimate number of meaningful social connections based on personality traits, age, occupation, urbanization, and digital literacy",
    "random_seed": 64,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "extroversion_score",
          "agreeableness_score"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "digital_literacy_score",
          "age"
        ],
        "correlation": -0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "urbanization_index",
          "digital_literacy_score"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "occupation_prestige",
          "education_years"
        ],
        "correlation": 0.68,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "extroversion_score",
        "description": "Personality trait score for extroversion (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 55,
          "std": 18,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "agreeableness_score",
        "description": "Personality trait score for agreeableness (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 62,
          "std": 15,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "age",
        "description": "Age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 42,
          "std": 16,
          "min_clip": 18,
          "max_clip": 85
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "occupation_prestige",
        "description": "Occupational prestige score (20-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 58,
          "std": 20,
          "min_clip": 20,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "urbanization_index",
        "description": "Level of urbanization in residence area (0-10)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 10
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "digital_literacy_score",
        "description": "Digital technology proficiency score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.5,
          "scale": 65,
          "location": 10
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "education_years",
        "description": "Years of formal education",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 14,
          "std": 3.5,
          "min_clip": 8,
          "max_clip": 22
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "social_network_size",
      "description": "Number of meaningful social connections",
      "data_type": "float",
      "expression": "25 + 0.45*extroversion_score + 0.25*agreeableness_score - 0.15*age + 0.12*occupation_prestige + 2.8*urbanization_index + 0.18*digital_literacy_score + 0.8*education_years",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}