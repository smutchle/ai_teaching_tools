{
  "dataset_config": {
    "name": "ds006_neuroscience_brain_region_volume_dirty",
    "description": "Estimate volumetric measurements of specific brain structures",
    "random_seed": 6,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "age",
          "cognitive_score"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "physical_activity_hours",
          "bmi"
        ],
        "correlation": -0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "apoe4_alleles",
          "cognitive_score"
        ],
        "correlation": -0.3,
        "method": "cholesky"
      },
      {
        "variables": [
          "education_years",
          "cognitive_score"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "sleep_hours",
          "cognitive_score"
        ],
        "correlation": 0.25,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "age",
        "description": "Age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 12,
          "min_clip": 18,
          "max_clip": 95
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "sex",
        "description": "Biological sex",
        "data_type": "categorical",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 1
        },
        "categories": [
          "Female",
          "Female",
          "Female",
          "Female",
          "Female",
          "Male",
          "Male",
          "Male",
          "Male",
          "Male"
        ],
        "missing_rate": 0.02
      },
      {
        "name": "apoe4_alleles",
        "description": "Number of APOE4 alleles (0-2)",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 0.8,
          "scale": 0.6,
          "location": 0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "education_years",
        "description": "Years of formal education",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 14,
          "std": 3,
          "min_clip": 6,
          "max_clip": 22
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "physical_activity_hours",
        "description": "Weekly physical activity hours",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 4.5,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "bmi",
        "description": "Body mass index",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 26.5,
          "std": 4.2,
          "min_clip": 16.0,
          "max_clip": 45.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "sleep_hours",
        "description": "Average daily sleep hours",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 7.2,
          "std": 1.1,
          "min_clip": 4.0,
          "max_clip": 11.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "cognitive_score",
        "description": "Standardized cognitive assessment score",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 100,
          "std": 15,
          "min_clip": 60,
          "max_clip": 140
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "brain_region_volume",
      "description": "Volumetric measurement of brain region in cubic millimeters",
      "data_type": "float",
      "expression": "12000 - 25*age + 150*education_years + 45*physical_activity_hours + 8*cognitive_score - 800*apoe4_alleles - 15*bmi + 85*sleep_hours",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}