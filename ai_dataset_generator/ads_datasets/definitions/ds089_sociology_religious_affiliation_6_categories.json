{
  "dataset_config": {
    "name": "ds089_sociology_religious_affiliation_6_categories",
    "description": "Classification dataset for predicting religious affiliation based on family background, geography, education, age, and social networks",
    "random_seed": 89,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "family_religiosity",
          "community_religious_density"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "education_years",
          "age"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "family_religiosity",
          "religious_social_network_size"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "community_religious_density",
          "religious_social_network_size"
        ],
        "correlation": 0.4,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "family_religiosity",
        "description": "Family religious background intensity score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 45,
          "std": 25,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "community_religious_density",
        "description": "Religious institutions density in geographic area (per 1000 residents)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 3.5,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "education_years",
        "description": "Years of formal education completed",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 13.5,
          "std": 3.2,
          "min_clip": 8,
          "max_clip": 22
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "age",
        "description": "Age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 42,
          "std": 16,
          "min_clip": 18,
          "max_clip": 85
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "religious_social_network_size",
        "description": "Number of close friends/family who are religiously active",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 4.2,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "urban_rural_index",
        "description": "Geographic urbanization index (0=rural, 100=urban)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "religious_affiliation_6_categories",
      "description": "Religious affiliation classification",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-2.5 + 0.08*family_religiosity + 0.35*community_religious_density + 0.12*religious_social_network_size - 0.15*education_years + 0.02*age - 0.025*urban_rural_index)))",
      "noise_percent": 2.5,
      "categories": [
        "Secular/Non-religious",
        "Secular/Non-religious",
        "Spiritual but not religious",
        "Mainline Protestant",
        "Evangelical Protestant",
        "Catholic",
        "Catholic",
        "Other Christian",
        "Non-Christian Religious",
        "Highly Religious"
      ],
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}