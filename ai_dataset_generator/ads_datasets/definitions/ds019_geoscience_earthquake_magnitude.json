{
  "dataset_config": {
    "name": "ds019_geoscience_earthquake_magnitude",
    "description": "Predict seismic event strength on Richter scale",
    "random_seed": 19,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "fault_displacement",
          "fault_length"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "focal_depth",
          "stress_accumulation"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "fault_length",
          "stress_accumulation"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "fault_displacement",
          "tectonic_strain"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "fault_displacement",
        "description": "Fault displacement in meters",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 2.5,
          "location": 0.1
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "focal_depth",
        "description": "Focal depth in kilometers",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 15.0,
          "std": 8.5,
          "min_clip": 1.0,
          "max_clip": 700.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "fault_length",
        "description": "Fault length in kilometers",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.2,
          "scale": 12.0,
          "location": 1.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "stress_accumulation",
        "description": "Stress accumulation in MPa",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 45.0,
          "std": 18.0,
          "min_clip": 5.0,
          "max_clip": 150.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "tectonic_strain",
        "description": "Tectonic strain rate in nanostrain per year",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 10.0,
          "max": 200.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "rock_friction",
        "description": "Rock friction coefficient",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.65,
          "std": 0.12,
          "min_clip": 0.2,
          "max_clip": 0.9
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "seismic_velocity",
        "description": "P-wave velocity in km/s",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.2,
          "std": 1.1,
          "min_clip": 3.5,
          "max_clip": 8.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "crustal_thickness",
        "description": "Crustal thickness in kilometers",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 35.0,
          "std": 8.0,
          "min_clip": 15.0,
          "max_clip": 70.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "earthquake_magnitude",
      "description": "Earthquake magnitude on Richter scale",
      "data_type": "float",
      "expression": "2.5 + 0.8*np.log(fault_displacement + 1) + 0.15*np.log(fault_length + 1) + 0.02*stress_accumulation - 0.008*focal_depth + 0.003*tectonic_strain - 1.2*rock_friction + 0.12*seismic_velocity + 0.01*crustal_thickness",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}