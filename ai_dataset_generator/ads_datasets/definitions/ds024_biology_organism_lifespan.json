{
  "dataset_config": {
    "name": "ds024_biology_organism_lifespan",
    "description": "Predict organism lifespan based on diet quality, genetic factors, stress exposure, and reproduction events",
    "random_seed": 24,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "diet_quality_score",
          "metabolic_rate"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "genetic_longevity_index",
          "telomere_length"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "stress_hormone_level",
          "immune_system_strength"
        ],
        "correlation": -0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "reproduction_frequency",
          "energy_allocation_score"
        ],
        "correlation": -0.51,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "diet_quality_score",
        "description": "Nutritional quality score from 0-100",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 18,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "genetic_longevity_index",
        "description": "Genetic predisposition for longevity (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.5,
          "std": 1.8,
          "min_clip": 0,
          "max_clip": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "stress_hormone_level",
        "description": "Average cortisol-equivalent stress hormone level (ng/mL)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 45,
          "location": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "reproduction_frequency",
        "description": "Number of reproduction events per year",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 2.5,
          "location": 0.1
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "metabolic_rate",
        "description": "Basal metabolic rate (kcal/kg/day)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 85,
          "std": 22,
          "min_clip": 30,
          "max_clip": 150
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "telomere_length",
        "description": "Average telomere length (kilobases)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 8.5,
          "std": 2.1,
          "min_clip": 3.0,
          "max_clip": 15.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "immune_system_strength",
        "description": "Immune response efficiency score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 72,
          "std": 16,
          "min_clip": 20,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "energy_allocation_score",
        "description": "Efficiency of energy allocation between maintenance and growth (0-10)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 2.0,
          "max": 9.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "organism_lifespan",
      "description": "Organism lifespan in days",
      "data_type": "float",
      "expression": "200 + 12*diet_quality_score + 85*genetic_longevity_index - 3.2*stress_hormone_level - 45*reproduction_frequency - 1.8*metabolic_rate + 35*telomere_length + 6.5*immune_system_strength + 28*energy_allocation_score",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}