{
  "dataset_config": {
    "name": "ds074_biology_root_depth",
    "description": "Predict maximum rooting depth in centimeters based on soil conditions and plant species characteristics",
    "random_seed": 74,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "soil_moisture",
          "nutrient_availability"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "soil_compaction",
          "clay_content"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "plant_biomass",
          "nutrient_availability"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "soil_moisture",
        "description": "Soil moisture content as percentage",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 25.0,
          "std": 8.0,
          "min_clip": 5.0,
          "max_clip": 45.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "clay_content",
        "description": "Percentage of clay in soil texture",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 30.0,
          "location": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "soil_compaction",
        "description": "Soil bulk density in g/cm\u00b3",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 1.35,
          "std": 0.25,
          "min_clip": 0.8,
          "max_clip": 1.8
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "plant_biomass",
        "description": "Above-ground plant biomass in kg/m\u00b2",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 2.5,
          "location": 0.2
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "nutrient_availability",
        "description": "Soil nutrient index (0-100 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 55.0,
          "std": 18.0,
          "min_clip": 10.0,
          "max_clip": 95.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "soil_ph",
        "description": "Soil pH level",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.8,
          "std": 0.8,
          "min_clip": 4.5,
          "max_clip": 8.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "temperature_avg",
        "description": "Average soil temperature in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 18.0,
          "std": 5.0,
          "min_clip": 5.0,
          "max_clip": 32.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "root_depth",
      "description": "Maximum rooting depth in centimeters",
      "data_type": "float",
      "expression": "80 + 2.5*soil_moisture - 25*soil_compaction + 0.8*nutrient_availability + 15*plant_biomass - 0.3*clay_content + 3*soil_ph + 1.2*temperature_avg",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}