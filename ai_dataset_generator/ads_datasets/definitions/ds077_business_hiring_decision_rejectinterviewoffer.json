{
  "dataset_config": {
    "name": "ds077_business_hiring_decision_rejectinterviewoffer",
    "description": "Business hiring decision classification dataset with resume keywords, experience, education, skills match, and references to categorize candidate progression stages",
    "random_seed": 77,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "experience_years",
          "education_level"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "resume_keywords_score",
          "skills_match_percentage"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "education_level",
          "skills_match_percentage"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "experience_years",
          "reference_quality_score"
        ],
        "correlation": 0.33,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "resume_keywords_score",
        "description": "Score based on relevant keywords found in resume (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 18,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "experience_years",
        "description": "Years of relevant work experience",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 8.5,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "education_level",
        "description": "Education level score (1=High School, 2=Associate, 3=Bachelor, 4=Master, 5=PhD)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2.8,
          "std": 0.9,
          "min_clip": 1,
          "max_clip": 5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "skills_match_percentage",
        "description": "Percentage match between candidate skills and job requirements",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 58,
          "std": 22,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "reference_quality_score",
        "description": "Quality score of professional references (0-10)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.2,
          "std": 1.8,
          "min_clip": 0,
          "max_clip": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "application_completeness",
        "description": "Completeness score of job application (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 82,
          "std": 15,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "hiring_decision_rejectinterviewoffer",
      "description": "Hiring decision categorizing candidate progression stage",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-8.5 + 0.08*resume_keywords_score + 0.35*experience_years + 0.9*education_level + 0.07*skills_match_percentage + 0.4*reference_quality_score + 0.02*application_completeness)))",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0,
      "categories": [
        "Reject",
        "Reject",
        "Reject",
        "Reject",
        "Interview",
        "Interview",
        "Interview",
        "Interview",
        "Offer",
        "Offer"
      ]
    }
  }
}