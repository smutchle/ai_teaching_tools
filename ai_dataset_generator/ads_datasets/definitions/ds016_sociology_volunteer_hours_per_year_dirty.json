{
  "dataset_config": {
    "name": "ds016_sociology_volunteer_hours_per_year_dirty",
    "description": "Estimate annual time devoted to unpaid community service based on demographic and social factors",
    "random_seed": 16,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "education_years",
          "annual_income"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "age",
          "community_involvement_score"
        ],
        "correlation": 0.42,
        "method": "cholesky"
      },
      {
        "variables": [
          "education_years",
          "community_involvement_score"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "annual_income",
          "work_hours_per_week"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "age",
        "description": "Age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 45,
          "std": 16,
          "min_clip": 18,
          "max_clip": 85
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "annual_income",
        "description": "Annual household income in thousands",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.5,
          "scale": 65,
          "location": 25
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "education_years",
        "description": "Years of formal education completed",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 14.5,
          "std": 3.2,
          "min_clip": 8,
          "max_clip": 22
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "work_hours_per_week",
        "description": "Hours worked per week in paid employment",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 38,
          "std": 12,
          "min_clip": 0,
          "max_clip": 70
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "community_involvement_score",
        "description": "Composite score measuring community ties and social engagement",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.5,
          "std": 2.1,
          "min_clip": 1,
          "max_clip": 10
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "employment_status",
        "description": "Current employment status category",
        "data_type": "categorical",
        "distribution": {
          "type": "normal",
          "mean": 0.5,
          "std": 0.25
        },
        "categories": [
          "Unemployed",
          "Unemployed",
          "Part-time",
          "Part-time",
          "Full-time",
          "Full-time",
          "Full-time",
          "Full-time",
          "Retired",
          "Self-employed"
        ],
        "missing_rate": 0.02
      },
      {
        "name": "household_size",
        "description": "Number of people in household",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 2.2,
          "location": 1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "years_in_community",
        "description": "Number of years living in current community",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.4,
          "scale": 12,
          "location": 1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "volunteer_hours_per_year",
      "description": "Annual hours spent in unpaid community service and volunteer work",
      "data_type": "float",
      "expression": "25 + 0.8*age - 0.5*work_hours_per_week + 2.5*education_years + 12*community_involvement_score + 0.15*annual_income + 1.2*years_in_community - 2*household_size",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}