{
  "dataset_config": {
    "name": "ds069_geoscience_coastal_erosion_rate_dirty",
    "description": "Geoscience dataset measuring coastal erosion rate based on wave energy, tidal range, sediment type, vegetation coverage, and sea level trends",
    "random_seed": 69,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "wave_energy",
          "tidal_range"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "sediment_grain_size",
          "beach_slope"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "vegetation_density",
          "root_depth"
        ],
        "correlation": 0.68,
        "method": "cholesky"
      },
      {
        "variables": [
          "sea_level_rise_rate",
          "storm_frequency"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "wave_energy",
        "description": "Average wave energy in kJ/m",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 45.0,
          "location": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "tidal_range",
        "description": "Mean tidal range in meters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2.8,
          "std": 1.2,
          "min_clip": 0.3,
          "max_clip": 8.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "sediment_grain_size",
        "description": "Average sediment grain size in mm",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.35,
          "std": 0.18,
          "min_clip": 0.05,
          "max_clip": 2.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "beach_slope",
        "description": "Beach slope angle in degrees",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 8.5,
          "std": 3.2,
          "min_clip": 1.0,
          "max_clip": 25.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "vegetation_density",
        "description": "Coastal vegetation coverage percentage",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.0,
          "max": 85.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "root_depth",
        "description": "Average vegetation root depth in cm",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 35.0,
          "location": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "sea_level_rise_rate",
        "description": "Sea level rise rate in mm/year",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 3.2,
          "std": 1.1,
          "min_clip": 0.5,
          "max_clip": 8.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "storm_frequency",
        "description": "Number of major storms per year",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 4.2,
          "location": 0.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "coastal_erosion_rate",
      "description": "Shoreline retreat rate in meters per year",
      "data_type": "float",
      "expression": "0.8 + 0.12*wave_energy + 0.35*tidal_range - 2.5*sediment_grain_size + 0.08*beach_slope - 0.015*vegetation_density - 0.02*root_depth + 0.45*sea_level_rise_rate + 0.25*storm_frequency",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}