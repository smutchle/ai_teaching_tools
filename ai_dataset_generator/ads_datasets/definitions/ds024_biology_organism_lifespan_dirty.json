{
  "dataset_config": {
    "name": "ds024_biology_organism_lifespan_dirty",
    "description": "Predict survival duration in days or years based on diet quality, genetic factors, stress exposure, and reproduction events",
    "random_seed": 24,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "diet_quality_score",
          "metabolic_rate"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "genetic_longevity_index",
          "telomere_length"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "stress_exposure_level",
          "cortisol_baseline"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "reproduction_events",
          "energy_expenditure"
        ],
        "correlation": 0.41,
        "method": "cholesky"
      },
      {
        "variables": [
          "diet_quality_score",
          "genetic_longevity_index"
        ],
        "correlation": 0.33,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "diet_quality_score",
        "description": "Nutritional quality index from 0-100",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65.0,
          "std": 18.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "genetic_longevity_index",
        "description": "Genetic predisposition to longevity score",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 50.0,
          "std": 15.0,
          "min_clip": 10.0,
          "max_clip": 90.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "stress_exposure_level",
        "description": "Environmental and psychological stress level",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 45.0,
          "location": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "reproduction_events",
        "description": "Number of reproduction cycles or offspring",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 3.2,
          "location": 0.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "metabolic_rate",
        "description": "Baseline metabolic rate index",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 75.0,
          "std": 20.0,
          "min_clip": 20.0,
          "max_clip": 150.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "telomere_length",
        "description": "Telomere length as aging biomarker",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 8.5,
          "std": 2.1,
          "min_clip": 3.0,
          "max_clip": 15.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "cortisol_baseline",
        "description": "Baseline cortisol stress hormone level",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.3,
          "scale": 12.0,
          "location": 2.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "energy_expenditure",
        "description": "Daily energy expenditure rate",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2200.0,
          "std": 450.0,
          "min_clip": 800.0,
          "max_clip": 4000.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "organism_lifespan",
      "description": "Survival duration in days",
      "data_type": "float",
      "expression": "365 + 2.8*diet_quality_score + 3.5*genetic_longevity_index + 4.2*telomere_length - 1.8*stress_exposure_level - 45*reproduction_events - 0.6*metabolic_rate - 8.5*cortisol_baseline - 0.05*energy_expenditure",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "outlier_rate": 0.01,
      "outlier_method": "extreme_both",
      "outlier_multiplier": 3.0
    }
  }
}