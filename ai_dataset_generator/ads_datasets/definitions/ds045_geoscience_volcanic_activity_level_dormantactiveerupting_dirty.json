{
  "dataset_config": {
    "name": "ds045_geoscience_volcanic_activity_level_dormantactiveerupting_dirty",
    "description": "Categorize volcano status based on seismicity, gas emissions, deformation, and thermal anomalies",
    "random_seed": 45,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "seismic_magnitude",
          "seismic_frequency"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "so2_emissions",
          "co2_emissions"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "ground_deformation",
          "seismic_magnitude"
        ],
        "correlation": 0.48,
        "method": "cholesky"
      },
      {
        "variables": [
          "thermal_anomaly_temp",
          "so2_emissions"
        ],
        "correlation": 0.42,
        "method": "cholesky"
      },
      {
        "variables": [
          "gas_plume_height",
          "so2_emissions"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "seismic_magnitude",
        "description": "Average seismic magnitude over monitoring period",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 2.5,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "seismic_frequency",
        "description": "Number of seismic events per day",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 15.0,
          "location": 1.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "so2_emissions",
        "description": "Sulfur dioxide emissions in tons per day",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.2,
          "scale": 500.0,
          "location": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "co2_emissions",
        "description": "Carbon dioxide emissions in tons per day",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.4,
          "scale": 800.0,
          "location": 50.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "ground_deformation",
        "description": "Ground deformation rate in mm per year",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.0,
          "std": 8.0,
          "min_clip": -20.0,
          "max_clip": 50.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "thermal_anomaly_temp",
        "description": "Peak thermal anomaly temperature in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 45.0,
          "std": 25.0,
          "min_clip": 10.0,
          "max_clip": 150.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "gas_plume_height",
        "description": "Maximum gas plume height in kilometers",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.6,
          "scale": 3.0,
          "location": 0.1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "surface_temperature",
        "description": "Average surface temperature around crater in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 25.0,
          "std": 15.0,
          "min_clip": 0.0,
          "max_clip": 80.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "volcanic_activity_level_dormantactiveerupting",
      "description": "Classification of volcanic activity level",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-4.5 + 0.8*seismic_magnitude + 0.05*seismic_frequency + 0.003*so2_emissions + 0.002*co2_emissions + 0.15*ground_deformation + 0.04*thermal_anomaly_temp + 0.6*gas_plume_height + 0.02*surface_temperature)))",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "Dormant",
        "Dormant",
        "Dormant",
        "Dormant",
        "Active",
        "Active",
        "Active",
        "Erupting",
        "Erupting",
        "Erupting"
      ]
    }
  }
}