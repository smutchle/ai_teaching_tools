{
  "dataset_config": {
    "name": "ds095_geoscience_mineral_deposit_type_5_ore_classes_dirty",
    "description": "Geoscience dataset for identifying economic mineralization category based on geological characteristics",
    "random_seed": 95,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "alteration_intensity",
          "hydrothermal_fluid_temp"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "host_rock_porosity",
          "permeability_index"
        ],
        "correlation": 0.72,
        "method": "cholesky"
      },
      {
        "variables": [
          "structural_complexity",
          "fracture_density"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "geochemical_anomaly",
          "alteration_intensity"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "tectonic_stress",
          "structural_complexity"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "host_rock_porosity",
        "description": "Porosity percentage of host rock formation",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 15.5,
          "std": 6.2,
          "min_clip": 2.0,
          "max_clip": 35.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "alteration_intensity",
        "description": "Intensity index of hydrothermal alteration",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 45.0,
          "location": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "structural_complexity",
        "description": "Structural complexity score based on fault systems",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 1.0,
          "max": 10.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "geochemical_anomaly",
        "description": "Geochemical anomaly strength in parts per million",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 125.8,
          "std": 42.3,
          "min_clip": 10.0,
          "max_clip": 500.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "tectonic_stress",
        "description": "Tectonic stress magnitude in megapascals",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 85.0,
          "location": 15.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "hydrothermal_fluid_temp",
        "description": "Temperature of hydrothermal fluids in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 285.0,
          "std": 75.0,
          "min_clip": 150.0,
          "max_clip": 600.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "permeability_index",
        "description": "Rock permeability index in millidarcies",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 8.2,
          "std": 3.1,
          "min_clip": 0.5,
          "max_clip": 20.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "fracture_density",
        "description": "Fracture density per cubic meter",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.1,
          "max": 5.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "mineral_deposit_type_5_ore_classes",
      "description": "Economic mineralization category classification",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-2.5 + 0.08*alteration_intensity + 0.015*geochemical_anomaly + 0.25*structural_complexity + 0.12*host_rock_porosity + 0.02*tectonic_stress + 0.005*hydrothermal_fluid_temp + 0.3*permeability_index + 0.4*fracture_density)))",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "Barren Rock",
        "Low Grade Gold",
        "Copper Porphyry",
        "Copper Porphyry",
        "Polymetallic VMS",
        "Polymetallic VMS",
        "High Grade Gold",
        "High Grade Gold",
        "Rare Earth Elements",
        "Rare Earth Elements"
      ]
    }
  }
}