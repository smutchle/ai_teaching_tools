{
  "dataset_config": {
    "name": "ds094_geoscience_flood_risk_zone_lowmediumhighextreme_dirty",
    "description": "Geoscience dataset for flood risk zone classification mapping inundation hazard categories",
    "random_seed": 94,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "elevation_meters",
          "distance_to_water_km"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "rainfall_intensity_mm_hr",
          "drainage_capacity_m3_s"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "land_cover_permeability",
          "drainage_capacity_m3_s"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "elevation_meters",
          "slope_gradient_percent"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "distance_to_water_km",
          "soil_infiltration_rate_mm_hr"
        ],
        "correlation": -0.42,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "elevation_meters",
        "description": "Ground elevation above sea level in meters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 185.0,
          "std": 95.0,
          "min_clip": 0.0,
          "max_clip": 800.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "distance_to_water_km",
        "description": "Distance to nearest water body in kilometers",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 5.2,
          "location": 0.1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "rainfall_intensity_mm_hr",
        "description": "Peak rainfall intensity in millimeters per hour",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 45.0,
          "location": 5.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "land_cover_permeability",
        "description": "Land cover permeability index (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 55.0,
          "std": 22.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "drainage_capacity_m3_s",
        "description": "Local drainage system capacity in cubic meters per second",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 12.5,
          "std": 8.2,
          "min_clip": 0.5,
          "max_clip": 50.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "slope_gradient_percent",
        "description": "Terrain slope gradient as percentage",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 8.5,
          "location": 0.2
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "soil_infiltration_rate_mm_hr",
        "description": "Soil water infiltration rate in millimeters per hour",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 25.0,
          "std": 12.0,
          "min_clip": 2.0,
          "max_clip": 80.0
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "flood_risk_zone_lowmediumhighextreme",
      "description": "Flood risk zone classification based on inundation hazard",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-2.5 - 0.015*elevation_meters + 0.8/distance_to_water_km + 0.025*rainfall_intensity_mm_hr - 0.012*land_cover_permeability - 0.08*drainage_capacity_m3_s - 0.05*slope_gradient_percent - 0.008*soil_infiltration_rate_mm_hr)))",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "Low",
        "Low",
        "Low",
        "Medium",
        "Medium",
        "Medium",
        "High",
        "High",
        "Extreme",
        "Extreme"
      ]
    }
  }
}