{
  "dataset_config": {
    "name": "ds027_business_loan_default_risk_lowmedhigh",
    "description": "Classify creditworthiness into risk categories (Low/Med/High) based on credit score, income, debt-to-income ratio, and employment history",
    "random_seed": 27,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "credit_score",
          "annual_income"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "debt_to_income_ratio",
          "monthly_debt_payments"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "employment_years",
          "annual_income"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "credit_score",
          "debt_to_income_ratio"
        ],
        "correlation": -0.4,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "credit_score",
        "description": "FICO credit score ranging from 300 to 850",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 680,
          "std": 85,
          "min_clip": 300,
          "max_clip": 850
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "annual_income",
        "description": "Annual gross income in dollars",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.5,
          "scale": 65000,
          "location": 25000
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "debt_to_income_ratio",
        "description": "Total monthly debt payments divided by monthly gross income",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 0.28,
          "std": 0.12,
          "min_clip": 0.02,
          "max_clip": 0.65
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "employment_years",
        "description": "Years of continuous employment with current employer",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 4.5,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "monthly_debt_payments",
        "description": "Total monthly debt payments in dollars",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 1800,
          "std": 750,
          "min_clip": 100,
          "max_clip": 8000
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "loan_amount_requested",
        "description": "Amount of loan being requested in dollars",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 5000,
          "max": 75000
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "loan_default_risk_lowmedhigh",
      "description": "Loan default risk classification: Low, Medium, or High risk",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(8 - 0.012*credit_score - 0.00003*annual_income + 15*debt_to_income_ratio - 0.2*employment_years + 0.00002*loan_amount_requested)))",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0,
      "categories": [
        "Low Risk",
        "Low Risk",
        "Low Risk",
        "Low Risk",
        "Medium Risk",
        "Medium Risk",
        "Medium Risk",
        "High Risk",
        "High Risk",
        "High Risk"
      ]
    }
  }
}