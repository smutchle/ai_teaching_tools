{
  "dataset_config": {
    "name": "ds095_geoscience_mineral_deposit_type_5_ore_classes",
    "description": "Geoscience dataset for mineral deposit type classification with 5 ore classes based on host rock, alteration, structure, geochemistry, and tectonic setting",
    "random_seed": 95,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "host_rock_competency",
          "structural_complexity"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "alteration_intensity",
          "geochemical_anomaly"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "tectonic_stress",
          "structural_complexity"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "geochemical_anomaly",
          "hydrothermal_activity"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "host_rock_competency",
        "description": "Mechanical strength and competency of host rock formation",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65.0,
          "std": 15.0,
          "min_clip": 10.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "alteration_intensity",
        "description": "Degree of hydrothermal alteration in surrounding rock",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 45.0,
          "location": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "structural_complexity",
        "description": "Complexity index of geological structures and fracturing",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 15.0,
          "max": 85.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "geochemical_anomaly",
        "description": "Geochemical anomaly strength for ore-forming elements",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 42.0,
          "std": 18.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "tectonic_stress",
        "description": "Regional tectonic stress intensity indicator",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 55.0,
          "std": 20.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "hydrothermal_activity",
        "description": "Historical hydrothermal activity level in the area",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 38.0,
          "location": 8.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "depth_to_mineralization",
        "description": "Estimated depth to primary mineralization zone in meters",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 150.0,
          "std": 80.0,
          "min_clip": 10.0,
          "max_clip": 500.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "mineral_deposit_type_5_ore_classes",
      "description": "Economic mineralization category based on ore type and formation process",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(\u22122.5 + 0.04*host_rock_competency + 0.06*alteration_intensity + 0.03*structural_complexity + 0.08*geochemical_anomaly + 0.02*tectonic_stress + 0.05*hydrothermal_activity - 0.01*depth_to_mineralization)))",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0,
      "categories": [
        "Epithermal Gold",
        "Epithermal Gold",
        "Porphyry Copper",
        "Porphyry Copper",
        "Skarn Iron",
        "Skarn Iron",
        "VMS Base Metals",
        "VMS Base Metals",
        "Orogenic Gold",
        "Orogenic Gold"
      ]
    }
  }
}