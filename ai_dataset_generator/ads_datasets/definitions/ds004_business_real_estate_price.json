{
  "dataset_config": {
    "name": "ds004_business_real_estate_price",
    "description": "Predict property sale or rental prices based on square footage, bedrooms, location score, age, and amenities count",
    "random_seed": 4,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "square_footage",
          "bedrooms"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "square_footage",
          "bathrooms"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "location_score",
          "amenities_count"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "bedrooms",
          "bathrooms"
        ],
        "correlation": 0.72,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "square_footage",
        "description": "Total square footage of the property",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 2200,
          "std": 800,
          "min_clip": 600,
          "max_clip": 8000
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "bedrooms",
        "description": "Number of bedrooms",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 3.2,
          "std": 1.1,
          "min_clip": 1,
          "max_clip": 8
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "bathrooms",
        "description": "Number of bathrooms",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2.3,
          "std": 0.8,
          "min_clip": 1.0,
          "max_clip": 6.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "location_score",
        "description": "Location desirability score from 1-100",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.5,
          "scale": 65,
          "location": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "property_age",
        "description": "Age of the property in years",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 25,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "amenities_count",
        "description": "Number of premium amenities (pool, garage, etc.)",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 4.5,
          "std": 2.2,
          "min_clip": 0,
          "max_clip": 12
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "lot_size",
        "description": "Lot size in acres",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 0.35,
          "location": 0.1
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "garage_spaces",
        "description": "Number of garage parking spaces",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 2.1,
          "std": 0.9,
          "min_clip": 0,
          "max_clip": 4
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "real_estate_price",
      "description": "Property sale price in dollars",
      "data_type": "float",
      "expression": "150000 + 125*square_footage + 18000*bedrooms + 15000*bathrooms + 2800*location_score - 1200*property_age + 8500*amenities_count + 45000*lot_size + 12000*garage_spaces",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}