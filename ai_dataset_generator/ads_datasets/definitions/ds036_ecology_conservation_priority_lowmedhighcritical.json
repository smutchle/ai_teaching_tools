{
  "dataset_config": {
    "name": "ds036_ecology_conservation_priority_lowmedhighcritical",
    "description": "Rank ecological areas for protection efforts based on endemism, threat level, ecosystem services, rarity, and legal status",
    "random_seed": 36,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "endemism_score",
          "species_rarity_index"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "habitat_threat_level",
          "current_protection_status"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "ecosystem_service_value",
          "biodiversity_richness"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "endemism_score",
          "biodiversity_richness"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "endemism_score",
        "description": "Endemic species density score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.2,
          "scale": 35.0,
          "location": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "habitat_threat_level",
        "description": "Threat level to habitat integrity (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.5,
          "std": 2.1,
          "min_clip": 0.0,
          "max_clip": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "ecosystem_service_value",
        "description": "Economic value of ecosystem services (thousands USD per hectare)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 45.0,
          "location": 10.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "species_rarity_index",
        "description": "Composite rarity index for species present (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 42.0,
          "std": 18.5,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "current_protection_status",
        "description": "Current legal protection level (0-5 scale)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.0,
          "max": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "biodiversity_richness",
        "description": "Species richness index (number of species per unit area)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 125.0,
          "std": 45.0,
          "min_clip": 20.0,
          "max_clip": 300.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "habitat_connectivity",
        "description": "Connectivity to other protected areas (0-100 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 55.0,
          "std": 22.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "conservation_priority_lowmedhighcritical",
      "description": "Conservation priority ranking for protection efforts",
      "data_type": "categorical",
      "expression": "0.25*endemism_score + 0.35*habitat_threat_level + 0.15*ecosystem_service_value + 0.30*species_rarity_index - 0.20*current_protection_status + 0.10*biodiversity_richness + 0.08*habitat_connectivity",
      "noise_percent": 2.5,
      "categories": [
        "Low",
        "Low",
        "Low",
        "Medium",
        "Medium",
        "Medium",
        "High",
        "High",
        "Critical",
        "Critical"
      ],
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}