{
  "dataset_config": {
    "name": "ds040_sociology_technology_adoption_earlylatenon_adopter",
    "description": "Classify innovation diffusion groups based on age, education, income, tech literacy, and peer influence",
    "random_seed": 40,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "education_years",
          "income_thousands"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "tech_literacy_score",
          "education_years"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "peer_influence_score",
          "social_network_size"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "income_thousands",
          "tech_literacy_score"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "age",
        "description": "Age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 42,
          "std": 15,
          "min_clip": 18,
          "max_clip": 80
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "education_years",
        "description": "Years of formal education",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 14,
          "std": 3,
          "min_clip": 8,
          "max_clip": 22
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "income_thousands",
        "description": "Annual income in thousands of dollars",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.5,
          "scale": 55,
          "location": 25
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "tech_literacy_score",
        "description": "Technology literacy score (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65,
          "std": 18,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "peer_influence_score",
        "description": "Peer influence score (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6.2,
          "std": 2.1,
          "min_clip": 0,
          "max_clip": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "social_network_size",
        "description": "Number of close social connections",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 12,
          "location": 3
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "urban_rural_index",
        "description": "Urban-rural index (1=rural, 10=urban)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 1,
          "max": 10
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "media_exposure_hours",
        "description": "Weekly hours of technology media exposure",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.2,
          "scale": 8,
          "location": 1
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "technology_adoption_earlylatenon_adopter",
      "description": "Technology adoption category based on innovation diffusion theory",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-3.5 - 0.08*age + 0.25*education_years + 0.015*income_thousands + 0.04*tech_literacy_score + 0.35*peer_influence_score + 0.08*social_network_size + 0.12*urban_rural_index + 0.15*media_exposure_hours)))",
      "noise_percent": 2.5,
      "categories": [
        "Non-adopter",
        "Non-adopter",
        "Non-adopter",
        "Late Adopter",
        "Late Adopter",
        "Late Adopter",
        "Late Adopter",
        "Early Adopter",
        "Early Adopter",
        "Early Adopter"
      ],
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}