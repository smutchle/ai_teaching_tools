{
  "dataset_config": {
    "name": "ds036_ecology_conservation_priority_lowmedhighcritical_dirty",
    "description": "Rank ecological areas for conservation protection efforts based on endemism, threat level, ecosystem services, rarity, and legal status",
    "random_seed": 36,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "endemism_index",
          "species_rarity_score"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "threat_level_score",
          "habitat_degradation_rate"
        ],
        "correlation": 0.72,
        "method": "cholesky"
      },
      {
        "variables": [
          "ecosystem_services_value",
          "biodiversity_richness"
        ],
        "correlation": 0.58,
        "method": "cholesky"
      },
      {
        "variables": [
          "legal_protection_score",
          "conservation_funding"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "endemism_index",
        "description": "Measure of endemic species concentration (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 35,
          "location": 5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "threat_level_score",
        "description": "Combined threat assessment score (0-10)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.8,
          "std": 2.2,
          "min_clip": 0,
          "max_clip": 10
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "ecosystem_services_value",
        "description": "Economic value of ecosystem services (thousands USD per hectare)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 25,
          "location": 2
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "species_rarity_score",
        "description": "Composite rarity index based on species abundance (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 42,
          "std": 18,
          "min_clip": 0,
          "max_clip": 100
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "legal_protection_score",
        "description": "Current legal protection status score (0-5)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "habitat_degradation_rate",
        "description": "Annual habitat loss percentage (0-15%)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.5,
          "scale": 4.2,
          "location": 0.1
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "biodiversity_richness",
        "description": "Species richness index (number of species per unit area)",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 180,
          "std": 65,
          "min_clip": 10,
          "max_clip": 500
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "conservation_funding",
        "description": "Available conservation funding (thousands USD)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 120,
          "std": 45,
          "min_clip": 0,
          "max_clip": 300
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "conservation_priority_lowmedhighcritical",
      "description": "Conservation priority ranking for protection efforts",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(\u22124.5 + 0.08*endemism_index + 0.65*threat_level_score + 0.04*ecosystem_services_value + 0.06*species_rarity_score - 0.45*legal_protection_score + 0.35*habitat_degradation_rate + 0.008*biodiversity_richness - 0.012*conservation_funding)))",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "Low",
        "Low",
        "Low",
        "Medium",
        "Medium",
        "Medium",
        "High",
        "High",
        "Critical",
        "Critical"
      ]
    }
  }
}