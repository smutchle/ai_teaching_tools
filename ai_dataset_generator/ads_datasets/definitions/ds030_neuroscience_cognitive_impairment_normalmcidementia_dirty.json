{
  "dataset_config": {
    "name": "ds030_neuroscience_cognitive_impairment_normalmcidementia_dirty",
    "description": "Classify degree of cognitive decline using age, biomarkers, imaging features, cognitive scores, and genetics",
    "random_seed": 30,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "age",
          "amyloid_beta_level"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "mmse_score",
          "hippocampal_volume"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "tau_protein_level",
          "amyloid_beta_level"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "age",
          "mmse_score"
        ],
        "correlation": -0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "cortical_thickness",
          "hippocampal_volume"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "age",
        "description": "Patient age in years",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 72,
          "std": 8,
          "min_clip": 50,
          "max_clip": 95
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "amyloid_beta_level",
        "description": "Amyloid beta protein level in pg/mL",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.5,
          "scale": 450,
          "location": 200
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "tau_protein_level",
        "description": "Tau protein level in pg/mL",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 85,
          "std": 25,
          "min_clip": 20,
          "max_clip": 200
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "hippocampal_volume",
        "description": "Hippocampal volume in mm\u00b3",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 3800,
          "std": 450,
          "min_clip": 2500,
          "max_clip": 5000
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "cortical_thickness",
        "description": "Average cortical thickness in mm",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 2.8,
          "std": 0.3,
          "min_clip": 2.0,
          "max_clip": 3.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "mmse_score",
        "description": "Mini-Mental State Examination score (0-30)",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 24,
          "std": 5,
          "min_clip": 0,
          "max_clip": 30
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "apoe4_alleles",
        "description": "Number of APOE4 alleles (0, 1, or 2)",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 2
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "white_matter_lesions",
        "description": "White matter lesion volume in mL",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.2,
          "scale": 8,
          "location": 0.5
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "cognitive_impairment_normalmcidementia",
      "description": "Degree of cognitive decline classification",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(-12 + 0.08*age + 0.005*amyloid_beta_level + 0.02*tau_protein_level - 0.002*hippocampal_volume - 2*cortical_thickness - 0.25*mmse_score + 0.8*apoe4_alleles + 0.15*white_matter_lesions)))",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "Normal",
        "Normal",
        "Normal",
        "Normal",
        "MCI",
        "MCI",
        "MCI",
        "Dementia",
        "Dementia",
        "Dementia"
      ]
    }
  }
}