{
  "dataset_config": {
    "name": "ds013_sociology_social_mobility_score",
    "description": "Measure economic advancement across generations",
    "random_seed": 13,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "parental_education_years",
          "household_income_quintile"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "parental_education_years",
          "access_to_resources_index"
        ],
        "correlation": 0.48,
        "method": "cholesky"
      },
      {
        "variables": [
          "household_income_quintile",
          "neighborhood_quality_score"
        ],
        "correlation": 0.62,
        "method": "cholesky"
      },
      {
        "variables": [
          "neighborhood_quality_score",
          "access_to_resources_index"
        ],
        "correlation": 0.51,
        "method": "cholesky"
      },
      {
        "variables": [
          "cultural_capital_score",
          "parental_education_years"
        ],
        "correlation": 0.43,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "parental_education_years",
        "description": "Average years of education for parents",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 13.5,
          "std": 3.2,
          "min_clip": 6.0,
          "max_clip": 22.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "household_income_quintile",
        "description": "Household income quintile (1-5 scale)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 1.0,
          "max": 5.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "neighborhood_quality_score",
        "description": "Composite neighborhood quality index (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 65.0,
          "std": 18.5,
          "min_clip": 10.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "access_to_resources_index",
        "description": "Access to educational and social resources (0-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 6.8,
          "location": 0.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "cultural_capital_score",
        "description": "Cultural capital and social connections score (0-50)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 28.5,
          "std": 8.7,
          "min_clip": 5.0,
          "max_clip": 50.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "geographic_mobility",
        "description": "Geographic mobility opportunities (0-20 scale)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 2.0,
          "max": 18.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "social_mobility_score",
      "description": "Social mobility score measuring economic advancement across generations",
      "data_type": "float",
      "expression": "25 + 2.8*parental_education_years + 4.5*household_income_quintile + 0.35*neighborhood_quality_score + 3.2*access_to_resources_index + 0.45*cultural_capital_score + 1.1*geographic_mobility",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}