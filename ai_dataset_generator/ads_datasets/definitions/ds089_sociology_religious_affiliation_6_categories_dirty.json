{
  "dataset_config": {
    "name": "ds089_sociology_religious_affiliation_6_categories_dirty",
    "description": "Classify spiritual or secular identity based on family background, geography, education, age, and social networks",
    "random_seed": 89,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "family_religiosity",
          "social_network_religiosity"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "education_years",
          "urban_rural_index"
        ],
        "correlation": 0.42,
        "method": "cholesky"
      },
      {
        "variables": [
          "age",
          "family_religiosity"
        ],
        "correlation": 0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "household_income",
          "education_years"
        ],
        "correlation": 0.68,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "family_religiosity",
        "description": "Family religious background intensity score",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 5.5,
          "std": 2.8,
          "min_clip": 0,
          "max_clip": 10
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "urban_rural_index",
        "description": "Geographic urbanization index (0=rural, 10=urban)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 10
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "education_years",
        "description": "Years of formal education completed",
        "data_type": "int",
        "distribution": {
          "type": "normal",
          "mean": 13.5,
          "std": 3.2,
          "min_clip": 6,
          "max_clip": 22
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "age",
        "description": "Age in years",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 2.5,
          "scale": 45,
          "location": 18
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "social_network_religiosity",
        "description": "Religiosity level of social network and friends",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 4.8,
          "std": 2.5,
          "min_clip": 0,
          "max_clip": 10
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      },
      {
        "name": "household_income",
        "description": "Annual household income in thousands",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 1.8,
          "scale": 55,
          "location": 25
        },
        "missing_rate": 0.02,
        "outlier_rate": 0.01,
        "outlier_method": "extreme_both",
        "outlier_multiplier": 3.0
      }
    ],
    "target": {
      "name": "religious_affiliation_6_categories",
      "description": "Religious affiliation classification",
      "data_type": "categorical",
      "expression": "2.5 + 0.8*family_religiosity + 0.6*social_network_religiosity - 0.3*education_years - 0.4*urban_rural_index + 0.02*age - 0.01*household_income",
      "noise_percent": 2.5,
      "missing_rate": 0.02,
      "categories": [
        "Secular/Atheist",
        "Secular/Atheist",
        "Spiritual but not Religious",
        "Mainline Protestant",
        "Catholic",
        "Evangelical Protestant",
        "Evangelical Protestant",
        "Other Christian",
        "Non-Christian Religious",
        "Highly Religious"
      ]
    }
  }
}