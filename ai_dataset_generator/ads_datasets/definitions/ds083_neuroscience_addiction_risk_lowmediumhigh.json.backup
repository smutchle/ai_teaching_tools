{
  "dataset_config": {
    "name": "ds083_neuroscience_addiction_risk_lowmediumhigh",
    "description": "Neuroscience dataset for stratifying vulnerability to addiction dependence based on genetics, trauma history, social environment, personality traits, and substance exposure",
    "random_seed": 83,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "genetic_risk_score",
          "family_addiction_history"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "childhood_trauma_score",
          "social_support_index"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "impulsivity_score",
          "novelty_seeking_score"
        ],
        "correlation": 0.55,
        "method": "cholesky"
      },
      {
        "variables": [
          "early_substance_exposure_age",
          "peer_substance_use_score"
        ],
        "correlation": -0.4,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "genetic_risk_score",
        "description": "Polygenic risk score for addiction susceptibility (0-100)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 45.0,
          "std": 15.0,
          "min_clip": 0.0,
          "max_clip": 100.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "family_addiction_history",
        "description": "Number of family members with addiction history",
        "data_type": "int",
        "distribution": {
          "type": "weibull",
          "shape": 1.2,
          "scale": 1.5,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "childhood_trauma_score",
        "description": "Childhood trauma exposure score (0-40)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 8.0,
          "location": 0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "social_support_index",
        "description": "Social support network strength index (20-80)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 55.0,
          "std": 12.0,
          "min_clip": 20.0,
          "max_clip": 80.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "impulsivity_score",
        "description": "Impulsivity trait score (10-50)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 28.0,
          "std": 8.0,
          "min_clip": 10.0,
          "max_clip": 50.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "novelty_seeking_score",
        "description": "Novelty seeking personality trait score (15-60)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 35.0,
          "std": 10.0,
          "min_clip": 15.0,
          "max_clip": 60.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "early_substance_exposure_age",
        "description": "Age of first substance exposure in years",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 16.5,
          "std": 3.0,
          "min_clip": 10.0,
          "max_clip": 25.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "peer_substance_use_score",
        "description": "Peer group substance use prevalence score (0-30)",
        "data_type": "float",
        "distribution": {
          "type": "uniform",
          "min": 0.0,
          "max": 30.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "addiction_risk_lowmediumhigh",
      "description": "Addiction risk stratification level",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(\u22122.5 + 0.04*genetic_risk_score + 0.3*family_addiction_history + 0.05*childhood_trauma_score - 0.02*social_support_index + 0.06*impulsivity_score + 0.04*novelty_seeking_score - 0.15*early_substance_exposure_age + 0.08*peer_substance_use_score)))",
      "noise_percent": 2.5,
      "categories": [
        "Low",
        "Low",
        "Low",
        "Low",
        "Medium",
        "Medium",
        "Medium",
        "High",
        "High",
        "High"
      ],
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}