{
  "dataset_config": {
    "name": "ds071_biology_metabolic_rate",
    "description": "Estimate energy expenditure in kilocalories per day based on body mass, temperature, activity level, diet composition, and age",
    "random_seed": 71,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "body_mass_kg",
          "basal_metabolic_rate"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "age_years",
          "activity_level_score"
        ],
        "correlation": -0.35,
        "method": "cholesky"
      },
      {
        "variables": [
          "protein_intake_percent",
          "fat_intake_percent"
        ],
        "correlation": -0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "body_temperature_celsius",
          "metabolic_efficiency"
        ],
        "correlation": 0.42,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "body_mass_kg",
        "description": "Body mass in kilograms",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 70.0,
          "std": 15.0,
          "min_clip": 40.0,
          "max_clip": 120.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "body_temperature_celsius",
        "description": "Core body temperature in Celsius",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 37.0,
          "std": 0.5,
          "min_clip": 35.5,
          "max_clip": 38.5
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "activity_level_score",
        "description": "Physical activity level score (1-10 scale)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.0,
          "scale": 6.0,
          "location": 1.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "protein_intake_percent",
        "description": "Percentage of daily calories from protein",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 18.0,
          "std": 4.0,
          "min_clip": 10.0,
          "max_clip": 30.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "fat_intake_percent",
        "description": "Percentage of daily calories from fat",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 32.0,
          "std": 8.0,
          "min_clip": 15.0,
          "max_clip": 50.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "age_years",
        "description": "Age in years",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 18,
          "max": 75
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "basal_metabolic_rate",
        "description": "Basal metabolic rate component",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 1600.0,
          "std": 200.0,
          "min_clip": 1200.0,
          "max_clip": 2200.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "metabolic_efficiency",
        "description": "Individual metabolic efficiency factor",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 1.0,
          "std": 0.15,
          "min_clip": 0.7,
          "max_clip": 1.3
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "metabolic_rate",
      "description": "Daily energy expenditure in kilocalories per day",
      "data_type": "float",
      "expression": "basal_metabolic_rate * metabolic_efficiency + 15*body_mass_kg + 200*activity_level_score + 50*(body_temperature_celsius - 37) + 8*protein_intake_percent + 3*fat_intake_percent - 5*age_years",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0
    }
  }
}