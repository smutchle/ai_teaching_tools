{
  "dataset_config": {
    "name": "ds030_neuroscience_cognitive_impairment_normalmcidementia",
    "description": "Classify degree of cognitive decline using age, biomarkers, imaging features, cognitive scores, and genetics",
    "random_seed": 30,
    "n_rows": 500000,
    "correlations": [
      {
        "variables": [
          "age",
          "amyloid_beta"
        ],
        "correlation": 0.45,
        "method": "cholesky"
      },
      {
        "variables": [
          "mmse_score",
          "moca_score"
        ],
        "correlation": 0.65,
        "method": "cholesky"
      },
      {
        "variables": [
          "hippocampal_volume",
          "mmse_score"
        ],
        "correlation": 0.52,
        "method": "cholesky"
      },
      {
        "variables": [
          "tau_protein",
          "amyloid_beta"
        ],
        "correlation": 0.38,
        "method": "cholesky"
      },
      {
        "variables": [
          "age",
          "hippocampal_volume"
        ],
        "correlation": -0.42,
        "method": "cholesky"
      }
    ],
    "features": [
      {
        "name": "age",
        "description": "Age in years",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 72.5,
          "std": 8.2,
          "min_clip": 50,
          "max_clip": 95
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "amyloid_beta",
        "description": "Amyloid beta 42 levels in CSF (pg/mL)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 185.3,
          "std": 45.7,
          "min_clip": 80,
          "max_clip": 350
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "tau_protein",
        "description": "Total tau protein levels in CSF (pg/mL)",
        "data_type": "float",
        "distribution": {
          "type": "weibull",
          "shape": 2.1,
          "scale": 85.0,
          "location": 20.0
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "hippocampal_volume",
        "description": "Hippocampal volume from MRI (mm\u00b3)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 6850.0,
          "std": 980.0,
          "min_clip": 4000,
          "max_clip": 9500
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "mmse_score",
        "description": "Mini-Mental State Examination score (0-30)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 24.8,
          "std": 4.2,
          "min_clip": 10,
          "max_clip": 30
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "moca_score",
        "description": "Montreal Cognitive Assessment score (0-30)",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 22.1,
          "std": 5.8,
          "min_clip": 8,
          "max_clip": 30
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "apoe4_alleles",
        "description": "Number of APOE4 alleles (0, 1, or 2)",
        "data_type": "int",
        "distribution": {
          "type": "uniform",
          "min": 0,
          "max": 2
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      },
      {
        "name": "education_years",
        "description": "Years of formal education",
        "data_type": "float",
        "distribution": {
          "type": "normal",
          "mean": 14.2,
          "std": 3.1,
          "min_clip": 8,
          "max_clip": 22
        },
        "missing_rate": 0.0,
        "outlier_rate": 0.0
      }
    ],
    "target": {
      "name": "cognitive_impairment_normalmcidementia",
      "description": "Cognitive impairment classification",
      "data_type": "categorical",
      "expression": "1 / (1 + np.exp(-(8.5 - 0.08*age + 0.012*amyloid_beta - 0.025*tau_protein + 0.0008*hippocampal_volume + 0.35*mmse_score + 0.28*moca_score - 0.85*apoe4_alleles + 0.12*education_years)))",
      "noise_percent": 2.5,
      "missing_rate": 0.0,
      "outlier_rate": 0.0,
      "categories": [
        "Dementia",
        "Dementia",
        "Dementia",
        "MCI",
        "MCI",
        "MCI",
        "Normal",
        "Normal",
        "Normal",
        "Normal"
      ]
    }
  }
}